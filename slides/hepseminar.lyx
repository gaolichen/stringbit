One-loop Self-energy
\end_layout

\begin_layout Itemize
Let 
\begin_inset Formula $\ket{E}=T_{r}\ket{0}+T_{p}T_{q}\ket{0}C_{pq}+\cdots$
\end_inset

, 
\begin_inset Formula $E=E_{r}+\Delta E_{r}$
\end_inset

, by time independent perturbation theory, we find
\begin_inset Formula 
\begin{align*}
C_{pq} & =\frac{1}{E_{r}-E_{p}-E_{q}}\frac{1}{N}V_{qpr}+\mathcal{O}\left(N^{-2}\right),\\
\Delta E_{r} & =\frac{1}{N^{2}}\sum_{L=1}^{M-1}\sum_{p,q}W_{rpq}^{\left(L,K\right)}\frac{1}{E_{r}-E_{p}-E_{q}}V_{qpr}^{\left(K,L\right)},
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
For ground energy
\begin_inset Formula 
\[
\Delta E_{G}\sim\frac{1}{N^{2}}\sum_{L=1}^{M-1}{\sum_{p,q}}^{\prime}\frac{KLM\left|\det C\right|^{s}}{E_{G}-E_{p}-E_{q}}.
\]

\end_inset


\end_layout

\begin_layout Itemize
Large 
\begin_inset Formula $M$
\end_inset

 behavior:
\begin_inset Formula $\Delta E_{G}\sim\frac{1}{N^{2}}M^{5-s/4}$
\end_inset

 for even 
\begin_inset Formula $s$
\end_inset

 and 
\begin_inset Formula $\Delta E_{G}\sim\frac{1}{N^{2}}M^{4-s/4}$
\end_inset

 for odd 
\begin_inset Formula $s$
\end_inset

 .
 
\end_layout

\begin_layout Itemize
Protostring: when 
\begin_inset Formula $s=24$
\end_inset

, 
\begin_inset Formula $\Delta E_{G}\sim\frac{1}{N^{2}}M^{-1}\sim1/P^{+}$
\end_inset

, which resembles Poincare invariant dispersion relation 
\begin_inset Formula $P^{-}\sim1/P^{+}$
\end_inset

.
 
\end_layout

\begin_layout EndFrame

\end_layout

