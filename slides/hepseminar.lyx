Define trace operator 
\begin_inset Formula 
\[
T\left(\theta_{1},\cdots,\theta_{k}\right)=\tr\psi\left(\theta_{1}\right)\psi\left(\theta_{2}\right)\cdots\psi\left(\theta_{k}\right)
\]

\end_inset

 where 
\begin_inset Formula $\psi\left(\theta_{k}\right)=\sum_{i=0}^{s}\frac{1}{i!}\bar{\phi}_{c_{1}\cdots c_{i}}\theta_{k}^{c_{1}}\cdots\theta_{k}^{c_{i}}$
\end_inset

 and 
\begin_inset Formula $\theta_{k}^{1},\cdots\theta_{k}^{s}$
\end_inset

 are s Grassmann variables.
\end_layout

\begin_layout Itemize
The trace state space is spanned by 
\begin_inset Formula 
\[
T\left(\theta_{1},\cdots,\theta_{K}\right)T\left(\eta_{1},\cdots,\eta_{L}\right)\cdots\ket{0}
\]

\end_inset


\end_layout

\begin_layout Itemize
A 
\begin_inset Formula $M$
\end_inset

-bit single trace energy eigenstate can be written as
\begin_inset Formula 
\[
T_{r}\ket{0}=\int d^{s}\theta_{1}\cdots d^{s}\theta_{M}\, T\left(\theta_{1},\cdots,\theta_{M}\right)\psi_{r}\left(\theta_{1},\dots,\theta_{M}\right)\ket{0},
\]

\end_inset

 where wave function 
\begin_inset Formula $\psi_{r}$
\end_inset

 satisfies
\begin_inset Formula 
\[
h\psi_{r}=E_{r}\psi_{r},\quad h=\sum_{k=1}^{M}h_{k,k+1}
\]

\end_inset


\begin_inset Formula 
\[
h_{kl}=-2\left(s-2\theta_{k}^{a}\frac{d}{d\theta_{k}^{a}}\right)-2\theta_{k}^{a}\frac{d}{d\theta_{l}^{a}}-2\theta_{l}^{a}\frac{d}{d\theta_{k}^{a}}-2i\theta_{k}^{a}\theta_{l}^{a}-2i\frac{d}{d\theta_{k}^{a}}\frac{d}{d\theta_{l}^{a}}
\]

\end_inset


\end_layout

\begin_layout EndFrame

\end_layout

\begin_layout BeginFrame
Review: 
\begin_inset Formula $N=\infty$
\end_inset

 results
\end_layout

\begin_layout Itemize
Eigenoperators
\begin_inset Formula 
\[
\alpha_{n}=\frac{1}{\sqrt{M}}\sum_{k=1}^{M}\theta_{k}e^{-2\pi ikn/M},\quad\beta_{n}=\frac{1}{\sqrt{M}}\sum_{k=1}^{M}\frac{d}{d\theta_{k}}e^{-2\pi ikn/M},
\]

\end_inset


\begin_inset Formula 
\[
F_{k}=\sin\left(\frac{\pi}{4}-\frac{k\pi}{2M}\right)\alpha_{k}+\cos\left(\frac{\pi}{4}-\frac{k\pi}{2M}\right)\beta_{k}
\]

\end_inset


\begin_inset Formula 
\[
\left\{ F_{k},F_{l}\right\} =\left\{ F_{k}^{\dagger},F_{l}^{\dagger}\right\} =0,\quad\left\{ F_{k},F_{l}^{\dagger}\right\} =\delta_{kl}
\]

\end_inset


\end_layout

\begin_layout Itemize
Energy spectrum
\begin_inset Formula 
\[
\psi_{\{\lambda_{a,k}\}}=F_{1}^{\dagger\lambda_{1}}F_{2}^{\dagger\lambda_{2}}\cdots F_{M-1}^{\dagger\lambda_{M-1}}\psi_{G},\quad\lambda_{k}=0,\,1
\]

\end_inset


\begin_inset Formula 
\[
E_{\{\lambda_{a,k}\}}=-4\cot\frac{\pi}{2M}+8\sum_{k=1}^{M-1}\lambda_{k}\sin\frac{k\pi}{M},\quad\lambda_{k}=0,\,1
\]

\end_inset


\begin_inset Formula 
\[
E_{G}=-4\cot\frac{\pi}{2M}=-\frac{8M}{\pi}+\frac{2\pi}{3M}+\mathcal{O}\left(M^{-3}\right)
\]

\end_inset


\end_layout

\begin_layout EndFrame

\end_layout

\begin_layout BeginFrame
Cubic Interaction Vertices
\end_layout

\begin_layout Itemize
Vertex 
\begin_inset Formula $V$
\end_inset

 and 
\begin_inset Formula $W$
\end_inset

:
\begin_inset Formula 
\[
HT_{r}\ket{0}=E_{r}T_{r}\ket{0}+\frac{1}{N}\sum_{L=1}^{M-1}\sum_{p,q}T_{p}T_{q}\ket{0}V_{qpr}^{\left(K,L\right)}+\mathcal{O}\left(\frac{1}{N^{2}}\right)
\]

\end_inset


\begin_inset Formula 
\[
HT_{p}T_{q}\ket{0}=\left(E_{p}+E_{q}\right)T_{p}T_{q}\ket{0}+\frac{1}{N}\sum_{r}T_{r}\ket{0}W_{rpq}^{\left(L,K\right)}+\mathcal{O}\left(\frac{1}{N^{2}}\right)
\]

\end_inset

 where 
\begin_inset Formula $K+L=M$
\end_inset

.
\end_layout

\begin_layout Itemize
For 
\begin_inset Formula $\psi_{r}=F_{1}^{\dagger\lambda_{1}}\cdots F_{M-1}^{\dagger\lambda_{M-1}}\psi_{G}^{\left(M\right)}\equiv\mathbb{F}_{r}^{\dagger}\psi_{G}^{\left(M\right)}$
\end_inset

, 
\begin_inset Formula $\psi_{q}=\mathbb{F}_{q}^{\dagger}\psi_{G}^{\left(K\right)}$
\end_inset

, 
\begin_inset Formula $\psi_{q}=\mathbb{F}_{q}^{\dagger}\psi_{G}^{\left(L\right)}$
\end_inset

,
\begin_inset Formula 
\[
V_{qpr}^{\left(K,L\right)}=M\left|\det C\right|^{\frac{1}{2}}\left\langle \mathbb{F}_{q}\mathbb{F}_{p}h^{\left(V\right)}\mathbb{F}_{r}^{\dagger}\exp\left(\frac{1}{2}f_{k}^{\dagger}D_{kl}f_{l}^{\dagger}\right)\right\rangle 
\]

\end_inset


\begin_inset Formula 
\[
W_{rpq}^{\left(L,K\right)}=KL\left|\det C\right|^{\frac{1}{2}}\left\langle \mathbb{F}_{q}\mathbb{F}_{p}h^{\left(W\right)}\mathbb{F}_{r}^{\dagger}\exp\left(\frac{1}{2}f_{k}^{\dagger}D_{kl}f_{l}^{\dagger}\right)\right\rangle ^{\dagger}
\]

\end_inset


\end_layout

\begin_layout EndFrame

\end_layout

\begin_layout BeginFrame
Cubic Interaction Vertices (cont)
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f_{0}=F_{0}^{\left(L\right)}\sqrt{\frac{L}{M}}+F_{0}^{\left(K\right)}\sqrt{\frac{K}{M}},\quad f_{M-1}=e^{-i\pi/4}\left(F_{0}^{\left(L\right)}\sqrt{\frac{K}{M}}-F_{0}^{\left(K\right)}\sqrt{\frac{L}{M}}\right)
\]

\end_inset


\begin_inset Formula 
\[
f_{1},\cdots,f_{M-2}=F_{1}^{\left(L\right)},\cdots,F_{L-1}^{\left(L\right)},F_{1}^{\left(K\right)},\,\cdots,\, F_{K-1}^{\left(K\right)}
\]

\end_inset


\begin_inset Formula 
\[
F_{m}=\sum_{n=0}^{M-1}\left(f_{n}C_{mn}+f_{n}^{\dagger}S_{mn}\right),\quad0\leq m\leq M-1,
\]

\end_inset


\begin_inset Formula 
\begin{align*}
C_{mn} & =-\frac{1}{\sqrt{ML}}\frac{1-e^{-2\pi imL/M}}{1-e^{-2\pi i\left(n/L-m/M\right)}}\cos\left(\frac{n\pi}{2L}-\frac{m\pi}{2M}\right),\quad1\leq n<L\\
C_{m,L+n-1} & =\frac{1}{\sqrt{MK}}\frac{1-e^{-2\pi imL/M}}{1-e^{-2\pi i\left(n/K-m/M\right)}}\cos\left(\frac{n\pi}{2K}-\frac{m\pi}{2M}\right),\quad1\leq n<K\\
C_{m,M-1} & =-\frac{1}{\sqrt{LK}}\frac{1-e^{-2\pi imL/M}}{1-e^{2i\pi m/M}}\cos\left(\frac{m\pi}{2M}-\frac{\pi}{4}\right),
\end{align*}

\end_inset


\begin_inset Formula 
\[
C_{mn}D_{nl}+S_{ml}=0,\quad S_{mn}=C_{m,-n}
\]

\end_inset


\end_layout

\begin_layout EndFrame

\end_layout

\begin_layout BeginFrame
Cubic Interaction Vertices (cont)
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
h^{\left(V,W\right)}=\frac{2}{M}\left(A_{nm}^{\left(V,W\right)\dagger}F_{n}^{\dagger}F_{m}^{\dagger}+A_{nm}^{\left(V,W\right)}F_{n}F_{m}+2A_{-n,m}^{\left(V,W\right)}F_{n}^{\dagger}F_{m}\right)+\frac{2}{M}\mu^{\left(V,W\right)}
\]

\end_inset

 
\begin_inset Formula 
\begin{align*}
A_{nm}^{\left(V\right)} & =\sin\frac{m-n}{2M}\pi+\exp\left(2\pi i\frac{\left(L+1\right)\left(m+n\right)}{2M}\right)\sin\frac{\left(L+1/2\right)\left(m-n\right)\pi}{M}\\
\mu^{\left(V\right)} & =-\cot\frac{\pi}{2M}-\cot\frac{2L+1}{2M}\pi,\\
A_{nm}^{\left(W\right)} & =\left[1+\exp\left(\pi i\frac{n+m}{M}\right)\right]\left[1+\exp\left(2\pi iL\frac{m+n}{M}\right)\right]\sin\frac{m-n}{2M}\pi,\\
\mu^{\left(W\right)} & =-4\cot\frac{\pi}{2M}.
\end{align*}

\end_inset


\end_layout

\begin_layout EndFrame

\end_layout

