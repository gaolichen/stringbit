\begin_inset Formula 
\[
\bar{a}\equiv\bar{\phi},\quad\bar{b}\equiv\bar{\phi}_{1},\quad\left[a_{\alpha}^{\beta},\bar{a}_{\gamma}^{\delta}\right]=\delta_{\alpha}^{\delta}\delta_{\gamma}^{\beta},\quad\left\{ b_{\alpha}^{\beta},\bar{b}_{\gamma}^{\delta}\right\} =\delta_{\alpha}^{\delta}\delta_{\gamma}^{\beta}
\]

\end_inset


\end_layout

