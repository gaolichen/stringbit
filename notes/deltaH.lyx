\begin_layout Standard
When 
\begin_inset Formula $s=1$
\end_inset

, the color operator is
\begin_inset Formula 
\[
G_{\alpha}^{\beta}=\left(\bar{a}a+\bar{b}b-\normord{a\bar{a}+b\bar{b}}\right)_{\alpha}^{\beta}
\]

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\tr G^{2} & = & \tr\left(\bar{a}a+\bar{b}b-\normord{a\bar{a}+b\bar{b}}\right)\left(\bar{a}a+\bar{b}b-\normord{a\bar{a}+b\bar{b}}\right)\\
 & = & \tr\left(\bar{a}a+\bar{b}b\right)\left(\bar{a}a+\bar{b}b\right)+\tr\left(\normord{a\bar{a}+b\bar{b}}\right)\left(\normord{a\bar{a}+b\bar{b}}\right)\\
 &  & -\tr\left(\bar{a}a+\bar{b}b\right)\left(\normord{a\bar{a}+b\bar{b}}\right)-\tr\left(\normord{a\bar{a}+b\bar{b}}\right)\left(\bar{a}a+\bar{b}b\right)
\end{eqnarray*}

\end_inset

 
\begin_inset Formula 
\begin{eqnarray*}
\text{second term} & = & \tr\left(\normord{a\bar{a}+b\bar{b}}\right)\left(\normord{a\bar{a}+b\bar{b}}\right)\\
 & = & \tr\normord{a\bar{a}}\normord{a\bar{a}}+\tr\normord{b\bar{b}}\normord{b\bar{b}}\\
 &  & +\tr\normord{a\bar{a}}\normord{b\bar{b}}+\tr\normord{b\bar{b}}\normord{a\bar{a}}\\
 & = & \tr\bar{a}\normord{a\bar{a}}a+N\tr\bar{a}a+\tr\bar{b}\normord{b\bar{b}}b+N\tr\bar{b}b\\
 &  & +\tr\bar{a}\normord{b\bar{b}}a+\tr\bar{b}\normord{a\bar{a}}b\\
 & = & \tr\bar{a}a\bar{a}a-\tr\bar{b}b\bar{b}b+2N\tr\bar{b}b-\tr\bar{a}b\bar{b}a+N\tr\bar{a}\bar{a}+\tr\bar{b}a\bar{a}b-N\tr\bar{b}b\\
 & = & \tr\bar{a}a\bar{a}a-\tr\bar{b}b\bar{b}b-\tr\bar{a}b\bar{b}a+\tr\bar{b}a\bar{a}b+N\tr\bar{a}\bar{a}+N\tr\bar{b}b
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\tr\bar{b}a\bar{a}b & = & -\tr a\bar{a}b\bar{b}+N\tr a\bar{a}\\
 & = & -\tr\bar{a}b\bar{b}a-N\tr b\bar{b}+N\tr\bar{a}a+N^{2}\\
 & = & -\tr\bar{a}b\bar{b}a+N\tr\bar{b}b+N\tr\bar{a}a
\end{eqnarray*}

\end_inset

 therefore
\begin_inset Formula 
\[
\text{second term}=\tr\bar{a}a\bar{a}a-\tr\bar{b}b\bar{b}b-2\tr\bar{a}b\bar{b}a+2N\left(\tr\bar{a}\bar{a}+\tr\bar{b}b\right)
\]

\end_inset


