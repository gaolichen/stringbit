\begin_layout Standard
Let 
\begin_inset Formula $\ket{G}$
\end_inset

 be the ground state,
\begin_inset Formula 
\begin{eqnarray*}
h_{M,p+1}\ket{G} & = & \left(\alpha+\frac{2}{M}\sum_{n,m=1}^{M-1}\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\right)\ket{G}\\
 &  & +\frac{2}{M}\sum_{n=1}^{M-1}\left[1-\exp\left(-2\pi i\frac{\left(p+1\right)n}{M}\right)\right]\left(s_{n}+ic_{n}\right)F_{0}^{\dagger}F_{n}^{\dagger}\ket{G}
\end{eqnarray*}

\end_inset

 If
\begin_inset Formula 
\[
F_{m}=\sum_{n=0}^{M-2}\left(f_{n}C_{mn}+f_{n}^{\dagger}S_{mn}\right),\quad F_{m}^{\dagger}=\sum_{n=0}^{M-2}\left(f_{n}^{\dagger}C_{mn}^{*}+f_{n}S_{mn}^{*}\right)
\]

\end_inset

 where 
\begin_inset Formula $f_{1}$
\end_inset

 to 
\begin_inset Formula $f_{L-1}$
\end_inset

 are lowering operator of the first smaller chain and 
\begin_inset Formula $f_{L}$
\end_inset

 to 
\begin_inset Formula $f_{M-2}$
\end_inset

 are the ones for the other smaller chain.
 
\begin_inset Formula $f_{0}$
\end_inset

 and 
\begin_inset Formula $\bar{f}_{0}$
\end_inset

 are 
\begin_inset Formula 
\[
f_{0}=F_{0}^{\left(1\right)}\sqrt{\frac{K}{M}}-F_{0}^{\left(2\right)}\sqrt{\frac{L}{M}},\quad\bar{f}_{0}=f_{0}^{\dagger}=F_{0}^{\left(1\right)\dagger}\sqrt{\frac{K}{M}}-F_{0}^{\left(2\right)\dagger}\sqrt{\frac{L}{M}}
\]

\end_inset

 
\begin_inset Formula 
\[
\left\{ f_{m},f_{n}^{\dagger}\right\} =\delta_{mn},\quad\left\{ f_{m},f_{n}\right\} =\left\{ f_{m}^{\dagger},f_{n}^{\dagger}\right\} =0.
\]

\end_inset

 Since 
\begin_inset Formula 
\[
F_{0}=\beta_{0}=F_{0}^{\left(1\right)}\sqrt{\frac{L}{M}}+F_{0}^{\left(2\right)}\sqrt{\frac{K}{M}},\quad\bar{F}_{0}=F_{0}^{\dagger}=F_{0}^{\left(1\right)\dagger}\sqrt{\frac{L}{M}}+F_{0}^{\left(2\right)\dagger}\sqrt{\frac{K}{M}}
\]

\end_inset


\begin_inset Formula 
\[
\left\{ f_{0}^{\dagger},F_{0}\right\} =\left\{ \beta_{0}^{\left(1\right)},\alpha_{0}^{\left(1\right)}\right\} \frac{\sqrt{KL}}{M}-\left\{ \beta_{0}^{\left(2\right)},\alpha_{0}^{\left(2\right)}\right\} \frac{\sqrt{KL}}{M}=0
\]

\end_inset


\begin_inset Formula 
\[
F_{0}^{\left(1\right)}=F_{0}\sqrt{\frac{L}{M}}+f_{0}\sqrt{\frac{K}{M}},\quad F_{0}^{\left(2\right)}=F_{0}\sqrt{\frac{K}{M}}-f_{0}\sqrt{\frac{L}{M}}
\]

\end_inset

 we have
\begin_inset Formula 
\[
\left\{ F_{0},f_{n}^{\dagger}\right\} =\left\{ F_{0}^{\dagger},f_{n}^{\dagger}\right\} =\left\{ F_{0},f_{n}\right\} =\left\{ F_{0}^{\dagger},f_{n}\right\} =0,\quad0\leq n\leq M-1.
\]

\end_inset

We calculate 
\begin_inset Formula 
\begin{eqnarray*}
\left\{ F_{m},F_{n}\right\}  & = & \left\{ f_{i}C_{mi}+f_{i}^{\dagger}S_{mi},f_{j}C_{nj}+f_{j}^{\dagger}S_{nj}\right\} \\
 & = & \left\{ f_{i},f_{j}^{\dagger}\right\} C_{mi}S_{nj}+\left\{ f_{i}^{\dagger},f_{j}\right\} S_{mi}C_{nj}\\
 & = & C_{mi}S_{ni}+S_{mi}C_{ni},
\end{eqnarray*}

\end_inset

 which follows that 
\begin_inset Formula 
\[
CS^{T}=-\left(CS^{T}\right)^{T}.
\]

\end_inset

We also have
\begin_inset Formula 
\begin{eqnarray*}
\left\{ F_{m},F_{n}^{\dagger}\right\}  & = & \left\{ f_{i}C_{mi}+f_{i}^{\dagger}S_{mi},f_{j}^{\dagger}C_{nj}^{*}+f_{j}S_{nj}^{*}\right\} \\
 & = & \left\{ f_{i},f_{j}^{\dagger}\right\} C_{mi}C_{nj}^{*}+\left\{ f_{i}^{\dagger},f_{j}\right\} S_{mi}S_{nj}^{*}\\
 & = & C_{mi}C_{ni}^{*}+S_{mi}S_{ni}^{*}\\
 & != & \delta_{mn},
\end{eqnarray*}

\end_inset

from which it follows 
\begin_inset Formula 
\[
CC^{\dagger}+SS^{\dagger}=I.
\]

\end_inset

We express the ground state of large string in terms of the raising operators
 of the two smaller strings
\begin_inset Formula 
\[
\ket{G}=\exp\left\{ \frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right\} \ket{0}.
\]

\end_inset

Then using the formula
\begin_inset Formula 
\[
e^{A}Be^{-A}=B+\left[A,B\right]+\frac{1}{2}\left[A,\left[A,B\right]\right]+\cdots
\]

\end_inset

 we have 
\begin_inset Formula 
\begin{eqnarray*}
F_{m}\ket{G} & = & \exp\left\{ \frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right\} \left(F_{m}-\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},F_{m}\right]\right)\ket{0}\\
 & = & \exp\left\{ \frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right\} \left(f_{n}^{\dagger}S_{mn}-\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},F_{m}\right]\right)\ket{0}
\end{eqnarray*}

\end_inset

Using the identity 
\begin_inset Formula 
\[
\left[AB,C\right]=A\left\{ B,C\right\} -\left\{ A,C\right\} B.
\]

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},f_{m}\right] & = & \frac{1}{2}M_{kl}\left(f_{k}^{\dagger}\left\{ f_{l}^{\dagger},f_{m}\right\} -\left\{ f_{k}^{\dagger},f_{m}\right\} f_{l}^{\dagger}\right)\\
 & = & \frac{1}{2}M_{kl}\left(f_{k}^{\dagger}\delta_{lm}-f_{l}^{\dagger}\delta_{km}\right)\\
 & = & \frac{1}{2}M_{km}f_{k}^{\dagger}-\frac{1}{2}M_{ml}f_{l}^{\dagger}\\
 & = & f_{k}^{\dagger}M_{km}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},f_{m}f_{n}\right] & = & f_{m}\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},f_{n}\right]+\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},f_{m}\right]f_{n}\\
 & = & f_{m}f_{k}^{\dagger}M_{kn}+f_{k}^{\dagger}M_{km}f_{n}\\
 & = & \left\{ f_{m},f_{k}^{\dagger}\right\} M_{kn}-f_{k}^{\dagger}f_{m}M_{kn}+f_{k}^{\dagger}M_{km}f_{n}\\
 & = & M_{mn}+f_{k}^{\dagger}\left(M_{km}f_{n}-M_{kn}f_{m}\right)
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},f_{m}f_{n}\right]\right] & = & \left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},f_{k}^{\dagger}\left(M_{km}f_{n}-M_{kn}f_{m}\right)\right]\\
 & = & f_{k}^{\dagger}\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},M_{km}f_{n}-M_{kn}f_{m}\right]\\
 & = & f_{k}^{\dagger}M_{km}f_{l}^{\dagger}M_{ln}-f_{k}^{\dagger}M_{kn}f_{l}^{\dagger}M_{lm}\\
 & = & f_{k}^{\dagger}f_{l}^{\dagger}\left(M_{km}M_{ln}-M_{kn}M_{lm}\right)
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},F_{m}\right] & = & \frac{1}{2}\sum_{k,l}M_{kl}\left[f_{k}^{\dagger}f_{l}^{\dagger},F_{m}\right]\\
 & = & \frac{1}{2}\sum_{k,l}M_{kl}\left(f_{k}^{\dagger}\left\{ f_{l}^{\dagger},F_{m}\right\} -\left\{ f_{k}^{\dagger},F_{m}\right\} f_{l}\right)\\
 & = & \frac{1}{2}\sum_{k,l}M_{kl}\left(f_{k}^{\dagger}C_{mn}\delta_{ln}-C_{mn}\delta_{kn}f_{l}\right)\\
 & = & \frac{1}{2}\sum_{k,l}M_{kl}\left(f_{k}^{\dagger}C_{ml}-C_{mk}f_{l}\right)\\
 & = & \frac{1}{2}\sum_{k,l}\left(-C_{ml}M_{lk}f_{k}^{\dagger}-C_{mk}M_{kl}f_{l}\right)\\
 & = & -\sum_{k,l}C_{ml}M_{lk}f_{k}^{\dagger}
\end{eqnarray*}

\end_inset

it follows that 
\begin_inset Formula 
\[
F_{m}\ket{G}=\exp\left(\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right)\left(S+CM\right)_{mn}f_{n}^{\dagger}\ket{0}.
\]

\end_inset

 We therefore must have 
\begin_inset Formula 
\[
S=-CM.
\]

\end_inset

 We also impose the constraint 
\begin_inset Formula $F_{0}\ket{G}=0$
\end_inset

, from which it follows 
\begin_inset Formula 
\[
F_{0}\ket{0}=0.
\]

\end_inset

Requiring 
\begin_inset Formula $\ket{0}$
\end_inset

 annihilated by both 
\begin_inset Formula $F_{0}$
\end_inset

 and 
\begin_inset Formula $f_{0}$
\end_inset

 yields 
\begin_inset Formula 
\[
\ket{0}=\psi_{0}^{\left(1\right)}\psi_{0}^{\left(2\right)}
\]

\end_inset

 
\begin_inset Formula 
\begin{eqnarray*}
\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},F_{m}^{\dagger}\right] & = & \frac{1}{2}\sum_{k,l}M_{kl}\left[f_{k}^{\dagger}f_{l}^{\dagger},F_{m}^{\dagger}\right]\\
 & = & \frac{1}{2}\sum_{k,l}M_{kl}\left(f_{k}^{\dagger}\left\{ f_{l}^{\dagger},F_{m}^{\dagger}\right\} -\left\{ f_{k}^{\dagger},F_{m}^{\dagger}\right\} f_{l}^{\dagger}\right)\\
 & = & \frac{1}{2}\sum_{k,l}M_{kl}\left(f_{k}^{\dagger}\delta_{ln}S_{mn}^{*}-\delta_{kn}S_{mn}^{*}f_{l}^{\dagger}\right)\\
 & = & \frac{1}{2}\sum_{k,l}M_{kl}\left(f_{k}^{\dagger}S_{ml}^{*}-S_{mk}^{*}f_{l}^{\dagger}\right)\\
 & = & \frac{1}{2}\sum_{k,l}\left(-S_{ml}^{*}M_{lk}f_{k}^{\dagger}-S_{mk}^{*}M_{kl}f_{l}^{\dagger}\right)\\
 & = & -\sum_{k,l}S_{ml}^{*}M_{lk}f_{k}^{\dagger}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},F_{n}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\right] & = & F_{n}^{\dagger}\bar{A}_{nm}\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},F_{m}^{\dagger}\right]+\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},F_{n}^{\dagger}\right]\bar{A}_{nm}F_{m}^{\dagger}\\
 & = & -F_{n}^{\dagger}\bar{A}_{nm}S_{ml}^{*}M_{lk}f_{k}^{\dagger}-S_{nl}^{*}M_{lk}f_{k}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\\
 & = & \bar{A}_{nm}S_{ml}^{*}M_{lk}\left(f_{k}^{\dagger}F_{n}^{\dagger}-\left\{ F_{n}^{\dagger},f_{k}^{\dagger}\right\} \right)-S_{nl}^{*}M_{lk}f_{k}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\\
 & = & \bar{A}_{nm}S_{ml}^{*}M_{lk}\left(f_{k}^{\dagger}F_{n}^{\dagger}-S_{nk}^{*}\right)-S_{nl}^{*}M_{lk}f_{k}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\\
 & = & \bar{A}_{nm}S_{ml}^{*}M_{lk}f_{k}^{\dagger}F_{n}^{\dagger}-S_{nl}^{*}M_{lk}f_{k}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}-\bar{A}_{nm}S_{ml}^{*}M_{lk}S_{nk}^{*}\\
 & = & f_{k}^{\dagger}M_{kl}\left(S^{\dagger}\right)_{lm}\bar{A}_{mn}F_{n}^{\dagger}+f_{k}^{\dagger}M_{kl}\left(S^{\dagger}\right)_{ln}\bar{A}_{nm}F_{m}^{\dagger}-\bar{A}_{nm}S_{ml}^{*}M_{lk}\left(S^{\dagger}\right)_{kn}\\
 & = & 2f_{k}^{\dagger}\left(MS^{\dagger}\bar{A}\right)_{km}F_{m}^{\dagger}-\tr\left(\bar{A}S^{*}MS^{\dagger}\right)
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},F_{n}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\right]\ket{0} & = & \left[2f_{k}^{\dagger}\left(MS^{\dagger}\bar{A}\right)_{km}f_{n}^{\dagger}C_{mn}^{*}-\tr\left(\bar{A}S^{*}MS^{\dagger}\right)\right]\ket{0}\\
 & = & \left[2f_{k}^{\dagger}\left(MS^{\dagger}\bar{A}C^{*}\right)_{kl}f_{l}^{\dagger}-\tr\left(\bar{A}S^{*}MS^{\dagger}\right)\right]\ket{0}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},2f_{k}^{\dagger}M_{kl}\left(S^{\dagger}\right)_{ln}\bar{A}_{nm}F_{m}^{\dagger}\right] & = & 2f_{i}^{\dagger}\left(MS^{\dagger}\bar{A}\right)_{im}\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},F_{m}^{\dagger}\right]\\
 & = & 2f_{i}^{\dagger}\left(MS^{\dagger}\bar{A}\right)_{im}\left(-S_{ml}^{*}M_{lk}f_{k}^{\dagger}\right)\\
 & = & -2f_{k}^{\dagger}\left(MS^{\dagger}\bar{A}S^{*}M\right)_{kl}f_{l}^{\dagger}
\end{eqnarray*}

\end_inset

it follows that 
\begin_inset Formula 
\[
\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},F_{n}^{\dagger}F_{m}^{\dagger}\right]\right]=-2f_{k}^{\dagger}\left(MS^{\dagger}\bar{A}S^{*}M\right)_{kl}f_{l}^{\dagger}
\]

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\ket{0} & = & \left(\bar{A}_{nm}f_{i}^{\dagger}f_{j}^{\dagger}C_{ni}^{*}C_{mj}^{*}+\bar{A}_{nm}f_{i}S_{ni}^{*}f_{j}^{\dagger}C_{mj}^{*}\right)\ket{0}\\
 & = & \left(\bar{A}_{nm}f_{i}^{\dagger}f_{j}^{\dagger}C_{ni}^{*}C_{mj}^{*}+\bar{A}_{nm}S_{ni}^{*}C_{mj}^{*}\left\{ f_{i},f_{j}^{\dagger}\right\} \right)\ket{0}\\
 & = & \left[f_{i}^{\dagger}\left(C^{\dagger}\bar{A}C^{*}\right)_{ij}f_{j}^{\dagger}+\tr\left(\bar{A}C^{*}S^{\dagger}\right)\right]\ket{0}
\end{eqnarray*}

\end_inset

 Denote 
\begin_inset Formula $\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}$
\end_inset

 as 
\begin_inset Formula $X$
\end_inset

,
\begin_inset Formula 
\begin{eqnarray*}
F_{m}^{\dagger}\ket{G} & = & \exp\left(X\right)\left(F_{m}^{\dagger}-\left[X,F_{m}^{\dagger}\right]\right)\ket{0}\\
 & = & \exp\left(X\right)\left(C_{mn}^{*}f_{n}^{\dagger}+S_{ml}^{*}M_{lk}f_{k}^{\dagger}\right)\ket{0}\\
 & = & \left(C^{*}+S^{*}M\right)_{mn}f_{n}^{\dagger}\ket{G}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
X_{m}F_{0}^{\dagger}F_{m}^{\dagger}\ket{G} & = & -X_{m}\left(C^{*}+S^{*}M\right)_{mn}f_{n}^{\dagger}F_{0}^{\dagger}\ket{G}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
F_{n}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\ket{G} & = & \exp\left(X\right)\left(\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}-\left[X,\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\right]+\frac{1}{2}\left[X,\left[X,\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\right]\right]\right)\ket{0}\\
 & = & \left[f_{k}^{\dagger}\left(C^{\dagger}\bar{A}C^{*}\right)_{kl}f_{l}^{\dagger}+\tr\left(\bar{A}C^{*}S^{\dagger}\right)\right]\ket{G}\\
 &  & -\left[2f_{k}^{\dagger}\left(MS^{\dagger}\bar{A}C^{*}\right)_{kl}f_{l}^{\dagger}-\tr\left(\bar{A}S^{*}MS^{\dagger}\right)\right]\ket{G}\\
 &  & -f_{k}^{\dagger}\left(MS^{\dagger}\bar{A}S^{*}M\right)_{kl}f_{l}^{\dagger}\ket{G}\\
 & = & \tr\left[\bar{A}\left(C^{*}+S^{*}M\right)S^{\dagger}\right]\ket{G}\\
 &  & +f_{k}^{\dagger}\left(C^{\dagger}\bar{A}C^{*}-2MS^{\dagger}\bar{A}C^{*}-MS^{\dagger}\bar{A}S^{*}M\right)_{kl}f_{l}^{\dagger}\ket{G}\\
 & = & \tr\left[\bar{A}\left(C^{*}+S^{*}M\right)S^{\dagger}\right]\ket{G}\\
 &  & +f_{k}^{\dagger}\left(C^{\dagger}\bar{A}C^{*}-MS^{\dagger}\bar{A}C^{*}+C^{\dagger}\bar{A}S^{*}M-MS^{\dagger}\bar{A}S^{*}M\right)_{kl}f_{l}^{\dagger}\ket{G}\\
 & = & \tr\left[\bar{A}\left(C^{*}+S^{*}M\right)S^{\dagger}\right]\ket{G}\\
 &  & +f_{k}^{\dagger}\left[C^{\dagger}\bar{A}\left(C^{*}+S^{*}M\right)-MS^{\dagger}\bar{A}\left(S^{*}M+C^{*}\right)\right]_{kl}f_{l}^{\dagger}\ket{G}\\
 & = & \tr\left[\bar{A}\left(C^{*}+S^{*}M\right)S^{\dagger}\right]\ket{G}\\
 &  & +f_{k}^{\dagger}\left[\left(C^{*}+S^{*}M\right)^{T}\bar{A}\left(C^{*}+S^{*}M\right)\right]_{kl}f_{l}^{\dagger}\ket{G}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
C^{*}+S^{*}M & = & C^{*}+\left(-C^{*}M^{*}\right)M\\
 & = & C^{*}\left(I-M^{*}M\right)\\
 & = & C^{*}\left(I+MM^{\dagger}\right)^{T}\\
 & = & C^{*}\left(C^{-1}C^{-1\dagger}\right)^{T}\\
 & = & \left(C^{-1}\right)^{T}
\end{eqnarray*}

\end_inset

 Therefore, 
\begin_inset Formula 
\[
X_{m}F_{0}^{\dagger}F_{m}^{\dagger}\ket{G}=X_{m}F_{0}^{\dagger}C_{nm}^{-1}f_{n}^{\dagger}\ket{G}=F_{0}^{\dagger}f_{n}^{\dagger}C_{nm}^{-1}X_{m}\ket{G}
\]

\end_inset


\begin_inset Formula 
\[
F_{n}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\ket{G}=-\tr\left(S^{*}C^{-1}\bar{A}\right)\ket{G}+f_{k}^{\dagger}\left[C^{-1}\bar{A}\left(C^{-1}\right)^{T}\right]_{kl}f_{l}^{\dagger}\ket{G}
\]

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
h_{kl}\ket{G} & = & \frac{2}{M}\left[\gamma-\tr\left(S^{*}C^{-1}\bar{A}\right)\right]\ket{G}\\
 &  & +\frac{2}{M}\sum_{m,n=0}^{M-2}\left\{ f_{m}^{\dagger}\left[C^{-1}\bar{A}\left(C^{-1}\right)^{T}\right]_{mn}f_{n}^{\dagger}+F_{0}^{\dagger}f_{m}^{\dagger}C_{mn}^{-1}X_{n}\right\} \ket{G}\\
 & = & \frac{2}{M}\left(\gamma^{\prime}+\Omega_{mn}\frac{d}{dM_{mn}}+F_{0}^{\dagger}f_{m}^{\dagger}\Sigma_{m}\right)\exp\left(\frac{1}{2}M_{mn}f_{m}^{\dagger}f_{n}^{\dagger}\right)\ket{0}
\end{eqnarray*}

\end_inset

 where 
\begin_inset Formula 
\[
\gamma^{\prime}=\gamma-\tr\left(S^{*}C^{-1}\bar{A}\right)
\]

\end_inset


\begin_inset Formula 
\[
\Omega_{mn}=\left[C^{-1}\bar{A}\left(C^{-1}\right)^{T}\right]_{mn},\quad\Sigma_{m}=C_{mn}^{-1}X_{n}.
\]

\end_inset

 For 
\begin_inset Formula $k_{1}<k_{2}<\cdots<k_{2n}$
\end_inset


\begin_inset Formula 
\[
\left\langle f_{k_{2n}}f_{k_{2n-1}}\cdots f_{k_{1}}\exp\left(\frac{1}{2}M_{mn}f_{m}^{\dagger}f_{n}^{\dagger}\right)\right\rangle =\sum_{s_{i}}\left(-1\right)^{\pi\left(s_{i}\right)}\left(M_{k_{s_{1}}k_{s_{2}}}M_{k_{s_{3}}k_{s_{4}}}\cdots M_{k_{s_{2n-1}}k_{s_{2n}}}\right)
\]

\end_inset

 where 
\begin_inset Formula 
\[
s_{1}<s_{3}<\cdots<s_{2n-1},\quad s_{2i-1}<s_{2i}.
\]

\end_inset


\end_layout

\begin_layout Section
\start_of_appendix
The Hamiltonian and its action on color singlets
\end_layout

\begin_layout Standard
The (anti)-communication relations among string bit creation and annihilation
 operators is
\begin_inset Formula 
\begin{eqnarray}
\left[\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta},\left(\bar{\phi}_{b_{1}\cdots b_{m}}\right)_{\gamma}^{\delta}\right]_{\pm} & \equiv & \left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\left(\bar{\phi}_{b_{1}\cdots b_{m}}\right)_{\gamma}^{\delta}-\left(-1\right)^{mn}\left(\bar{\phi}_{b_{1}\cdots b_{m}}\right)_{\gamma}^{\delta}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\nonumber \\
 & = & \delta_{mn}\delta_{\alpha}^{\delta}\delta_{\gamma}^{\beta}\sum_{P}\left(-1\right)^{P}\delta_{a_{1}b_{P_{1}}}\cdots\delta_{a_{n}b_{P_{n}}}.\label{eq:App-H-commutation}
\end{eqnarray}

\end_inset

 The creation operator 
\begin_inset Formula $\bar{\phi}$
\end_inset

 creates a boson if 
\begin_inset Formula $n$
\end_inset

 is even and fermion if 
\begin_inset Formula $n$
\end_inset

 is odd.
 The summation go through all permutations of 
\begin_inset Formula $1,2,\dots,n$
\end_inset

.
 
\end_layout

\begin_layout Standard
The Hamiltonian in the paper can be written as 
\begin_inset Formula 
\[
H_{S}=H_{1}+H_{2}+H_{3}+H_{4}+H_{5},
\]

\end_inset

 where 
\begin_inset Formula $H_{i}$
\end_inset

 are
\begin_inset Formula 
\[
H_{1}=\frac{2}{N}\sum_{n=0}^{s}\sum_{k=0}^{s}\frac{s-2n}{n!k!}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{1}}\phi_{a_{1}\cdots a_{n}}
\]

\end_inset


\begin_inset Formula 
\[
H_{2}=\frac{2}{N}\sum_{n=0}^{s-1}\sum_{k=0}^{s-1}\frac{\left(-1\right)^{k}}{n!k!}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\bar{\phi}_{bb_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{1}}\phi_{ba_{1}\cdots a_{n}}
\]

\end_inset


\begin_inset Formula 
\[
H_{3}=\frac{2}{N}\sum_{n=0}^{s-1}\sum_{k=0}^{s-1}\frac{\left(-1\right)^{k}}{n!k!}\tr\bar{\phi}_{ba_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{bb_{1}\cdots b_{1}}\phi_{a_{1}\cdots a_{n}}
\]

\end_inset


\begin_inset Formula 
\[
H_{4}=\frac{2i}{N}\sum_{n=0}^{s-1}\sum_{k=0}^{s-1}\frac{\left(-1\right)^{k}}{n!k!}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{bb_{1}\cdots b_{1}}\phi_{ba_{1}\cdots a_{n}}
\]

\end_inset


\begin_inset Formula 
\[
H_{5}=-\frac{2i}{N}\sum_{n=0}^{s-1}\sum_{k=0}^{s-1}\frac{\left(-1\right)^{k}}{n!k!}\tr\bar{\phi}_{ba_{1}\cdots a_{n}}\bar{\phi}_{bb_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{1}}\phi_{a_{1}\cdots a_{n}}.
\]

\end_inset

 
\begin_inset Formula $H_{S}$
\end_inset

 commutes with the supersymmetry operators 
\begin_inset Formula 
\[
Q^{a}=\sum_{n=0}^{s-1}\frac{\left(-1\right)^{n}}{n!}\tr\left[e^{i\pi/4}\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{aa_{1}\cdots a_{n}}+e^{-i\pi/4}\bar{\phi}_{aa_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\right]
\]

\end_inset


\begin_inset Formula 
\[
\left\{ Q^{a},Q^{b}\right\} =2M\delta_{ab},
\]

\end_inset

 which will guarantee equal numbers of bosonic and fermionic eigensates
 at each energy level.
 
\end_layout

\begin_layout Standard
Using the commutation relations 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:App-H-commutation"

\end_inset

, we otain the action of 
\begin_inset Formula $H_{i}$
\end_inset

 on single trace states:
\begin_inset Formula 
\begin{eqnarray*}
H_{1}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0} & = & 2\sum_{k=1}^{M}\left(s-2\theta_{k}^{a}\frac{d}{d\theta_{k}^{a}}\right)T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}\\
 &  & +\frac{2}{N}\sum_{k=1}^{M}\left(s-2\theta_{k}^{a}\frac{d}{d\theta_{k}^{a}}\right)\sum_{l\neq k,k+1}T\left(\theta_{1},\cdots,\theta_{k}\right)T\left(\theta_{k+1},\cdots,\theta_{l-1}\right)\ket{0}
\end{eqnarray*}

\end_inset

 
\begin_inset Formula 
\begin{eqnarray*}
H_{2}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0} & = & 2\sum_{k=1}^{M}\theta_{k}^{a}\frac{d}{d\theta_{k+1}^{a}}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}\\
 &  & +\frac{2}{N}\sum_{k=1}^{M}\sum_{l\neq k,k+1}\theta_{k}^{a}\frac{d}{d\theta_{l}^{a}}T\left(\theta_{1},\cdots,\theta_{k}\right)T\left(\theta_{k+1},\cdots,\theta_{l-1}\right)\ket{0}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
H_{3}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0} & = & 2\sum_{k=1}^{M}\theta_{k+1}^{a}\frac{d}{d\theta_{k}^{a}}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}\\
 &  & +\frac{2}{N}\sum_{k=1}^{M}\sum_{l\neq k,k+1}\theta_{l}^{a}\frac{d}{d\theta_{k}^{a}}T\left(\theta_{1},\cdots,\theta_{k}\right)T\left(\theta_{k+1},\cdots,\theta_{l-1}\right)\ket{0}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
H_{4}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0} & = & -2i\sum_{k=1}^{M}\theta_{k}^{a}\theta_{k+1}^{a}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}\\
 &  & -\frac{2i}{N}\sum_{k=1}^{M}\sum_{l\neq k,k+1}\theta_{k}^{a}\theta_{l}^{a}T\left(\theta_{1},\cdots,\theta_{k}\right)T\left(\theta_{k+1},\cdots,\theta_{l-1}\right)\ket{0}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
H_{5}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0} & = & -2i\sum_{k=1}^{M}\frac{d}{d\theta_{k}^{a}}\frac{d}{d\theta_{k+1}^{a}}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}\\
 &  & -\frac{2i}{N}\sum_{k=1}^{M}\sum_{l\neq k,k+1}\frac{d}{d\theta_{k}^{a}}\frac{d}{d\theta_{l}^{a}}T\left(\theta_{1},\cdots,\theta_{k}\right)T\left(\theta_{k+1},\cdots,\theta_{l-1}\right)\ket{0}
\end{eqnarray*}

\end_inset

 The action of 
\begin_inset Formula $H_{i}$
\end_inset

 on double traces
\begin_inset Formula 
\begin{eqnarray*}
H_{1}T\left(\theta_{1}\cdots\theta_{K}\right)T\left(\eta_{1}\cdots\eta_{L}\right)\ket{0}_{\mathrm{Fussion}} & = & \frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}\left(s-2\theta_{k}^{a}\frac{d}{d\theta_{k}^{a}}\right)T\left(\theta_{k+1}\cdots\theta_{k}\eta_{l}\cdots\eta_{l-1}\right)\ket{0}\\
 &  & +\frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}\left(s-2\eta_{l}^{a}\frac{d}{d\eta_{l}^{a}}\right)T\left(\theta_{k}\cdots\theta_{k-1}\eta_{l+1}\cdots\eta_{l}\right)\ket{0}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
H_{2}T\left(\theta_{1}\cdots\theta_{K}\right)T\left(\eta_{1}\cdots\eta_{L}\right)\ket{0}_{\mathrm{Fussion}} & = & \frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}\theta_{k}^{a}\frac{d}{d\eta_{l}}T\left(\theta_{k+1}\cdots\theta_{k}\eta_{l}\cdots\eta_{l-1}\right)\ket{0}\\
 &  & +\frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}\eta_{l}^{a}\frac{d}{d\theta_{k}^{a}}T\left(\theta_{k}\cdots\theta_{k-1}\eta_{l+1}\cdots\eta_{l}\right)\ket{0}.
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Section
Eigenfunction and eigenstates
\end_layout

\begin_layout Subsection*
M=2
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\psi_{0}=\prod_{k=1}^{M/2-1}\left(c_{k}-s_{k}\alpha_{M-k}\alpha_{k}\right),\quad\bar{\psi}_{0}=i^{M/2+1}\prod_{i=1}^{M/2-1}\left(-s_{i}+c_{i}\alpha_{M-i}\alpha_{i}\right)\alpha_{0}\alpha_{M/2}
\]

\end_inset


\begin_inset Formula 
\[
\psi=F_{k_{s}}^{\dagger}F_{k_{s-1}}^{\dagger}\cdots F_{k_{1}}^{\dagger}\psi_{0},\quad\bar{\psi}=F_{k_{1}}^{+}F_{k_{2}}^{+}\cdots F_{k_{s}}^{+}\psi_{0},\quad0\leq k_{i}\leq M-1
\]

\end_inset


\begin_inset Formula 
\[
\psi_{0}=1,\quad\bar{\psi}_{0}=-\alpha_{0}\alpha_{1},
\]

\end_inset


\begin_inset Formula 
\[
\psi_{1}=F_{1}^{\dagger}\psi_{0}=\alpha_{1},\quad\bar{\psi}_{1}=F_{1}^{+}\bar{\psi}_{0}=-\beta_{1}\bar{\psi}_{0}=-\alpha_{0},
\]

\end_inset


\begin_inset Formula 
\[
\psi_{2}=F_{1}^{\dagger}F_{0}^{\dagger}\psi_{0}=-\alpha_{0}\alpha_{1},\quad\bar{\psi}_{2}=F_{0}^{+}F_{1}^{+}\bar{\psi}_{0}=-\beta_{0}\left(-\alpha_{0}\right)=1,
\]

\end_inset

 where 
\begin_inset Formula $\psi_{0}$
\end_inset

 does not satisfy the cyclic symmetry requirement and is unphysical.
 We can find 
\begin_inset Formula $T_{1}$
\end_inset

 and 
\begin_inset Formula $T_{2}$
\end_inset

,
\begin_inset Formula 
\[
T=\tr\left(\bar{a}+\bar{b}\theta_{1}\right)\left(\bar{a}+\bar{b}\theta_{2}\right)
\]

\end_inset

 
\begin_inset Formula 
\begin{eqnarray*}
T_{1} & = & \int d\theta_{1}d\theta_{2}\, T\psi_{1}\\
 & = & \frac{1}{\sqrt{2}}\int d\theta_{1}d\theta_{2}\,\tr\left(\bar{a}+\bar{b}\theta_{1}\right)\left(\bar{a}+\bar{b}\theta_{2}\right)\left(\theta_{2}-\theta_{1}\right)\\
 & = & -\sqrt{2}\tr\bar{a}\bar{b}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
T_{2} & = & \int d\theta_{1}d\theta_{2}\, T\psi_{2}\\
 & = & -\int d\theta_{1}d\theta_{2}\, T\alpha_{0}\alpha_{1}\\
 & = & -\frac{1}{2}\int d\theta_{1}d\theta_{2}\, T\left(\theta_{1}+\theta_{2}\right)\left(-\theta_{1}+\theta_{2}\right)\\
 & = & \int d\theta_{1}d\theta_{2}\,\tr\left(\bar{a}+\bar{b}\theta_{1}\right)\left(\bar{a}+\bar{b}\theta_{2}\right)\theta_{2}\theta_{1}\\
 & = & \tr\bar{a}\bar{a}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Let us verify the completeness of the eigenfunctions.
 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\sum_{r=1}^{2}\psi_{r}\bar{\psi}_{r}\left(\phi\right) & = & -\alpha_{1}\alpha_{0}\left(\phi\right)-\alpha_{0}\alpha_{1}\\
 & = & \frac{1}{2}\left(\theta_{1}-\theta_{2}\right)\left(\phi_{1}+\phi_{2}\right)-\frac{1}{2}\left(\theta_{1}+\theta_{2}\right)\left(\theta_{2}-\theta_{1}\right)\\
 & = & \frac{1}{2}\left(\theta_{1}-\theta_{2}\right)\left(\phi_{1}+\phi_{2}\right)+\theta_{2}\theta_{1}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\int d^{2}\tr\left(\bar{a}+\bar{b}\theta_{1}\right)\left(\bar{a}+\bar{b}\theta_{2}\right)\sum_{r}\psi_{r}\bar{\psi}_{r}\left(\phi\right) & = & \sum_{r}T_{r}\bar{\psi}_{r}\\
 & = & -\sqrt{2}\tr\bar{a}\bar{b}\left(-\frac{1}{\sqrt{2}}\right)\left(\phi_{1}+\phi_{2}\right)+\tr\bar{a}\bar{a}\\
 & = & \tr\left(\bar{a}+\bar{b}\phi_{1}\right)\left(\bar{a}+\bar{b}\phi_{2}\right)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Subsection*
M=3
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\psi_{0}=\prod_{k=1}^{\left\lfloor M/2\right\rfloor }\left(c_{k}-s_{k}\alpha_{M-k}\alpha_{k}\right),\quad\tilde{\psi}_{0}=\left(-i\right)^{\left\lfloor M/2\right\rfloor }\alpha_{0}\prod_{i=1}^{\left\lfloor M/2\right\rfloor }\left(-s_{i}+c_{i}\alpha_{M-i}\alpha_{i}\right)
\]

\end_inset


\begin_inset Formula 
\[
\psi=F_{k_{s}}^{\dagger}F_{k_{s-1}}^{\dagger}\cdots F_{k_{1}}^{\dagger}\psi_{0},\quad\bar{\psi}=F_{k_{1}}^{-}F_{k_{2}}^{-}\cdots F_{k_{s}}^{-}\bar{\psi}_{0}
\]

\end_inset


\end_layout

\begin_layout Standard
Firstly we calculate
\begin_inset Formula 
\begin{eqnarray*}
\alpha_{M-k}\alpha_{k} & = & \frac{1}{M}\sum_{i,j=1}^{M}\theta_{i}\theta_{j}e^{-2\pi i\left[i\left(M-k\right)+jk\right]/M}\\
 & = & \frac{1}{M}\sum_{i,j=1}^{M}\theta_{i}\theta_{j}e^{-2\pi i\left(j-i\right)k/M}\\
 & = & -\frac{2i}{M}\sum_{i=1}^{M}\sum_{j>i}^{M}\theta_{i}\theta_{j}\sin\frac{2\pi k\left(j-i\right)}{M}
\end{eqnarray*}

\end_inset

For 
\begin_inset Formula $M=3$
\end_inset

,
\begin_inset Formula 
\begin{eqnarray*}
\alpha_{2}\alpha_{1} & = & -\frac{2i}{3}\sum_{i=1}^{3}\sum_{j>i}^{3}\theta_{i}\theta_{j}\sin\frac{2\pi\left(j-i\right)}{3}\\
 & = & -\frac{i}{\sqrt{3}}\left(\theta_{1}\theta_{2}+\theta_{2}\theta_{3}+\theta_{3}\theta_{1}\right)
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\alpha_{0}\alpha_{1}\alpha_{2} & = & \frac{1}{\sqrt{3}}\left(\theta_{1}+\theta_{2}+\theta_{3}\right)\frac{i}{\sqrt{3}}\left(\theta_{1}\theta_{2}+\theta_{2}\theta_{3}+\theta_{3}\theta_{1}\right)\\
 & = & i\theta_{1}\theta_{2}\theta_{3}\\
 & = & -i\theta_{3}\theta_{2}\theta_{1}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\psi_{0} & = & c_{1}-s_{1}\alpha_{2}\alpha_{1}\\
 & = & c_{1}+s_{1}\frac{i}{\sqrt{3}}\left(\theta_{1}\theta_{2}+\theta_{2}\theta_{3}+\theta_{3}\theta_{1}\right)
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\bar{\psi}_{0} & = & -i\alpha_{0}\left(-s_{1}+c_{1}\alpha_{2}\alpha_{1}\right)\\
 & = & \frac{is_{1}}{\sqrt{3}}\left(\theta_{1}+\theta_{2}+\theta_{3}\right)-ic_{1}\alpha_{2}\alpha_{1}\alpha_{0}\\
 & = & \frac{is_{1}}{\sqrt{3}}\left(\theta_{1}+\theta_{2}+\theta_{3}\right)-c_{1}\theta_{1}\theta_{2}\theta_{3}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\psi_{1} & = & F_{0}^{\dagger}\psi_{0}=\alpha_{0}\psi_{0}\\
 & = & \frac{1}{\sqrt{3}}\left(\theta_{1}+\theta_{2}+\theta_{3}\right)\left[c_{1}+s_{1}\frac{i}{\sqrt{3}}\left(\theta_{1}\theta_{2}+\theta_{2}\theta_{3}+\theta_{3}\theta_{1}\right)\right]\\
 & = & \frac{c_{1}}{\sqrt{3}}\left(\theta_{1}+\theta_{2}+\theta_{3}\right)+is_{1}\theta_{1}\theta_{2}\theta_{3}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\bar{\psi}_{1} & = & F_{0}^{-}\bar{\psi}_{0}=\beta_{0}\bar{\psi}_{0}\\
 & = & -i\left(-s_{1}+c_{1}\alpha_{2}\alpha_{1}\right)\\
 & = & is_{1}-\frac{c_{1}}{\sqrt{3}}\left(\theta_{1}\theta_{2}+\theta_{2}\theta_{3}+\theta_{3}\theta_{1}\right)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
F_{M-k}^{\pm}F_{k}^{\pm}\bar{\phi}_{k} & = & -\left(-s_{k}\alpha_{M-k}-c_{k}\beta_{M-k}\right)\alpha_{k}\\
 & = & s_{k}\alpha_{M-k}\alpha_{k}+c_{k}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\psi_{2} & = & F_{2}^{\dagger}F_{1}^{\dagger}\psi_{0}\\
 & = & -\left(s_{1}+c_{1}\alpha_{2}\alpha_{1}\right)\\
 & = & -s_{1}+\frac{ic_{1}}{\sqrt{3}}\left(\theta_{1}\theta_{2}+\theta_{2}\theta_{3}+\theta_{3}\theta_{1}\right)
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\bar{\psi}_{2} & = & F_{1}^{-}F_{2}^{-}\bar{\psi}_{0}=-F_{2}^{-}F_{1}^{-}\bar{\psi}_{0}\\
 & = & i\alpha_{0}\left(s_{1}\alpha_{2}\alpha_{1}+c_{1}\right)\\
 & = & \frac{ic_{1}}{\sqrt{3}}\left(\theta_{1}+\theta_{2}+\theta_{3}\right)+s_{1}\theta_{1}\theta_{2}\theta_{3}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\psi_{3} & = & F_{2}^{\dagger}F_{1}^{\dagger}F_{0}^{\dagger}\psi_{0}=\alpha_{0}\psi_{2}\\
 & = & -\frac{s_{1}}{\sqrt{3}}\left(\theta_{1}+\theta_{2}+\theta_{3}\right)+ic_{1}\theta_{1}\theta_{2}\theta_{3}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\bar{\psi}_{3} & = & F_{0}^{-}F_{1}^{-}F_{2}^{-}\bar{\psi}_{0}=\beta_{0}\bar{\psi}_{2}\\
 & = & i\left(s_{1}\alpha_{2}\alpha_{1}+c_{1}\right)\\
 & = & ic_{1}+\frac{s_{1}}{\sqrt{3}}\left(\theta_{1}\theta_{2}+\theta_{2}\theta_{3}+\theta_{3}\theta_{1}\right)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\psi_{0}=c_{1}-s_{1}\alpha_{2}\alpha_{1},\quad\bar{\psi}_{1}=-i\alpha_{0}\left(-s_{1}+c_{1}\alpha_{2}\alpha_{1}\right)
\]

\end_inset


\begin_inset Formula 
\[
\psi_{1}=\alpha_{0}\left(c_{1}-s_{1}\alpha_{2}\alpha_{1}\right),\quad\bar{\psi}_{1}=-i\left(-s_{1}+c_{1}\alpha_{2}\alpha_{1}\right)
\]

\end_inset


\begin_inset Formula 
\[
\psi_{2}=-s_{1}-c_{1}\alpha_{2}\alpha_{1},\quad\bar{\psi}_{2}=i\alpha_{0}\left(c_{1}+s_{1}\alpha_{2}\alpha_{1}\right)
\]

\end_inset


\begin_inset Formula 
\[
\psi_{3}=-\alpha_{0}\left(s_{1}+c_{1}\alpha_{2}\alpha_{1}\right),\quad\bar{\psi}_{3}=i\left(c_{1}+s_{1}\alpha_{2}\alpha_{1}\right)
\]

\end_inset

To verify:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\alpha_{0}=\frac{1}{\sqrt{3}}\left(\theta_{1}+\theta_{2}+\theta_{3}\right)
\]

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\int d^{3}\theta\,\bar{\psi}_{1}\psi_{1} & = & -i\int d^{3}\theta\,\alpha_{0}\left(-s_{1}+c_{1}\alpha_{2}\alpha_{1}\right)\left(c_{1}-s_{1}\alpha_{2}\alpha_{1}\right)\\
 & = & -i\int d^{3}\theta\left(c_{1}^{2}+s_{1}^{2}\right)\alpha_{0}\alpha_{2}\alpha_{1}\\
 & = & -i\int d^{3}\theta\,\alpha_{2}\alpha_{1}\alpha_{0}\\
 & = & 1
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\int d^{3}\theta\,\bar{\psi}_{1}\psi_{3} & = & i\int d^{3}\theta\,\alpha_{0}\left(-s_{1}+c_{1}\alpha_{2}\alpha_{1}\right)\left(s_{1}+c_{1}\alpha_{2}\alpha_{1}\right)\\
 & = & i\int d^{3}\theta\,\left(-s_{1}c_{1}+s_{1}c_{1}\right)\alpha_{0}\alpha_{2}\alpha_{1}\\
 & = & 0
\end{eqnarray*}

\end_inset

 likewise
\begin_inset Formula 
\[
\int d^{3}\theta\,\bar{\psi}_{3}\psi_{1}=0,\quad\int d^{3}\theta\,\bar{\psi}_{3}\psi_{3}=1.
\]

\end_inset

 Let us now verify the completeness.
\begin_inset Formula 
\[
T\left(\theta\right)=\tr\left(\bar{a}+\bar{b}\theta_{1}\right)\left(\bar{a}+\bar{b}\theta_{2}\right)\left(\bar{a}+\bar{b}\theta_{3}\right)
\]

\end_inset

Denote 
\begin_inset Formula $\sigma_{k}=\alpha_{k}\left(\phi\right)$
\end_inset

 and 
\begin_inset Formula $\alpha_{k}=\alpha_{k}\left(\theta\right)$
\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\sum_{r}\psi_{r}\left(\theta\right)\bar{\psi}_{r}\left(\phi\right) & = & -i\left(c_{1}-s_{1}\alpha_{2}\alpha_{1}\right)\sigma_{0}\left(-s_{1}+c_{1}\sigma_{2}\sigma_{1}\right)\\
 &  & -i\alpha_{0}\left(c_{1}-s_{1}\alpha_{2}\alpha_{1}\right)\left(-s_{1}+c_{1}\sigma_{2}\sigma_{1}\right)\\
 &  & -i\left(s_{1}+c_{1}\alpha_{2}\alpha_{1}\right)\sigma_{0}\left(c_{1}+s_{1}\sigma_{2}\sigma_{1}\right)\\
 &  & -i\alpha_{0}\left(s_{1}+c_{1}\alpha_{2}\alpha_{1}\right)\left(c_{1}+s_{1}\sigma_{2}\sigma_{1}\right)\\
 & = & -i\alpha_{2}\alpha_{1}\alpha_{0}-i\alpha_{2}\alpha_{1}\sigma_{0}-i\alpha_{0}\sigma_{2}\sigma_{1}-i\sigma_{2}\sigma_{1}\sigma_{0}.
\end{eqnarray*}

\end_inset

 In the language of 
\begin_inset Formula $\theta$
\end_inset

,
\begin_inset Formula 
\begin{eqnarray*}
\sum_{r}\psi_{r}\left(\theta\right)\bar{\psi}_{r}\left(\phi\right) & = & \theta_{3}\theta_{2}\theta_{1}-\frac{1}{3}\left(\theta_{1}\theta_{2}+\theta_{2}\theta_{3}+\theta_{3}\theta_{1}\right)\left(\phi_{1}+\phi_{2}+\phi_{3}\right)\\
 &  & -\frac{1}{3}\left(\theta_{1}+\theta_{2}+\theta_{3}\right)\left(\phi_{1}\phi_{2}+\phi_{2}\phi_{3}+\phi_{3}\phi_{1}\right)+\phi_{3}\phi_{2}\phi_{1}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\int d^{3}\theta\, T\left(\theta\right)\sum_{r}\psi_{r}\left(\theta\right)\bar{\psi}_{r}\left(\phi\right) & = & \tr\bar{a}^{3}-\tr\bar{a}\bar{a}\bar{b}\left(\phi_{1}+\phi_{2}+\phi_{3}\right)\\
 &  & +\tr\bar{a}\bar{b}\bar{b}\left(\phi_{1}\phi_{2}+\phi_{2}\phi_{3}+\phi_{3}\phi_{1}\right)-\tr\bar{b}^{3}\\
 & = & T\left(-\phi\right)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Section
More Mathematical Details
\end_layout

\begin_layout Subsection
Symmetry of Matrices
\end_layout

\begin_layout Standard
Let us explore the symmetry of the matrices.
 For convenience, we define 
\begin_inset Formula 
\[
\hat{n}=\begin{cases}
L-n, & \quad1\leq n<L\\
M-n-1+L, & \quad L\leq n<M-1\\
M-1, & \quad n=M-1
\end{cases}.
\]

\end_inset

From 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_C_mn1"

\end_inset

-
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_S_mM1"

\end_inset

, we see that, for 
\begin_inset Formula $1\leq m\leq M-1$
\end_inset

,
\begin_inset Formula 
\begin{equation}
C_{mn}=C_{M-m,\hat{n}}^{*},\quad S_{mn}=-S_{M-m,\hat{n}}^{*},\quad1\leq n\leq M-1,\label{eq:overlap_CS_symmetry1}
\end{equation}

\end_inset

We now rename the indices of 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_CMS"

\end_inset

 as 
\begin_inset Formula $m\to M-m$
\end_inset

, 
\begin_inset Formula $n\to\hat{n}$
\end_inset

, 
\begin_inset Formula 
\begin{equation}
\sum_{n=1}^{M-1}C_{M-m,\hat{n}}D_{\hat{n}\hat{l}}+S_{M-m,\hat{l}}=0.
\end{equation}

\end_inset

 Taking compex conjugate and applying 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_CS_symmetry1"

\end_inset

 yields
\begin_inset Formula 
\begin{equation}
\sum_{n=1}^{M-1}C_{mn}D_{\hat{n}\hat{l}}^{*}-S_{ml}=0,\quad1\leq l\leq M-1,\label{eq:overlap_CMS2}
\end{equation}

\end_inset

Comparing 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_CMS2"

\end_inset

 and 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:over_CMS3"

\end_inset

 with 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_CMS"

\end_inset

, we arrive at
\begin_inset Formula 
\[
D_{nl}=-D_{\hat{n}\hat{l}}^{*}.
\]

\end_inset


\end_layout

\begin_layout Standard
Now let us consider 
\begin_inset Formula $C^{-1}$
\end_inset

, which satisfies 
\begin_inset Formula 
\[
C_{mn}C_{nl}^{-1}-\delta_{ml}=0.
\]

\end_inset

 Renaming the indecie as 
\begin_inset Formula $m\to M-m$
\end_inset

, 
\begin_inset Formula $n\to\hat{n}$
\end_inset

, 
\begin_inset Formula $l\to M-l$
\end_inset

 and taking complex conjugates yield 
\begin_inset Formula 
\[
C_{M-m,\hat{n}}^{*}\left(C_{\hat{n},M-l}^{-1}\right)^{*}-\delta_{ml}=0.
\]

\end_inset

Using 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_CS_symmetry1"

\end_inset

, we obtain 
\begin_inset Formula 
\[
\sum_{n=1}^{M-1}C_{mn}C_{\hat{n},M-l}^{-1*}-\delta_{ml}=0,
\]

\end_inset

 from which it follows
\begin_inset Formula 
\[
C_{nm}^{-1}=C_{\hat{n},M-m}^{-1*},\quad1\leq n<M-1
\]

\end_inset


\end_layout

\begin_layout Standard
With above results, we can show that 
\begin_inset Formula $\gamma^{\prime}=\gamma-\tr S^{*}C^{-1}A^{\dagger}$
\end_inset

 is always real.
 As 
\begin_inset Formula $\gamma$
\end_inset

 is real, we only need to show 
\begin_inset Formula $\tr S^{*}C^{-1}A^{\dagger}$
\end_inset

 is real.
 By inspection, we see that
\begin_inset Formula 
\[
A_{nm}=-A_{M-n,M-m}^{*},
\]

\end_inset

which implies 
\begin_inset Formula $A^{\dagger}$
\end_inset

 also satisifies the same relation 
\begin_inset Formula 
\[
A_{nm}^{\dagger}=-\left(A_{M-n,M-m}^{\dagger}\right)^{*}.
\]

\end_inset

 Now let 
\begin_inset Formula $X=\tr S^{*}C^{-1}A^{\dagger}$
\end_inset

, then 
\begin_inset Formula 
\[
X^{*}=S_{mn}C_{nl}^{-1*}A_{ml}.
\]

\end_inset

 Renaming indices as 
\begin_inset Formula $m\to M-m$
\end_inset

, 
\begin_inset Formula $n\to\hat{n}$
\end_inset

, 
\begin_inset Formula $l\to M-l$
\end_inset

, 
\begin_inset Formula 
\[
X^{*}=S_{M-m,\hat{n}}C_{\hat{n},M-l}^{-1*}A_{M-m,M-l}=S_{mn}^{*}C_{nl}^{-1}A_{ml}^{*}=X
\]

\end_inset

It then follows that 
\begin_inset Formula $X$
\end_inset

 is real and 
\begin_inset Formula $\gamma^{\prime}$
\end_inset

 is real.
\end_layout

\begin_layout Standard
We now consider the matrix 
\begin_inset Formula $\Omega$
\end_inset

.
 We have 
\begin_inset Formula 
\[
\Omega_{nk}=C_{nm}^{-1}A_{lm}^{*}C_{kl}^{-1}.
\]

\end_inset

 Changing indices as 
\begin_inset Formula $n\to\hat{n}$
\end_inset

, 
\begin_inset Formula $m\to M-m$
\end_inset

, 
\begin_inset Formula $l\to M-l$
\end_inset

, 
\begin_inset Formula $k\to\hat{k}$
\end_inset

 and taking complex conjugate yields 
\begin_inset Formula 
\begin{align*}
\Omega_{\hat{n}\hat{k}}^{*} & =\sum_{m,l=1}^{M-1}C_{\hat{n},M-m}^{-1*}A_{M-l,M-m}C_{\hat{k},M-l}^{-1*}=-\sum_{m,l=1}^{M-1}C_{nm}^{-1}A_{lm}^{*}C_{kl}^{-1}=-\Omega_{nk}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
We now consider the relation of matrices for 
\begin_inset Formula $L$
\end_inset

 and 
\begin_inset Formula $K$
\end_inset

.
 By inspection, we have 
\begin_inset Formula 
\[
C_{mn}^{\left(K\right)}=-\frac{1-e^{2\pi imK/M}}{1-e^{2\pi imL/M}}C_{m,L+n-1}\equiv-u_{m}^{\left(K\right)}C_{m,L+n-1}
\]

\end_inset


\begin_inset Formula 
\[
C_{m,K+n-1}^{\left(K\right)}=-u_{m}^{\left(K\right)}C_{mn}^{\left(L\right)},\quad C_{m,M-1}^{\left(K\right)}=u_{m}C_{m,M-1}^{\left(L\right)}.
\]

\end_inset

In matrix multiplication, we have
\begin_inset Formula 
\[
C^{\left(K\right)}=\begin{pmatrix}u_{1}^{\left(K\right)}\\
 & \ddots\\
 &  & u_{M-1}^{\left(K\right)}
\end{pmatrix}C^{\left(L\right)}\begin{pmatrix}0 & -\mathbf{I}_{L} & 0\\
-\mathbf{I}_{K} & 0 & 0\\
0 & 0 & 1
\end{pmatrix}.
\]

\end_inset

 Similarly, for the 
\begin_inset Formula $S$
\end_inset

 matrix
\begin_inset Formula 
\[
S^{\left(K\right)}=\begin{pmatrix}u_{1}^{\left(K\right)}\\
 & \ddots\\
 &  & u_{M-1}^{\left(K\right)}
\end{pmatrix}S^{\left(L\right)}\begin{pmatrix}0 & -\mathbf{I}_{L} & 0\\
-\mathbf{I}_{K} & 0 & 0\\
0 & 0 & 1
\end{pmatrix}.
\]

\end_inset

It then follows 
\begin_inset Formula 
\[
D^{\left(K\right)}=\begin{pmatrix}0 & -\mathbf{I}_{K} & 0\\
-\mathbf{I}_{L} & 0 & 0\\
0 & 0 & 1
\end{pmatrix}D^{\left(L\right)}\begin{pmatrix}0 & -\mathbf{I}_{L} & 0\\
-\mathbf{I}_{K} & 0 & 0\\
0 & 0 & 1
\end{pmatrix}
\]

\end_inset


\begin_inset Formula 
\[
D_{mn}^{\left(K\right)}=D_{L+m-1,L+n-1}^{\left(L\right)},\quad1\leq m,n<K
\]

\end_inset


\begin_inset Formula 
\[
D_{m,K+n-1}^{\left(K\right)}=D_{L+m-1,n}^{\left(L\right)},\quad1\leq m<K,\,1\leq n<L
\]

\end_inset


\begin_inset Formula 
\[
D_{m,M-1}^{\left(K\right)}=-D_{L+m-1,M-1}^{\left(L\right)},\quad1\leq m<K
\]

\end_inset


\begin_inset Formula 
\[
D_{K+m-1,n}^{\left(K\right)}=D_{m,L+n-1}^{\left(L\right)},\quad1\leq m<L,\,1\leq n<K
\]

\end_inset


\begin_inset Formula 
\[
D_{K+m-1,K+n-1}^{\left(K\right)}=D_{m,n}^{\left(L\right)},\quad1\leq m<L,\,1\leq n<L
\]

\end_inset


\begin_inset Formula 
\[
D_{K+m-1,M-1}^{\left(K\right)}=-D_{m,M-1}^{\left(L\right)}\quad1\leq m<L
\]

\end_inset


\end_layout

\begin_layout Section
Divergence of Bosonic String One-Loop Amplitudes
\end_layout

\begin_layout Standard
In this appendix we consider the divergence of bosonic string one-loop amplitude
s.
 Here, we only consider the case of two string vertex.
 The leading order divergence of open string one-loop amplitude is
\begin_inset Formula 
\[
A\sim\int_{0}^{1}\frac{dq}{q}w^{\left(D-26\right)/24}q^{\left(2-D\right)/12}\left[f\left(q^{2}\right)\right]^{2-D}
\]

\end_inset

 where 
\begin_inset Formula 
\[
f\left(q^{2}\right)=\prod_{i=1}^{\infty}\left(1-q^{2n}\right)
\]

\end_inset


\begin_inset Formula 
\[
q=\exp\left(\frac{2\pi^{2}}{\ln w}\right)
\]

\end_inset

The divergence comes from the case when 
\begin_inset Formula $q\to0$
\end_inset

, i.e., 
\begin_inset Formula $w\to1^{-}$
\end_inset

.
 
\end_layout

\begin_layout Standard
For two string interaction, the conformal mapping is 
\begin_inset Formula 
\[
\rho=\alpha_{1}\left(\ln\psi\left(\frac{x_{1}}{z},w\right)-\ln\psi\left(\frac{w}{z},w\right)\right)
\]

\end_inset

 where 
\begin_inset Formula 
\[
\ln\psi\left(x,w\right)=\ln\left(\frac{1-x}{\sqrt{x}}\right)+\frac{\ln^{2}x}{2\ln w}+\sum_{n=1}^{\infty}\left[\ln\left(1-w^{n}x\right)+\ln\left(1-w^{n}/x\right)-2\ln\left(1-w^{n}\right)\right].
\]

\end_inset

 Let 
\begin_inset Formula $x\to-1+\epsilon$
\end_inset

, 
\begin_inset Formula $w\to1-\delta$
\end_inset

, 
\begin_inset Formula 
\begin{align*}
\Re\ln\psi\left(x,\omega\right) & =\Re\ln\left(\frac{2-\epsilon}{\sqrt{-1+\epsilon}}\right)+\Re\frac{\ln^{2}\left(-1+\epsilon\right)}{2\ln\left(1-\delta\right)}+\Re\sum_{n=1}^{\infty}\ln\left(1-w^{n}x-w^{n}/x+w^{2n}\right)\\
 & =\ln\frac{2-\epsilon}{\sqrt{1-\epsilon}}+\frac{\ln^{2}\left(1-\epsilon\right)-\pi^{2}}{2\ln\left(1-\delta\right)}+\sum_{n=1}^{\infty}\ln\left(1-w^{n}\left(-1+\epsilon\right)+\frac{w^{n}}{1-\epsilon}+w^{2n}\right)\\
 & \approx\ln2+\frac{\epsilon^{2}}{8}-\frac{\epsilon^{2}+\epsilon^{3}}{2\delta}+\frac{\pi^{2}}{2\delta}\\
 & +\sum_{n=1}^{\infty}\ln\left(1+2w^{n}+w^{2n}+\epsilon^{2}w^{n}+\epsilon^{3}w^{n}\right)+\mathcal{O}\left(\epsilon^{3}\right)\\
 & \approx\ln2+\frac{\epsilon^{2}}{8}-\frac{\epsilon^{2}+\epsilon^{3}}{2\delta}+\frac{\pi^{2}}{2\delta}\\
 & +\sum_{n=1}^{\infty}2\ln\left(1+w^{n}\right)+\sum_{n=1}^{\infty}\frac{w^{n}}{\left(1+w^{n}\right)^{2}}\left(\epsilon^{2}+\epsilon^{3}\right)+\mathcal{O}\left(\epsilon^{3}\right)
\end{align*}

\end_inset


\begin_inset Formula 
\[
\Re\ln\psi\left(x_{1},w\right)-\Re\ln\psi\left(x_{2},w\right)=\left(\epsilon_{1}^{2}-\epsilon_{2}^{2}\right)\left(8-\frac{1}{2\delta}+X\right)+\left(\epsilon_{1}^{3}-\epsilon_{2}^{3}\right)\left(X-\frac{1}{2\delta}\right),
\]

\end_inset

 where 
\begin_inset Formula 
\[
\epsilon_{1}=\frac{x_{1}}{z}+1,\quad\epsilon_{2}=\frac{w}{z}+1
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\Re\rho\left(\epsilon_{1},\epsilon_{2};\delta\right) & =\alpha_{1}\left(\epsilon_{1}^{2}-\epsilon_{2}^{2}\right)\left(8-\frac{1}{2\delta}+X\right)+\alpha_{1}\left(\epsilon_{1}^{3}-\epsilon_{2}^{3}\right)\left(X-\frac{1}{2\delta}\right)\\
 & \equiv\alpha_{1}\left(\epsilon_{1}^{2}-\epsilon_{2}^{2}\right)A+\alpha_{1}\left(\epsilon_{1}^{3}-\epsilon_{2}^{3}\right)B
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\Re\frac{d\rho}{dz} & =\Re\frac{\partial\rho}{\partial\epsilon_{1}}\frac{d\epsilon_{1}}{dz}+\Re\frac{\partial\rho}{\partial\epsilon_{2}}\frac{d\epsilon_{2}}{dz}\\
 & =\alpha_{1}\left(2\epsilon_{1}A+3\epsilon_{1}^{2}B\right)\left(-\frac{x_{1}}{z^{2}}\right)-\alpha_{1}\left(2\epsilon_{2}A+3\epsilon_{2}^{2}B\right)\left(-\frac{w}{z^{2}}\right)\\
 & =2\frac{\alpha_{1}}{z^{2}}A\left(-\epsilon_{1}x_{1}+\epsilon_{2}w\right)+3\frac{\alpha_{1}}{z^{2}}B\left(-\epsilon_{1}^{2}x_{1}+\epsilon_{2}^{2}w\right)
\end{align*}

\end_inset

 The length of the cut is determined by
\begin_inset Formula 
\begin{multline*}
\frac{d}{dx}\ln\psi\left(x,w\right)=\frac{\sqrt{x}}{1-x}+\left(-\frac{1}{2}x^{-3/2}-\frac{1}{2}x^{-1/2}\right)+\frac{\ln x}{x\ln w}+\sum_{n=1}^{\infty}\left[-\frac{w^{n}}{1-w^{n}x}+\frac{w^{n}}{x^{2}\left(1-w^{n}/x\right)}\right]\\
=\frac{\sqrt{x}}{1-x}-\frac{1+x}{2x\sqrt{x}}+\frac{\ln x}{x\ln w}+\sum_{n=1}^{\infty}w^{n}\left[\frac{1}{x\left(x-w^{n}\right)}-\frac{1}{1-w^{n}x}\right]
\end{multline*}

\end_inset

 
\begin_inset Formula 
\[
\frac{d\rho}{dz}=-\alpha_{1}\frac{1}{z^{2}}\left(\right)
\]

\end_inset


\end_layout

