\begin_body

\begin_layout Title
Stringbit vertex calculation
\end_layout

\begin_layout Standard
The energy eigenstate of large 
\begin_inset Formula $N$
\end_inset

 limit can be written as 
\begin_inset Formula 
\[
\ket{E_{r}}=\int d^{M}\theta\psi_{r}\left(\theta_{1}\cdots\theta_{M}\right)\ket{\theta_{1}\cdots\theta_{M}}.
\]

\end_inset

where 
\begin_inset Formula $\Phi$
\end_inset

 is eigenfunction of 
\begin_inset Formula $h$
\end_inset


\begin_inset Formula 
\[
h\psi_{r}\left(\theta_{1},\cdots,\theta_{M}\right)=E_{r}\psi_{r}\left(\theta_{1},\cdots,\theta_{M}\right).
\]

\end_inset

 The ground energy state can be expressed as 
\begin_inset Formula 
\[
\psi_{0}^{\left(b\right)}=\prod_{k=1}^{\left\lfloor M/2\right\rfloor }\left(1+r_{k}^{-}\alpha_{k}\alpha_{M-k}\right)
\]

\end_inset

 for bosonic eigenfunction and 
\begin_inset Formula 
\[
\psi_{0}^{\left(f\right)}=\alpha_{0}\prod_{k=1}^{\left\lfloor M/2\right\rfloor }\left(1+r_{k}^{-}\alpha_{k}\alpha_{M-k}\right)
\]

\end_inset

 for fermionic eigenfunction.
 The variables in above formulas are defined as
\begin_inset Formula 
\[
r_{k}^{\pm}=\tan\frac{k\pi}{M}\pm\sec\frac{k\pi}{M},
\]

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\alpha_{n} & = & \frac{1}{\sqrt{M}}\sum_{k=1}^{M}\theta_{k}e^{-2\pi ikn/M},\quad\beta_{n}=\frac{1}{\sqrt{M}}\sum_{k=1}^{M}\frac{d}{d\theta_{k}}e^{-2\pi ikn/M},\\
\theta_{k} & = & \frac{1}{\sqrt{M}}\sum_{n=0}^{M-1}\alpha_{n}e^{2\pi ikn/M},\quad\frac{d}{d\theta_{k}}=\frac{1}{\sqrt{M}}\sum_{n=0}^{M-1}\beta_{n}e^{2\pi ikn/M},\\
n & = & 0,\dots M-1,\quad k=1,\dots M.
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left\{ \alpha_{n},\beta_{m}\right\} =\delta_{m+n,M}+\delta_{m,0}\delta_{n,0}.
\]

\end_inset

The Hermitianlity of Hamiltonian requires that 
\begin_inset Formula 
\[
\theta_{k}^{\dagger}=\frac{d}{d\theta_{k}},\quad\frac{d}{d\theta_{k}}^{\dagger}=\theta_{k},
\]

\end_inset

 from which it follows that 
\begin_inset Formula 
\[
\alpha_{n}^{\dagger}=\beta_{M-n},\quad\beta_{n}^{\dagger}=\alpha_{M-n},\quad\left\{ \alpha_{n},\alpha_{m}^{\dagger}\right\} =\left\{ \beta_{n},\beta_{m}^{\dagger}\right\} =\delta_{m,n},\quad0\leq n,m\leq M-1
\]

\end_inset


\end_layout

\begin_layout Section*
Lowering and raising operators
\end_layout

\begin_layout Standard
We normalize the coefficient 
\begin_inset Formula $r_{k}^{\pm}$
\end_inset

 as
\begin_inset Formula 
\begin{eqnarray*}
\sqrt{1+r_{k}^{\pm2}} & = & \sqrt{1+\tan^{2}\frac{k\pi}{M}+\sec^{2}\frac{k\pi}{M}\pm2\tan\frac{k\pi}{M}\sec\frac{k\pi}{M}}\\
 & = & \sqrt{2\sec^{2}\frac{k\pi}{M}\pm2\tan\frac{k\pi}{M}\sec\frac{k\pi}{M}}\\
 & = & \sqrt{\frac{2\pm2\sin\frac{k\pi}{M}}{\cos^{2}\frac{k\pi}{M}}}\\
 & = & \sqrt{\frac{2}{1\mp\sin\frac{k\pi}{M}}}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\frac{r_{k}^{\pm}}{\sqrt{1+r_{k}^{\pm2}}} & = & \left(\tan\frac{k\pi}{M}\pm\sec\frac{k\pi}{M}\right)\sqrt{\frac{1\mp\sin\frac{k\pi}{M}}{2}}\\
 & = & \frac{\pm\left(1\pm\sin\frac{k\pi}{M}\right)}{\cos\frac{k\pi}{M}}\sqrt{\frac{1\mp\sin\frac{k\pi}{M}}{2}}\\
 & = & \pm\frac{\pm\left(1\pm\sin\frac{k\pi}{M}\right)}{\cos\frac{k\pi}{M}}\sqrt{\frac{\cos^{2}\frac{k\pi}{M}}{2\left(1\pm\sin\frac{k\pi}{M}\right)}}\\
 & = & \pm\mathrm{sign}\left(\cos\frac{k\pi}{M}\right)\sqrt{\frac{1\pm\sin\frac{k\pi}{M}}{2}}\\
 & = & \pm\frac{\cos\frac{k\pi}{M}}{\sqrt{2\left(1\mp\sin\frac{k\pi}{M}\right)}}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\[
\bar{F}_{k}=\frac{r_{k}^{+}}{\sqrt{1+r_{k}^{+2}}}\alpha_{k}+\frac{1}{\sqrt{1+r_{k}^{+2}}}\beta_{k}=\begin{cases}
\sqrt{\frac{1+\sin\frac{k\pi}{M}}{2}}\alpha_{k}+\sqrt{\frac{1-\sin\frac{k\pi}{M}}{2}}\beta_{k} & k\leq M/2\\
-\sqrt{\frac{1+\sin\frac{k\pi}{M}}{2}}\alpha_{k}+\sqrt{\frac{1-\sin\frac{k\pi}{M}}{2}}\beta_{k} & k>M/2
\end{cases}
\]

\end_inset


\begin_inset Formula 
\[
F_{k}=\frac{r_{k}^{-}}{\sqrt{1+r_{k}^{-2}}}\alpha_{k}+\frac{1}{\sqrt{1+r_{k}^{-}}}\beta_{k}=\begin{cases}
-\sqrt{\frac{1-\sin\frac{k\pi}{M}}{2}}\alpha_{k}+\sqrt{\frac{1+\sin\frac{k\pi}{M}}{2}}\beta_{k} & k\leq M/2\\
\sqrt{\frac{1-\sin\frac{k\pi}{M}}{2}}\alpha_{k}+\sqrt{\frac{1+\sin\frac{k\pi}{M}}{2}}\beta_{k} & k>M/2
\end{cases}
\]

\end_inset

Define 
\begin_inset Formula 
\[
c_{k}\equiv\frac{1}{\sqrt{1+r_{k}^{-2}}}=\frac{\left|r_{k}^{+}\right|}{\sqrt{1+r_{k}^{+2}}}=\sqrt{\frac{1+\sin\frac{k\pi}{M}}{2}}=\cos\left(\frac{\pi}{4}-\frac{k\pi}{2M}\right)
\]

\end_inset


\begin_inset Formula 
\[
s_{k}\equiv-\frac{r_{k}^{-}}{\sqrt{1+r_{k}^{-2}}}=\frac{\cos\frac{k\pi}{M}}{\sqrt{2\left(1+\sin\frac{k\pi}{M}\right)}}=\sin\left(\frac{\pi}{4}-\frac{k\pi}{2M}\right)=\begin{cases}
\sqrt{\frac{1-\sin\frac{k\pi}{M}}{2}} & \text{for }k\leq M/2\\
-\sqrt{\frac{1-\sin\frac{k\pi}{M}}{2}} & \text{for }k>M/2
\end{cases}
\]

\end_inset


\begin_inset Formula 
\[
c_{k}^{2}+s_{k}^{2}=1,\quad c_{k}^{2}-s_{k}^{2}=\sin\frac{k\pi}{M},\quad c_{k}s_{k}=\frac{1}{2}\cos\frac{k\pi}{M}.
\]

\end_inset

Also 
\begin_inset Formula 
\[
c_{k}c_{l}+s_{k}s_{l}=\cos\frac{k-l}{2M}\pi,\quad c_{k}s_{l}+c_{l}s_{k}=\cos\frac{k+l}{2M}\pi,\quad c_{k}s_{l}-c_{l}s_{k}=\sin\frac{k-l}{2M}\pi.
\]

\end_inset

 Then we define
\begin_inset Formula 
\begin{equation}
F_{k}=s_{k}\alpha_{k}+c_{k}\beta_{k},\quad\bar{F}_{k}=c_{k}\alpha_{k}-s_{k}\beta_{k},\quad k=1,\cdots,M-1\label{eq:F-Fbar-definition}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\alpha_{k}=c_{k}\bar{F}_{k}+s_{k}F_{k},\quad\beta_{k}=-s_{k}\bar{F}_{k}+c_{k}F_{k},\quad k=1,\cdots,M-1,\label{eq:alphak-betak}
\end{equation}

\end_inset

 from which it follows that 
\begin_inset Formula 
\[
F_{k}^{\dagger}=\bar{F}_{M-n}=c_{k}\alpha_{M-n}+s_{k}\beta_{M-n},\quad k=1,\cdots,M-1
\]

\end_inset

 For zero modes, we define
\begin_inset Formula 
\[
F_{0}=F_{M}=\exp\left(\frac{i\pi}{4}\right)\beta_{0},\quad\bar{F}_{0}=\bar{F}_{M}=\exp\left(-\frac{i\pi}{4}\right)\alpha_{0},
\]

\end_inset

 For 
\begin_inset Formula $1\leq i,j\leq M-1$
\end_inset

, it is easy to see that 
\begin_inset Formula 
\begin{eqnarray*}
\left\{ \bar{F}_{i},\bar{F}_{j}\right\}  & = & \left\{ c_{i}\alpha_{i}-s_{i}\beta_{i},c_{j}\alpha_{j}-s_{j}\beta_{j}\right\} \\
 & = & -c_{i}s_{j}\left\{ \alpha_{i},\beta_{j}\right\} -s_{i}c_{j}\left\{ \beta_{i},\alpha_{j}\right\} \\
 & = & \left(c_{i}s_{i}-s_{i}c_{i}\right)\delta_{i+j,M}\\
 & = & 0
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\[
\left\{ \bar{F}_{0},\bar{F}_{i}\right\} =\left\{ \bar{F}_{0},\bar{F}_{0}\right\} =0
\]

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\left\{ F_{i},F_{j}\right\}  & = & \left\{ s_{i}\alpha_{i}+c_{i}\beta_{i},s_{j}\alpha_{j}+c_{j}\beta_{j}\right\} \\
 & = & s_{i}c_{j}\left\{ \alpha_{i},\beta_{j}\right\} +c_{i}s_{j}\left\{ \beta_{i},\alpha_{j}\right\} \\
 & = & \left(s_{i}c_{i}-c_{i}s_{i}\right)\delta_{i+j,M}\\
 & = & 0
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\[
\left\{ F_{0},F_{i}\right\} =\left\{ F_{0},F_{0}\right\} =0
\]

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\left\{ F_{i},\bar{F}_{j}\right\}  & = & \left\{ s_{i}\alpha_{i}+c_{i}\beta_{i},c_{j}\alpha_{j}-s_{j}\beta_{j}\right\} \\
 & = & -s_{i}s_{j}\left\{ \alpha_{i},\beta_{j}\right\} +c_{i}c_{j}\left\{ \beta_{i},\alpha_{j}\right\} \\
 & = & \left(s_{i}^{2}+c_{i}^{2}\right)\delta_{i+j,M}+\left(c_{0}^{2}-s_{0}^{2}\right)\delta_{i,0}\delta_{j,0}\\
 & = & \delta_{i+j,M}.
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\[
\left\{ F_{0},\bar{F}_{i}\right\} =\left\{ F_{i},\bar{F}_{0}\right\} =0,\quad\left\{ F_{0},\bar{F}_{M}\right\} =\left\{ F_{M},\bar{F}_{0}\right\} =\left\{ F_{0},\bar{F}_{0}\right\} =1.
\]

\end_inset

 In summary, we have 
\begin_inset Formula 
\[
\left\{ F_{k}^{\dagger},F_{l}^{\dagger}\right\} =\left\{ F_{k},F_{l}\right\} =0,\quad\left\{ F_{k},F_{l}^{\dagger}\right\} =\delta_{kl},\quad0\leq k,l\leq M-1
\]

\end_inset


\begin_inset Formula 
\[
\]

\end_inset


\end_layout

\begin_layout Standard
For grassmanian even function 
\begin_inset Formula $\psi\left(\theta\right)$
\end_inset

 of 
\begin_inset Formula $\theta$
\end_inset

, we define 
\begin_inset Formula $F_{k}^{+}$
\end_inset

 as 
\begin_inset Formula 
\[
\int d^{M}\theta\,\left[F_{k}^{+}\psi\left(\theta\right)\right]\chi\left(\theta\right)=\int d^{M}\theta\,\psi\left(\theta\right)\left[F_{k}\chi\left(\theta\right)\right].
\]

\end_inset

Likewise, for grassmanian odd function 
\begin_inset Formula $\psi\left(\theta\right)$
\end_inset

, we define 
\begin_inset Formula $F^{-}$
\end_inset

 as 
\begin_inset Formula 
\[
\int d^{M}\theta\,\left[F_{k}^{-}\psi\left(\theta\right)\right]\chi\left(\theta\right)=\int d^{M}\theta\,\psi\left(\theta\right)\left[F_{k}\chi\left(\theta\right)\right].
\]

\end_inset

We then have
\begin_inset Formula 
\[
F_{k}^{\pm}=\pm\left(s_{k}\alpha_{k}-c_{k}\beta_{k}\right),\quad\bar{F}_{k}^{\pm}=\pm\left(c_{k}\alpha_{k}+s_{k}\beta_{k}\right),\quad1\leq k\leq M-1
\]

\end_inset


\begin_inset Formula 
\[
F_{0}^{\pm}=\mp\beta_{0},\quad\bar{F}_{0}^{\pm}=\pm\alpha_{0}
\]

\end_inset

 For 
\begin_inset Formula $1\leq i,j\leq M-1$
\end_inset

, we have
\begin_inset Formula 
\begin{eqnarray*}
\left\{ F_{i}^{\pm},F_{j}^{\pm}\right\}  & = & \left\{ s_{i}\alpha_{i}-c_{i}\beta_{i},s_{j}\alpha_{j}-c_{j}\beta_{j}\right\} \\
 & = & -s_{i}c_{j}\left\{ \alpha_{i},\beta_{j}\right\} -c_{i}s_{j}\left\{ \beta_{i},\alpha_{j}\right\} \\
 & = & \left(-s_{i}c_{i}+c_{i}s_{i}\right)\delta_{i+j,M}\\
 & = & 0
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\[
\left\{ \tilde{F}_{0},\tilde{F}_{i}\right\} =\left\{ \tilde{F}_{0},\tilde{F}_{0}\right\} =0
\]

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\left\{ \bar{F}_{i}^{\pm},\bar{F}_{j}^{\pm}\right\}  & = & \left\{ c_{i}\alpha_{i}+s_{i}\beta_{i},c_{j}\alpha_{j}+s_{j}\beta_{j}\right\} \\
 & = & c_{i}s_{j}\left\{ \alpha_{i},\beta_{j}\right\} +s_{i}c_{j}\left\{ \beta_{i},\alpha_{j}\right\} \\
 & = & \left(-c_{i}s_{i}+s_{i}c_{i}\right)\delta_{i+j,M}+2c_{0}s_{0}\delta_{i,0}\delta_{j,0}\\
 & = & \delta_{i,0}\delta_{j,0}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\[
\left\{ \bar{F}_{0}^{\pm},\bar{F}_{i}^{\pm}\right\} =\left\{ \bar{F}_{0}^{\pm},\bar{F}_{0}^{\pm}\right\} =0
\]

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\left\{ F_{i}^{\pm},\bar{F}_{j}^{\pm}\right\}  & = & \left\{ s_{i}\alpha_{i}-c_{i}\beta_{i},c_{j}\alpha_{j}+s_{j}\beta_{j}\right\} \\
 & = & s_{i}s_{j}\left\{ \alpha_{i},\beta_{j}\right\} -c_{i}c_{j}\left\{ \beta_{i},\alpha_{j}\right\} \\
 & = & \left(-s_{i}^{2}-c_{i}^{2}\right)\delta_{i+j,M}+\left(s_{0}^{2}-c_{0}^{2}\right)\delta_{i,0}\delta_{j,0}\\
 & = & -\delta_{i+j,M},
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\[
\left\{ F_{0}^{\pm},\bar{F}_{i}^{\pm}\right\} =\left\{ F_{i}^{\pm},\bar{F}_{0}^{\pm}\right\} =0,\quad\left\{ F_{0}^{\pm},\bar{F}_{0}^{\pm}\right\} =-1.
\]

\end_inset

 In summary, we have
\begin_inset Formula 
\[
\left\{ F_{i}^{\pm},F_{j}^{\pm}\right\} =\left\{ F_{i}^{\dagger\pm},F_{j}^{\dagger\pm}\right\} =0,\quad\left\{ F_{i}^{\pm},F_{j}^{\dagger\pm}\right\} =-\delta_{ij}
\]

\end_inset


\end_layout

\begin_layout Section*
Eigenfunctions
\end_layout

\begin_layout Standard
Let us first calculate the action of ladder operator on eigenfunctions.
 
\begin_inset Formula 
\begin{eqnarray*}
\bar{F}_{k}\phi_{k} & = & \left(c_{k}\alpha_{k}-s_{k}\beta_{k}\right)\left(c_{k}-s_{k}\alpha_{M-k}\alpha_{k}\right)\\
 & = & c_{k}^{2}\alpha_{k}+s_{k}^{2}\beta_{k}\alpha_{M-k}\alpha_{k}\\
 & = & c_{k}^{2}\alpha_{k}+s_{k}^{2}\alpha_{k}\\
 & = & \alpha_{k}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\bar{F}_{M-k}\phi_{k} & = & \left(c_{k}\alpha_{M-k}+s_{k}\beta_{M-k}\right)\left(c_{k}-s_{k}\alpha_{M-k}\alpha_{k}\right)\\
 & = & c_{k}^{2}\alpha_{M-k}-s_{k}^{2}\beta_{M-k}\alpha_{M-k}\alpha_{k}\\
 & = & c_{k}^{2}\alpha_{M-k}+s_{k}^{2}\alpha_{M-k}\\
 & = & \alpha_{M-k}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
F_{k}\phi_{k} & = & \left(s_{k}\alpha_{k}+c_{k}\beta_{k}\right)\left(c_{k}-s_{k}\alpha_{M-k}\alpha_{k}\right)\\
 & = & s_{k}c_{k}\alpha_{k}-s_{k}c_{k}\beta_{k}\alpha_{M-k}\alpha_{k}\\
 & = & 0
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
F_{M-k}\phi_{k} & = & \left(-s_{k}\alpha_{M-k}+c_{k}\beta_{M-k}\right)\left(c_{k}-s_{k}\alpha_{M-k}\alpha_{k}\right)\\
 & = & -s_{k}c_{k}\alpha_{M-k}-s_{k}c_{k}\beta_{M-k}\alpha_{M-k}\alpha_{k}\\
 & = & -s_{k}c_{k}\alpha_{M-k}+s_{k}c_{k}\alpha_{M-k}\\
 & = & 0
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\bar{F}_{M-k}\bar{F}_{k}\phi_{k} & = & L_{M-k}^{+}\alpha_{k}\\
 & = & \left(c_{k}\alpha_{M-k}+s_{k}\beta_{M-k}\right)\alpha_{k}\\
 & = & s_{k}+c_{k}\alpha_{M-k}\alpha_{k}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
F_{M-k}^{\pm}\bar{\phi}_{k} & = & \pm\left(-s_{k}\alpha_{M-k}-c_{k}\beta_{M-k}\right)\left(-s_{k}+c_{k}\alpha_{M-k}\alpha_{k}\right)\\
 & = & \pm\left(s_{k}^{2}\alpha_{M-k}-c_{k}^{2}\beta_{M-k}\alpha_{M-k}\alpha_{k}\right)\\
 & = & \pm\left(s_{k}^{2}+c_{k}^{2}\right)\alpha_{M-k}\\
 & = & \pm\alpha_{M-k}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
F_{k}^{\pm}\bar{\phi}_{k} & = & \pm\left(s_{k}\alpha_{k}-c_{k}\beta_{k}\right)\left(-s_{k}+c_{k}\alpha_{M-k}\alpha_{k}\right)\\
 & = & \pm\left(-s_{k}^{2}\alpha_{k}-c_{k}^{2}\beta_{k}\alpha_{M-k}\alpha_{k}\right)\\
 & = & \mp\left(s_{k}^{2}+c_{k}^{2}\right)\alpha_{k}\\
 & = & \mp\alpha_{k}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
F_{M-k}^{\pm}F_{k}^{\pm}\bar{\phi}_{k} & = & -\left(-s_{k}\alpha_{M-k}-c_{k}\beta_{M-k}\right)\alpha_{k}\\
 & = & s_{k}\alpha_{M-k}\alpha_{k}+c_{k}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\bar{F}_{k}^{\pm}\bar{\phi}_{k} & = & \pm\left(c_{k}\alpha_{k}+s_{k}\beta_{k}\right)\left(-s_{k}+c_{k}\alpha_{M-k}\alpha_{k}\right)\\
 & = & \pm\left(-c_{k}s_{k}\alpha_{k}+c_{k}s_{k}\beta_{k}\alpha_{M-k}\alpha_{k}\right)\\
 & = & 0
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\bar{F}_{M-k}^{\pm}\bar{\phi}_{k} & = & \pm\left(c_{k}\alpha_{M-k}-s_{k}\beta_{M-k}\right)\left(-s_{k}+c_{k}\alpha_{M-k}\alpha_{k}\right)\\
 & = & \pm\left(-c_{k}s_{k}\alpha_{M-k}-c_{k}s_{k}\beta_{M-k}\alpha_{M-k}\alpha_{k}\right)\\
 & = & 0
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
We first consider the odd 
\begin_inset Formula $M$
\end_inset

 case.
 The ground eigenfunction is
\begin_inset Formula 
\begin{equation}
\psi_{0}=\prod_{k=1}^{\left\lfloor M/2\right\rfloor }\left(c_{k}-s_{k}\alpha_{M-k}\alpha_{k}\right)\equiv\prod_{k=1}^{\left\lfloor M/2\right\rfloor }\phi_{k}.\label{eq:ground_state_oddM}
\end{equation}

\end_inset

 It is easy to see that
\begin_inset Formula 
\[
F_{k}\psi_{0}=0,\quad k=0,1,\cdots,M-1
\]

\end_inset

 We define 
\begin_inset Formula 
\[
\tilde{\psi}_{0}=\left(-i\right)^{\left\lfloor M/2\right\rfloor }\alpha_{0}\prod_{i=1}^{\left\lfloor M/2\right\rfloor }\left(-s_{i}+c_{i}\alpha_{M-i}\alpha_{i}\right)\equiv\left(-i\right)^{\left\lfloor M/2\right\rfloor }\alpha_{0}\prod_{k=1}^{\left\lfloor M/2\right\rfloor }\bar{\phi}_{k}.
\]

\end_inset

One can verify that
\begin_inset Formula 
\begin{eqnarray*}
\int d^{M}\theta\,\tilde{\psi}_{0}\psi_{0} & = & \left(-i\right)^{\left\lfloor M/2\right\rfloor }\int d^{M}\theta\,\alpha_{0}\prod_{i=1}^{\left\lfloor M/2\right\rfloor }\left(-s_{i}+c_{i}\alpha_{M-i}\alpha_{i}\right)\left(c_{i}-s_{i}\alpha_{M-i}\alpha_{i}\right)\\
 & = & \left(-i\right)^{\left\lfloor M/2\right\rfloor }\int d^{M}\theta\,\alpha_{0}\prod_{i=1}^{\left\lfloor M/2\right\rfloor }\left(s_{i}^{2}+c_{i}^{2}\right)\alpha_{M-i}\alpha_{i}\\
 & = & \left(-i\right)^{\left\lfloor M/2\right\rfloor }\int d^{M}\theta\,\prod_{i=1}^{M}\alpha_{M-i}\\
 & = & 1,
\end{eqnarray*}

\end_inset

 where we used
\begin_inset Formula 
\[
\int d^{M}\theta\,\prod_{i=1}^{M}\alpha_{M-i}=i^{\left\lfloor \left(M-1\right)/2\right\rfloor }=\begin{cases}
i^{\left(M-1\right)/2} & \text{for odd }M\\
\left(-i\right)^{M/2-1} & \text{for even }M
\end{cases}.
\]

\end_inset

 Also,
\begin_inset Formula 
\begin{eqnarray*}
\int d^{M}\theta\, F_{0}^{-}\tilde{\psi}_{0}F_{0}^{\dagger}\psi_{0} & = & \int d^{M}\theta\,\left(F_{0}^{-}\tilde{\psi}_{0}^{\left(1\right)}\right)\left(F_{0}^{\dagger}\psi_{0}\right)\\
 & = & \int d^{M}\theta\,\left(\beta_{0}\tilde{\psi}_{0}^{\left(1\right)}\right)\left(\alpha_{0}\psi_{0}\right)\\
