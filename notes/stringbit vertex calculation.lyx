 & = & 2\exp\left(2\pi ik\frac{m-n}{M}\right)\sin\frac{\left(n+m\right)\pi}{2M}+2\exp\left(2\pi i\frac{k+l}{2}\frac{m-n}{M}\right)\sin\frac{\left(l-k-1/2\right)\left(m+n\right)}{M}\pi
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\[
\gamma=-\cot\frac{\pi}{2M}+\cot\frac{2\left(k-l\right)+1}{2M}\pi
\]

\end_inset

 We see that 
\begin_inset Formula 
\[
A^{T}=-A,\quad B_{nm}=2A_{-n,m},\quad B^{\dagger}=B,
\]

\end_inset

 so which can also write 
\begin_inset Formula 
\[
h_{kl}^{\left(1\right)}=\frac{2}{M}\sum_{n,m=1}^{M-1}\left(A_{nm}^{\dagger}F_{n}^{\dagger}F_{m}^{\dagger}+A_{nm}F_{n}F_{m}+2A_{-n,m}F_{n}^{\dagger}F_{m}\right)+\frac{2}{M}\gamma
\]

\end_inset

 which is expected.
 Using 
\begin_inset Formula 
\[
\alpha_{n}\beta_{0}=\exp\left(-\frac{i\pi}{4}\right)c_{n}F_{M-n}^{\dagger}F_{0}+s_{n}F_{n}F_{0}
\]

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\alpha_{0}\beta_{n} & = & \exp\left(\frac{i\pi}{4}\right)\left(-s_{n}F_{0}^{\dagger}F_{M-n}^{\dagger}+c_{n}F_{0}^{\dagger}F_{n}\right)\\
 & = & \exp\left(\frac{i\pi}{4}\right)\left(s_{n}F_{M-n}^{\dagger}F_{0}^{\dagger}+c_{n}F_{0}^{\dagger}F_{n}\right)
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\alpha_{n}\alpha_{0} & = & \exp\left(\frac{i\pi}{4}\right)\left(c_{n}F_{M-n}^{\dagger}F_{0}^{\dagger}+s_{n}F_{n}F_{0}^{\dagger}\right)\\
 & = & \exp\left(\frac{i\pi}{4}\right)\left(c_{n}F_{M-n}^{\dagger}F_{0}^{\dagger}-s_{n}F_{0}^{\dagger}F_{n}\right)
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\[
\beta_{n}\beta_{0}=\exp\left(-\frac{i\pi}{4}\right)\left(-s_{n}F_{M-n}^{\dagger}F_{0}+c_{n}F_{n}F_{0}\right)
\]

\end_inset

 we obtain
\begin_inset Formula 
\begin{eqnarray*}
h_{kl}^{\left(0\right)} & = & \frac{2}{M}\sum_{n=1}^{M-1}\left(\alpha_{n}\beta_{0}+\alpha_{0}\beta_{n}-i\alpha_{n}\alpha_{0}-i\beta_{n}\beta_{0}\right)\left[\exp\left(2\pi i\frac{kn}{M}\right)-\exp\left(2\pi i\frac{ln}{M}\right)\right]\\
 & = & \frac{2}{M}\sum_{n=1}^{M-1}\left[\exp\left(2\pi i\frac{kn}{M}\right)-\exp\left(2\pi i\frac{ln}{M}\right)\right]\exp\left(\frac{i\pi}{4}\right)\left(s_{n}-ic_{n}\right)F_{M-n}^{\dagger}F_{0}^{\dagger}\\
 &  & +\frac{2}{M}\sum_{n=1}^{M-1}\left[\exp\left(2\pi i\frac{kn}{M}\right)-\exp\left(2\pi i\frac{ln}{M}\right)\right]\exp\left(-\frac{i\pi}{4}\right)\left(c_{n}+is_{n}\right)F_{M-n}^{\dagger}F_{0}\\
 &  & +\frac{2}{M}\sum_{n=1}^{M-1}\left[\exp\left(2\pi i\frac{kn}{M}\right)-\exp\left(2\pi i\frac{ln}{M}\right)\right]\exp\left(\frac{i\pi}{4}\right)\left(c_{n}+is_{n}\right)F_{0}^{\dagger}F_{n}\\
 &  & +\frac{2}{M}\sum_{n=1}^{M-1}\left[\exp\left(2\pi i\frac{kn}{M}\right)-\exp\left(2\pi i\frac{ln}{M}\right)\right]\exp\left(-\frac{i\pi}{4}\right)\left(s_{n}-ic_{n}\right)F_{n}F_{0}\\
 & = & \frac{2}{M}\sum_{n=1}^{M-1}\left[\exp\left(-2\pi i\frac{kn}{M}\right)-\exp\left(-2\pi i\frac{ln}{M}\right)\right]\exp\left(\frac{i\pi}{4}\right)\left(-s_{n}-ic_{n}\right)F_{n}^{\dagger}F_{0}^{\dagger}\\
 &  & +\frac{2}{M}\sum_{n=1}^{M-1}\left[\exp\left(-2\pi i\frac{kn}{M}\right)-\exp\left(-2\pi i\frac{ln}{M}\right)\right]\exp\left(-\frac{i\pi}{4}\right)\left(c_{n}-is_{n}\right)F_{n}^{\dagger}F_{0}\\
 &  & +\frac{2}{M}\sum_{n=1}^{M-1}\left[\exp\left(2\pi i\frac{kn}{M}\right)-\exp\left(2\pi i\frac{ln}{M}\right)\right]\exp\left(\frac{i\pi}{4}\right)\left(c_{n}+is_{n}\right)F_{0}^{\dagger}F_{n}\\
 &  & +\frac{2}{M}\sum_{n=1}^{M-1}\left[\exp\left(2\pi i\frac{kn}{M}\right)-\exp\left(2\pi i\frac{ln}{M}\right)\right]\exp\left(-\frac{i\pi}{4}\right)\left(s_{n}-ic_{n}\right)F_{n}F_{0}\\
 & = & \frac{2i}{M}\sum_{n=1}^{M-1}\left[\exp\left(-2\pi i\frac{kn}{M}\right)-\exp\left(-2\pi i\frac{ln}{M}\right)\right]\exp\left(i\frac{n\pi}{2M}\right)F_{0}^{\dagger}F_{n}^{\dagger}\\
 &  & -\frac{2i}{M}\sum_{n=1}^{M-1}\left[\exp\left(-2\pi i\frac{kn}{M}\right)-\exp\left(-2\pi i\frac{ln}{M}\right)\right]\exp\left(i\frac{n\pi}{2M}\right)F_{n}^{\dagger}F_{0}\\
 &  & +\frac{2i}{M}\sum_{n=1}^{M-1}\left[\exp\left(2\pi i\frac{kn}{M}\right)-\exp\left(2\pi i\frac{ln}{M}\right)\right]\exp\left(-i\frac{n\pi}{2M}\right)F_{0}^{\dagger}F_{n}\\
 &  & -\frac{2i}{M}\sum_{n=1}^{M-1}\left[\exp\left(2\pi i\frac{kn}{M}\right)-\exp\left(2\pi i\frac{ln}{M}\right)\right]\exp\left(-i\frac{n\pi}{2M}\right)F_{n}F_{0}\\
 & \equiv & \frac{2}{M}\sum_{n=1}^{M-1}X_{n}^{\dagger}\left(F_{0}^{\dagger}F_{n}^{\dagger}-F_{n}^{\dagger}F_{0}\right)+\mathrm{h.c.}
\end{eqnarray*}

\end_inset

 where
\begin_inset Formula 
\[
X_{n}=i\left[\exp\left(2\pi i\frac{ln}{M}\right)-\exp\left(2\pi i\frac{kn}{M}\right)\right]\exp\left(-\frac{in\pi}{2M}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
h_{kl} & = & \frac{2}{M}\sum_{n,m=1}^{M-1}\left(\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}+A_{nm}F_{n}F_{m}+B_{nm}F_{n}^{\dagger}F_{m}\right)+\\
 &  & +\frac{2}{M}\sum_{n=1}^{M-1}\left[X_{n}^{\dagger}\left(F_{0}^{\dagger}F_{n}^{\dagger}-F_{n}^{\dagger}F_{0}\right)+\mathrm{h.c.}\right]+\frac{2}{M}\gamma,
\end{eqnarray*}

\end_inset

 we see that 
\begin_inset Formula 
\begin{eqnarray*}
A_{n0} & = & -\exp\left(2\pi ik\frac{n}{M}\right)\sin\frac{n\pi}{2M}-\exp\left(2\pi i\frac{\left(k+l\right)n}{2M}\right)\sin\frac{\left(l-k-1/2\right)n\pi}{M}\\
 & = & -\exp\left(2\pi ik\frac{n}{M}\right)\sin\frac{n\pi}{2M}-\frac{1}{2i}\exp\left(2\pi i\frac{\left(l-1/2\right)n}{M}\right)+\frac{1}{2i}\exp\left(2\pi i\frac{\left(k+1/2\right)n}{M}\right)\\
 & = & \exp\left(2\pi ik\frac{n}{M}\right)\left(-\sin\frac{n\pi}{2M}+\frac{1}{2i}\exp\left(\frac{in\pi}{2M}\right)\right)-\frac{1}{2i}\exp\left(2\pi i\frac{ln}{M}\right)\exp\left(-\frac{in\pi}{2M}\right)\\
 & = & -\frac{i}{2}\exp\left(2\pi ik\frac{n}{M}\right)\exp\left(-\frac{in\pi}{2M}\right)+\frac{i}{2}\exp\left(2\pi i\frac{ln}{M}\right)\exp\left(-\frac{in\pi}{2M}\right)\\
 & = & \frac{1}{2}X_{n}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\[
B_{n0}=2A_{-n,0}=X_{-n}=-X_{n}^{\dagger}
\]

\end_inset


\begin_inset Formula 
\[
B_{0n}=B_{n0}^{*}=2A_{-n,0}^{*}=X_{-n}^{*}=-X_{n}
\]

\end_inset

 Therefore, we can simply write 
\begin_inset Formula 
\[
h_{kl}=\frac{2}{M}\sum_{n,m=0}^{M-1}\left(A_{nm}^{\dagger}F_{n}^{\dagger}F_{m}^{\dagger}+A_{nm}F_{n}F_{m}+2A_{-n,m}F_{n}^{\dagger}F_{m}\right)+\frac{2}{M}\gamma
\]

\end_inset


\end_layout

