 & = & \left(E_{s}+T_{t}\right)T_{s}T_{k}\ket{0}\\
 &  & +\frac{1}{N}\left(-\right)^{sK\psi_{s}}\sum_{r}T_{r}\int d\theta d\eta\,\sum_{k=1}^{K}\sum_{l=1}^{L}\bar{h}_{lk}\bar{\psi}_{r}\left(\theta_{l+1}\cdots\theta_{l}\eta_{k}\cdots\eta_{k-1}\right)\psi_{s}\left(\theta\right)\psi_{t}\left(\eta\right)\ket{0}\\
 &  & +\frac{1}{N}\left(-\right)^{sK\psi_{s}}\sum_{r}T_{r}\int d\theta d\eta\,\sum_{k=1}^{K}\sum_{l=1}^{L}\bar{h}_{kl}\bar{\psi}_{r}\left(\theta_{l}\cdots\theta_{l-1}\eta_{k+1}\cdots\eta_{k}\right)\psi_{s}\left(\theta\right)\psi_{t}\left(\eta\right)\ket{0}+\cdots\\
 & \equiv & \left(E_{s}+T_{t}\right)T_{s}T_{t}\ket{0}+\frac{1}{N}\sum_{r}T_{r}\ket{0}\left(W_{rst}^{\left(1\right)}+W_{rst}^{\left(2\right)}\right)
\end{eqnarray*}

\end_inset

 
\begin_inset Formula 
\begin{eqnarray*}
W_{rst}^{\left(1\right)} & = & \left(-\right)^{sK\psi_{s}}\int d\theta d\eta\,\sum_{k=1}^{K}\sum_{l=1}^{L}\bar{h}_{lk}\bar{\psi}_{r}\left(\theta_{l+1}\cdots\theta_{k}\eta_{k}\cdots\eta_{k-1}\right)\psi_{s}\left(\theta_{1}\cdots\theta_{L}\right)\psi_{t}\left(\eta_{1}\cdots\eta_{K}\right)\\
 & = & \left(-\right)^{sK\psi_{s}}\int d\theta_{2}\cdots d\theta_{L}d\theta_{1}d\eta\,\sum_{k=1}^{K}\sum_{l=1}^{L}\bar{h}_{l+1,k}\bar{\psi}_{r}\left(\theta_{l+2}\cdots\theta_{l+1}\eta_{k}\cdots\eta_{k-1}\right)\psi_{s}\left(\theta_{2}\cdots\theta_{L}\theta_{1}\right)\psi_{t}\left(\eta_{1}\cdots\eta_{K}\right)\\
 & = & \left(-\right)^{sK\psi_{s}}\int d\theta_{1}\cdots d\theta_{L}d\eta\,\sum_{k=1}^{K}\sum_{l=1}^{L}\bar{h}_{l+1,k}\bar{\psi}_{r}\left(\theta_{l+2}\cdots\theta_{l+1}\eta_{k}\cdots\eta_{k-1}\right)\psi_{s}\left(\theta_{1}\cdots\theta_{L}\right)\psi_{t}\left(\eta_{1}\cdots\eta_{K}\right)\\
 & = & \left(-\right)^{sK\psi_{s}}KL\int d\theta d\eta\,\bar{h}_{L,1}\bar{\psi}_{r}\left(\theta_{1}\cdots\theta_{L}\eta_{1}\cdots\eta_{K}\right)\psi_{s}\left(\theta_{1}\cdots\theta_{L}\right)\psi_{t}\left(\eta_{1}\cdots\eta_{L}\right)\\
 & = & \left(-\right)^{sK\psi_{s}}KL\int d^{M}\theta\,\bar{h}_{L,L+1}\bar{\psi}_{r}\left(\theta_{1}\cdots\theta_{L}\theta_{L+1}\cdots\theta_{M}\right)\psi_{s}\left(\theta_{1}\cdots\theta_{L}\right)\psi_{t}\left(\theta_{L+1}\cdots\theta_{M}\right)\\
 & = & \left(-\right)^{sK\psi_{s}}KL\int d^{M}\theta\,\bar{\psi}_{r}\left(\theta_{1}\cdots\theta_{L}\theta_{L+1}\cdots\theta_{M}\right)h_{L,L+1}\psi_{s}\left(\theta_{1}\cdots\theta_{L}\right)\psi_{t}\left(\theta_{L+1}\cdots\theta_{M}\right)
\end{eqnarray*}

\end_inset

Likewise
\begin_inset Formula 
\begin{eqnarray*}
W_{rst}^{\left(2\right)} & = & \left(-\right)^{sK\psi_{s}}KL\int d\theta d\eta\,\bar{h}_{K,1}\bar{\psi}_{r}\left(\theta_{1}\cdots\theta_{L}\eta_{1}\cdots\eta_{K}\right)\psi_{s}\left(\theta_{1}\cdots\theta_{L}\right)\psi_{t}\left(\eta_{1}\cdots\eta_{K}\right)\\
