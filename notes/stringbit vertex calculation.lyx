Perturbation theory
\end_layout

\begin_layout Standard
In the language of 
\begin_inset Formula $\theta$
\end_inset

, the zero order energy eigenvalue 
\begin_inset Formula $E_{r}$
\end_inset

 and the corresponding eigenstate 
\begin_inset Formula $\psi_{r}\left(\theta_{1},\dots,\theta_{M}\right)$
\end_inset

are determined by 
\begin_inset Formula 
\[
h\psi_{r}\left(\theta_{1},\dots,\theta_{M}\right)=E_{r}\psi_{r}\left(\theta_{1},\dots,\theta_{M}\right).
\]

\end_inset

 Then single trace energy eigenstate reads 
\begin_inset Formula 
\[
T_{r}\ket{0}=\int d^{s}\theta_{1}\cdots d^{s}\theta_{M}\, T\left(\theta_{1},\cdots,\theta_{M}\right)\psi_{r}\left(\theta_{1},\dots,\theta_{M}\right)\ket{0}.
\]

\end_inset

Because of the cyclic symmetry of 
\begin_inset Formula $T\left(\theta_{1},\cdots,\theta_{M}\right)$
\end_inset

, one can impose to 
\begin_inset Formula $\psi_{r}$
\end_inset

 the constraint 
\begin_inset Formula 
\[
\psi_{r}\left(\theta_{1},\cdots,\theta_{M}\right)=\left(-\right)^{s\left(M-1\right)}\psi_{r}\left(\theta_{2},\cdots,\theta_{M},\theta_{1}\right)
\]

\end_inset

 without loss of generality.
 The sign factor follows from that the measure produce a factor of 
\begin_inset Formula $\left(-\right)^{s\left(M-1\right)}$
\end_inset

 the cyclic transformation is applied on it.
 
\end_layout

\begin_layout Standard
For the following calculation, we shall assume the existance of the conjugate
 function of 
\begin_inset Formula $\psi_{r}$
\end_inset

, which is denoted as 
\begin_inset Formula $\bar{\psi}_{r}$
\end_inset

 and satisfies the completeness relation 
\begin_inset Formula 
\[
\sum_{r}\psi_{r}\left(\theta_{1},\cdots,\theta_{M}\right)\bar{\psi}_{r}\left(\phi_{1},\cdots,\phi_{M}\right)=\delta^{\left(c\right)}\left(\theta-\phi\right),
\]

\end_inset

and normalized as
\begin_inset Formula 
\[
\int d^{s}\theta_{1}\cdots d^{s}\theta_{M}\,\bar{\psi}_{r}\left(\theta_{1},\cdots,\theta_{M}\right)\psi_{s}\left(\theta_{1},\cdots,\theta_{M}\right)=\delta_{rs}
\]

\end_inset

 where the 
\begin_inset Formula $\delta^{\left(c\right)}\left(\theta-\phi\right)$
\end_inset

 is the Dirac Delta function on the cyclic symmetry space.
 Particularly, we have 
\begin_inset Formula 
\[
\int d^{s}\theta_{1}\cdots d^{s}\theta_{M}\, T\left(\theta_{1},\cdots,\theta_{M}\right)\sum_{r}\left(-\right)^{sM\bar{\psi}_{r}}\psi_{r}\left(\theta\right)\bar{\psi}_{r}\left(\phi\right)=\sum_{r}\left(-\right)^{sMT_{r}}T_{r}\bar{\psi}_{r}\left(\phi\right)=T\left(\theta_{1},\cdots,\theta_{M}\right).
\]

\end_inset

We also assume all the eigenfunction 
\begin_inset Formula $\psi_{r}$
\end_inset

 and eigenstate 
\begin_inset Formula $T_{r}$
\end_inset

 have definite Grassmann oddness.
 For convenience, the Grassmann oddness of 
\begin_inset Formula $T_{r}$
\end_inset

 is denoted as 
\begin_inset Formula $\left(-\right)^{T_{r}}$
\end_inset

, which is 1 when 
\begin_inset Formula $T_{r}$
\end_inset

 is Grassmann even and -1 when it is odd.
 The action of 
\begin_inset Formula $H$
\end_inset

 on single trace energy state is 
\begin_inset Formula 
\begin{eqnarray*}
HT_{r}\ket{0} & = & E_{r}T_{r}\ket{0}+\frac{1}{N}\int d\theta\,\sum_{k=1}^{M}\sum_{l=k+2}^{M+k-1}\bar{h}_{kl}T\left(\theta_{l}\cdots\theta_{k}\right)T\left(\theta_{k+1}\cdots\theta_{l-1}\right)\ket{0}\psi_{r}\left(\theta_{1},\cdots,\theta_{M}\right)\\
 & = & E_{r}T_{r}\ket{0}+\frac{1}{N}\int d\theta\,\sum_{k=1}^{M}\sum_{L=1}^{M-2}\bar{h}_{k,k+L+1}T\left(\theta_{k+L+1}\cdots\theta_{k}\right)T\left(\theta_{k+1}\cdots\theta_{k+L}\right)\ket{0}\psi_{r}\left(\theta_{1},\cdots,\theta_{M}\right)\\
 & = & E_{r}T_{r}\ket{0}+\frac{1}{N}\int d\theta\,\sum_{k=1}^{M}\sum_{L=1}^{M-2}\bar{h}_{k,k+L+1}\sum_{s,t}\left(-\right)^{s\left(\left(M-L\right)T_{t}+LT_{s}\right)}T_{t}\bar{\psi}_{t}\left(\theta_{k+L+1}\cdots\theta_{k}\right)T_{s}\bar{\psi}_{s}\left(\theta_{k+1}\cdots\theta_{k+L}\right)\ket{0}\psi_{r}\left(\theta_{1},\cdots,\theta_{M}\right)\\
 & = & E_{r}T_{r}\ket{0}+\frac{1}{N}\int d\theta\,\sum_{L=1}^{M-2}\sum_{k=1}^{M}\bar{h}_{k,k+L+1}\sum_{s,t}\left(-\right)^{s\left(\left(M-L\right)T_{t}+LT_{s}\right)}T_{s}T_{t}\bar{\psi}_{t}\left(\theta_{k+L+1}\cdots\theta_{k}\right)\bar{\psi}_{s}\left(\theta_{k+1}\cdots\theta_{k+L}\right)\ket{0}\psi_{r}\left(\theta_{1},\cdots,\theta_{M}\right)\\
 & = & E_{r}T_{r}\ket{0}+\frac{1}{N}\sum_{L=1}^{M-2}\sum_{s,t}T_{s}T_{t}\ket{0}\left(-\right)^{s\left(\left(M-L\right)T_{t}+LT_{s}\right)+sM\left(T_{t}+T_{s}\right)}\int d\theta\,\sum_{k=1}^{M}\bar{h}_{k,k+L+1}\bar{\psi}_{t}\left(\theta_{k+L+1}\cdots\theta_{k}\right)\bar{\psi}_{s}\left(\theta_{k+1}\cdots\theta_{k+L}\right)\psi_{r}\left(\theta_{1},\cdots,\theta_{M}\right)\\
 & = & E_{r}T_{r}\ket{0}+\frac{1}{N}\sum_{L=1}^{M-2}\sum_{s,t}T_{s}T_{t}\ket{0}\left(-\right)^{s\left(LT_{t}+\left(M-L\right)T_{s}\right)}\int d\theta\,\sum_{k=1}^{M}\bar{h}_{k,k+L+1}\bar{\psi}_{t}\left(\theta_{k+L+1}\cdots\theta_{k}\right)\bar{\psi}_{s}\left(\theta_{k+1}\cdots\theta_{k+L}\right)\psi_{r}\left(\theta_{1},\cdots,\theta_{M}\right)\\
 & \equiv & E_{r}T_{r}\ket{0}+\frac{1}{N}\sum_{L=1}^{M-2}\sum_{s,t}T_{s}T_{t}\ket{0}V_{tsr}^{\left(K,L\right)}
\end{eqnarray*}

\end_inset

 where 
\begin_inset Formula $K=M-L$
\end_inset

 and
\begin_inset Formula 
\begin{eqnarray*}
V_{tsr}^{\left(K,L\right)} & = & \left(-\right)^{s\left(LT_{t}+KT_{s}\right)}\sum_{k=1}^{M}\int d\theta\,\bar{h}_{k,k+L+1}\bar{\psi}_{t}\left(\theta_{k+L+1}\cdots\theta_{k}\right)\bar{\psi}_{s}\left(\theta_{k+1}\cdots\theta_{k+L}\right)\psi_{r}\left(\theta_{1},\cdots,\theta_{M}\right)\\
 & = & \left(-\right)^{s\left(LT_{t}+KT_{s}\right)}\sum_{k=1}^{M}\int d\theta_{2}\cdots d\theta_{M}d\theta_{1}\,\bar{h}_{k+1,k+L+2}\bar{\psi}_{t}\left(\theta_{k+L+2}\cdots\theta_{k+1}\right)\bar{\psi}_{s}\left(\theta_{k+2}\cdots\theta_{k+L+1}\right)\psi_{r}\left(\theta_{2}\cdots,\theta_{M},\theta_{1}\right)\\
 & = & \left(-\right)^{s\left(LT_{t}+KT_{s}\right)}\sum_{k=1}^{M}\int d\theta_{1}\cdots d\theta_{M}\,\bar{h}_{k+1,k+L+2}\bar{\psi}_{t}\left(\theta_{k+L+2}\cdots\theta_{k+1}\right)\bar{\psi}_{s}\left(\theta_{k+2}\cdots\theta_{k+L+1}\right)\psi_{r}\left(\theta_{1}\cdots\theta_{M}\right)\\
 & = & \left(-\right)^{s\left(LT_{t}+KT_{s}\right)}M\int d\theta\,\bar{h}_{M,L+1}\bar{\psi}_{t}\left(\theta_{L+1}\cdots\theta_{M}\right)\bar{\psi}_{s}\left(\theta_{1}\cdots\theta_{L}\right)\psi_{r}\left(\theta_{1},\cdots,\theta_{M}\right)\\
 & = & \left(-\right)^{s\left(LT_{t}+KT_{s}\right)}M\int d\theta\,\bar{\psi}_{t}\left(\theta_{L+1}\cdots\theta_{M}\right)\bar{\psi}_{s}\left(\theta_{1}\cdots\theta_{L}\right)h_{M,L+1}\psi_{r}\left(\theta_{1},\cdots,\theta_{M}\right)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Without loss of generality, we can symmetrize or antisymmetrize 
