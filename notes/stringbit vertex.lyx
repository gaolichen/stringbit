\begin_layout Standard
The color operator is defined as 
\begin_inset Formula 
\begin{eqnarray*}
G_{\alpha}^{\beta} & = & \sum_{n=0}^{s}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}-\normord{\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{a_{1}\cdots a_{n}}}\right)_{\alpha}^{\beta}\\
 & = & \sum_{n=0}^{s}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}-\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{a_{1}\cdots a_{n}}+\delta_{a_{1}a_{1}}\cdots\delta_{a_{n}a_{n}}\right)_{\alpha}^{\beta}\\
 & = & \sum_{n=0}^{s}\left[\left(\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}-\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}+N\delta_{a_{1}a_{1}}\cdots\delta_{a_{n}a_{n}}\delta_{\alpha}^{\beta}\right]\\
 & = & \left[\sum_{n=0}^{s}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}-\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}+2^{s}N\delta_{\alpha}^{\beta}\right]
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\tr G^{2} & = & \sum_{n=0}^{s}\sum_{k=0}^{s}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}-\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{a_{1}\cdots a_{n}}\right)\left(\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}-\phi_{b_{1}\cdots a_{k}}\bar{\phi}_{b_{1}\cdots b_{k}}\right)+2^{s+1}N^{2}\\
 &  & +2^{s+1}N\sum_{n=0}^{s}\tr\left(\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}-\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{a_{1}\cdots a_{n}}\right)
\end{eqnarray*}

\end_inset

 Or 
\begin_inset Formula 
\begin{eqnarray*}
\tr G^{2} & = & \sum_{n=0}^{s}\sum_{k=0}^{s}\tr\left(\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}-\normord{\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{a_{1}\cdots a_{n}}}\right)\left(\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}-\normord{\phi_{b_{1}\cdots a_{k}}\bar{\phi}_{b_{1}\cdots b_{k}}}\right)\\
 & = & \sum_{n=0}^{s}\sum_{k=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}+\sum_{n=0}^{s}\sum_{k=0}^{s}\tr\normord{\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{a_{1}\cdots a_{n}}}\normord{\phi_{b_{1}\cdots a_{k}}\bar{\phi}_{b_{1}\cdots b_{k}}}\\
 &  & -\sum_{n=0}^{s}\sum_{k=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\normord{\phi_{b_{1}\cdots b_{k}}\bar{\phi}_{b_{1}\cdots b_{k}}}-\sum_{n=0}^{s}\sum_{k=0}^{s}\tr\normord{\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{a_{1}\cdots a_{n}}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}
\end{eqnarray*}

\end_inset

 
\begin_inset Formula 
\begin{eqnarray*}
\text{second term} & = & \sum_{n=0}^{s}\sum_{k=0}^{s}\tr\normord{\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{a_{1}\cdots a_{n}}}\normord{\phi_{b_{1}\cdots b_{k}}\bar{\phi}_{b_{1}\cdots b_{k}}}\\
 & = & \sum_{n=0}^{s}\sum_{k=0}^{s}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\left(\bar{\phi}_{b_{1}\cdots b_{k}}\right)_{\sigma}^{\alpha}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\\
 & = & \sum_{n=0}^{s}\sum_{k=0}^{s}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\left[\left(\bar{\phi}_{b_{1}\cdots b_{k}}\right)_{\sigma}^{\alpha},\left(\phi_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\right]_{\pm}\\
 &  & +\sum_{n=0}^{s}\sum_{k=0}^{s}\left(-\right)^{k^{2}}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\left(\bar{\phi}_{b_{1}\cdots b_{k}}\right)_{\sigma}^{\alpha}\\
 & = & \sum_{n=0}^{s}\sum_{k=0}^{s}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\delta_{\gamma}^{\alpha}\delta_{\sigma}^{\sigma}\delta_{b_{1}b_{1}}\cdots\delta_{b_{k}b_{k}}\\
 &  & +\sum_{n=0}^{s}\sum_{k=0}^{s}\left(-\right)^{k^{2}+kn}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\left(\bar{\phi}_{b_{1}\cdots b_{k}}\right)_{\sigma}^{\alpha}\\
 & = & \sum_{n=0}^{s}2^{s}N\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\beta}^{\alpha}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\\
 &  & +\sum_{n=0}^{s}\sum_{k=0}^{s}\left(-\right)^{k^{2}+kn}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\left[\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta},\left(\bar{\phi}_{b_{1}\cdots b_{k}}\right)_{\sigma}^{\alpha}\right]\\
 &  & +\sum_{n=0}^{s}\sum_{k=0}^{s}\left(-\right)^{k^{2}+kn+nk}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\left(\bar{\phi}_{b_{1}\cdots b_{k}}\right)_{\sigma}^{\alpha}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\\
 & = & 2^{s}N\sum_{n=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}+\sum_{n=0}^{s}\sum_{k=0}^{s}\left(-\right)^{k^{2}+kn}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\delta_{nk}\delta_{\alpha}^{\alpha}\delta_{\sigma}^{\beta}\delta_{a_{1}b_{1}}\cdots\delta_{a_{n}b_{n}}\\
 &  & +\sum_{n=0}^{s}\sum_{k=0}^{s}\left(-\right)^{k^{2}}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{b_{1}\cdots b_{k}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{a_{1}\cdots a_{n}}\\
 & = & 2^{s}N\sum_{n=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}+N\sum_{n=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\\
 &  & +\sum_{n=0}^{s}\sum_{k=0}^{s}\left(-\right)^{k^{2}}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{b_{1}\cdots b_{k}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{a_{1}\cdots a_{n}}\\
 & = & \left(2^{s}+1\right)N\sum_{n=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}+\sum_{n=0}^{s}\sum_{k=0}^{s}\left(-\right)^{k^{2}}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{b_{1}\cdots b_{k}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{a_{1}\cdots a_{n}}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\text{third term} & = & -\sum_{n=0}^{s}\sum_{k=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\normord{\phi_{b_{1}\cdots b_{k}}\bar{\phi}_{b_{1}\cdots b_{k}}}\\
 & = & -\sum_{n=0}^{s}\sum_{k=0}^{s}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left(\bar{\phi}_{b_{1}\cdots b_{k}}\right)_{\sigma}^{\alpha}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\\
 & = & -\sum_{n=0}^{s}\sum_{k=0}^{s}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\left[\left(\phi_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma},\left(\bar{\phi}_{b_{1}\cdots b_{k}}\right)_{\sigma}^{\alpha}\right]_{\pm}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\\
 &  & -\sum_{n=0}^{s}\sum_{k=0}^{s}\left(-\right)^{nk}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\left(\bar{\phi}_{b_{1}\cdots b_{k}}\right)_{\sigma}^{\alpha}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\\
 & = & -\sum_{n=0}^{s}\sum_{k=0}^{s}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\delta_{nk}\delta_{\beta}^{\alpha}\delta_{\sigma}^{\gamma}\delta_{a_{1}b_{1}}\cdots\delta_{a_{n}b_{n}}\\
 &  & -\sum_{n=0}^{s}\sum_{k=0}^{s}\left(-\right)^{nk+nk}\left(\bar{\phi}_{b_{1}\cdots b_{k}}\right)_{\sigma}^{\alpha}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\\
 & = & -\sum_{n=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\tr\phi_{a_{1}\cdots a_{n}}-\sum_{n=0}^{s}\sum_{k=0}^{s}\tr\bar{\phi}_{b_{1}\cdots b_{k}}\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\phi_{b_{1}\cdots b_{k}}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\text{forth term} & = & -\sum_{n=0}^{s}\sum_{k=0}^{s}\tr\normord{\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{a_{1}\cdots a_{n}}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}\\
 & = & -\sum_{n=0}^{s}\sum_{k=0}^{s}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\left(\bar{\phi}_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\sigma}^{\alpha}\\
 & = & -\sum_{n=0}^{s}\sum_{k=0}^{s}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left[\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta},\left(\bar{\phi}_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\right]_{\pm}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\sigma}^{\alpha}\\
 &  & -\sum_{n=0}^{s}\sum_{k=0}^{s}\left(-\right)^{kn}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left(\bar{\phi}_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\sigma}^{\alpha}\\
 & = & -\sum_{n=0}^{s}\sum_{k=0}^{s}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\sigma}^{\alpha}\delta_{nk}\delta_{\alpha}^{\sigma}\delta_{\gamma}^{\beta}\delta_{a_{1}b_{1}}\cdots\delta_{a_{n}b_{n}}\\
 &  & -\sum_{n=0}^{s}\sum_{k=0}^{s}\left(-\right)^{2kn}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\right)_{\beta}^{\gamma}\left(\bar{\phi}_{b_{1}\cdots b_{k}}\right)_{\gamma}^{\sigma}\left(\phi_{b_{1}\cdots b_{k}}\right)_{\sigma}^{\alpha}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\\
 & = & -\sum_{n=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\tr\phi_{a_{1}\cdots a_{n}}-\sum_{n=0}^{s}\sum_{k=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}\phi_{a_{1}\cdots a_{n}}
\end{eqnarray*}

\end_inset

 Therefore
\begin_inset Formula 
\begin{eqnarray*}
\tr G^{2} & = & \sum_{n=0}^{s}\sum_{k=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}\\
 &  & +\left(2^{s}+1\right)N\sum_{n=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}+\sum_{n=0}^{s}\sum_{k=0}^{s}\left(-\right)^{k}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{b_{1}\cdots b_{k}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{a_{1}\cdots a_{n}}\\
 &  & -\sum_{n=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\tr\phi_{a_{1}\cdots a_{n}}-\sum_{n=0}^{s}\sum_{k=0}^{s}\tr\bar{\phi}_{b_{1}\cdots b_{k}}\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\phi_{b_{1}\cdots b_{k}}\\
 &  & -\sum_{n=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\tr\phi_{a_{1}\cdots a_{n}}-\sum_{n=0}^{s}\sum_{k=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}\phi_{a_{1}\cdots a_{n}}\\
 & = & \sum_{n=0}^{s}\sum_{k=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}+\sum_{n=0}^{s}\sum_{k=0}^{s}\left(-\right)^{k}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{b_{1}\cdots b_{k}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{a_{1}\cdots a_{n}}\\
 &  & -2\sum_{n=0}^{s}\sum_{k=0}^{s}\tr\bar{\phi}_{b_{1}\cdots b_{k}}\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\phi_{b_{1}\cdots b_{k}}\\
 &  & +\left(2^{s}+1\right)N\sum_{n=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}-2\sum_{n=0}^{s}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\tr\phi_{a_{1}\cdots a_{n}}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\[
Q^{a}=\sum_{n=0}^{s-1}\frac{\left(-1\right)^{n}}{n!}\tr\left[e^{i\pi/4}\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{aa_{1}\cdots a_{n}}+e^{-i\pi/4}\bar{\phi}_{aa_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\right]
\]

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\sum_{k}\left[\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{aa_{1}\cdots a_{n}},\tr\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}\right] & = & \sum_{k}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{aa_{1}\cdots a_{n}}\tr\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}-\sum_{k}\tr\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{aa_{1}\cdots a_{n}}\\
 & = & \tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{aa_{1}\cdots a_{n}}-\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{aa_{1}\cdots a_{n}}=0
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\sum_{k}\left[\tr\bar{\phi}_{aa_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}},\tr\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}\right] & = & \sum_{k}\tr\bar{\phi}_{aa_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\tr\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}-\sum_{k}\tr\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}\tr\bar{\phi}_{aa_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\\
 & = & \tr\bar{\phi}_{aa_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}-\tr\bar{\phi}_{aa_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}=0,
\end{eqnarray*}

\end_inset

which implies
\begin_inset Formula 
\[
\left[Q^{a},\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\right]=0.
\]

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\sum_{n}\left[\tr\bar{\phi}_{a_{1}\cdots a_{k}}\phi_{aa_{1}\cdots a_{k}},\tr\bar{\phi}_{a_{1}\cdots a_{n}}\tr\phi_{a_{1}\cdots a_{n}}\right] & = & \sum_{n}\tr\bar{\phi}_{a_{1}\cdots a_{k}}\phi_{aa_{1}\cdots a_{k}}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\tr\phi_{a_{1}\cdots a_{n}}-\sum_{n}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\tr\phi_{a_{1}\cdots a_{n}}\tr\bar{\phi}_{a_{1}\cdots a_{k}}\phi_{aa_{1}\cdots a_{k}}\\
 & = & \tr\bar{\phi}_{a_{1}\cdots a_{k}}\tr\phi_{aa_{1}\cdots a_{k}}-\tr\bar{\phi}_{a_{1}\cdots a_{k}}\tr\phi_{aa_{1}\cdots a_{k}}=0
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\sum_{n}\left[\tr\bar{\phi}_{aa_{1}\cdots a_{k}}\phi_{a_{1}\cdots a_{k}},\tr\bar{\phi}_{a_{1}\cdots a_{n}}\tr\phi_{a_{1}\cdots a_{n}}\right] & = & \sum_{n}\tr\bar{\phi}_{aa_{1}\cdots a_{k}}\phi_{a_{1}\cdots a_{k}}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\tr\phi_{a_{1}\cdots a_{n}}-\sum_{n}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\tr\phi_{a_{1}\cdots a_{n}}\tr\bar{\phi}_{aa_{1}\cdots a_{k}}\phi_{a_{1}\cdots a_{k}}\\
 & = & \tr\bar{\phi}_{aa_{1}\cdots a_{k}}\tr\phi_{a_{1}\cdots a_{k}}-\tr\bar{\phi}_{aa_{1}\cdots a_{k}}\tr\phi_{a_{1}\cdots a_{k}}=0
\end{eqnarray*}

\end_inset

 which implies 
\begin_inset Formula 
\[
