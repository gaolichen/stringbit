The Hamiltonian and its action on color singlets
\end_layout

\begin_layout Standard
The (anti)communication relations among string bit creation and annihilation
 operators is
\begin_inset Formula 
\begin{multline}
\left[\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta},\left(\bar{\phi}_{b_{1}\cdots b_{m}}\right)_{\gamma}^{\delta}\right]_{\pm}\equiv\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\left(\bar{\phi}_{b_{1}\cdots b_{m}}\right)_{\gamma}^{\delta}-\left(-1\right)^{mn}\left(\bar{\phi}_{b_{1}\cdots b_{m}}\right)_{\gamma}^{\delta}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\\
=\delta_{mn}\delta_{\alpha}^{\delta}\delta_{\gamma}^{\beta}\sum_{P}\left(-1\right)^{P}\delta_{a_{1}b_{P_{1}}}\cdots\delta_{a_{n}b_{P_{n}}}.\label{eq:App-H-commutation}
\end{multline}

\end_inset

 The creation operator 
\begin_inset Formula $\bar{\phi}$
\end_inset

 creates a boson if 
\begin_inset Formula $n$
\end_inset

 is even and fermion if 
\begin_inset Formula $n$
\end_inset

 is odd.
 The summation go through all permutations of 
\begin_inset Formula $1,2,\dots,n$
\end_inset

.
 
\end_layout

\begin_layout Standard
The Hamiltonian in the paper can be written as 
\begin_inset Formula 
\[
H_{S}=H_{1}+H_{2}+H_{3}+H_{4}+H_{5}+s\xi\Delta H,
\]

\end_inset

 where 
\begin_inset Formula $H_{i}$
\end_inset

 are terms introduced by 
\begin_inset CommandInset citation
LatexCommand cite
key "Thorn:2014hia,Thorn:2015wli"

\end_inset


\begin_inset Formula 
\[
H_{1}=\frac{2}{N}\sum_{n=0}^{s}\sum_{k=0}^{s}\frac{s-2n}{n!k!}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}\phi_{a_{1}\cdots a_{n}},
\]

\end_inset


\begin_inset Formula 
\[
H_{2}=\frac{2}{N}\sum_{n=0}^{s-1}\sum_{k=0}^{s-1}\frac{\left(-1\right)^{k}}{n!k!}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\bar{\phi}_{bb_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}\phi_{ba_{1}\cdots a_{n}},
\]

\end_inset


\begin_inset Formula 
\[
H_{3}=\frac{2}{N}\sum_{n=0}^{s-1}\sum_{k=0}^{s-1}\frac{\left(-1\right)^{k}}{n!k!}\tr\bar{\phi}_{ba_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{bb_{1}\cdots b_{k}}\phi_{a_{1}\cdots a_{n}},
\]

\end_inset


\begin_inset Formula 
\[
H_{4}=\frac{2i}{N}\sum_{n=0}^{s-1}\sum_{k=0}^{s-1}\frac{\left(-1\right)^{k}}{n!k!}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{bb_{1}\cdots b_{k}}\phi_{ba_{1}\cdots a_{n}},
\]

\end_inset


\begin_inset Formula 
\[
H_{5}=-\frac{2i}{N}\sum_{n=0}^{s-1}\sum_{k=0}^{s-1}\frac{\left(-1\right)^{k}}{n!k!}\tr\bar{\phi}_{ba_{1}\cdots a_{n}}\bar{\phi}_{bb_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}\phi_{a_{1}\cdots a_{n}},
\]

\end_inset

 and 
\begin_inset Formula $\Delta H$
\end_inset

 is the new term introduced
\begin_inset Formula 
\begin{align*}
\Delta H & =\frac{2}{N}\left(\sum_{n=0}^{s}\sum_{k=0}^{s}\frac{1}{n!k!}\tr\bar{\phi}_{b_{1}\cdots b_{k}}\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\phi_{b_{1}\cdots b_{k}}-\tilde{M}\right),\\
\tilde{M} & =\sum_{n=0}^{s}\frac{1}{n!}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}-\frac{1}{N}\sum_{n=0}^{s}\frac{1}{n!}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\tr\phi_{a_{1}\cdots a_{n}}.
\end{align*}

\end_inset

 
\begin_inset Formula $H_{S}$
\end_inset

 commutes with the supersymmetry operators 
\begin_inset Formula 
\[
Q^{a}=\sum_{n=0}^{s-1}\frac{\left(-1\right)^{n}}{n!}\tr\left[e^{i\pi/4}\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{aa_{1}\cdots a_{n}}+e^{-i\pi/4}\bar{\phi}_{aa_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\right],
\]

\end_inset


\begin_inset Formula 
\[
\left\{ Q^{a},Q^{b}\right\} =2M\delta_{ab},
\]

\end_inset

 which will guarantee equal numbers of bosonic and fermionic eigenstates
 at each energy level.
 
\end_layout

\begin_layout Standard
Using the commutation relations 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:App-H-commutation"

\end_inset

, we obtain the action of 
\begin_inset Formula $H_{i}$
\end_inset

 on single trace states
\begin_inset CommandInset citation
LatexCommand cite
key "Thorn:2015wli"

\end_inset


\begin_inset Formula 
\begin{multline*}
H_{1}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}=2\sum_{k=1}^{M}\left(s-2\theta_{k}^{a}\frac{d}{d\theta_{k}^{a}}\right)T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}\\
+\frac{2}{N}\sum_{k=1}^{M}\left(s-2\theta_{k}^{a}\frac{d}{d\theta_{k}^{a}}\right)\sum_{l\neq k,k+1}T\left(\theta_{1},\cdots,\theta_{k}\right)T\left(\theta_{k+1},\cdots,\theta_{l-1}\right)\ket{0},
\end{multline*}

\end_inset

 
\begin_inset Formula 
\begin{multline*}
H_{2}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}=2\sum_{k=1}^{M}\theta_{k}^{a}\frac{d}{d\theta_{k+1}^{a}}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}\\
+\frac{2}{N}\sum_{k=1}^{M}\sum_{l\neq k,k+1}\theta_{k}^{a}\frac{d}{d\theta_{l}^{a}}T\left(\theta_{1},\cdots,\theta_{k}\right)T\left(\theta_{k+1},\cdots,\theta_{l-1}\right)\ket{0},
\end{multline*}

\end_inset


\begin_inset Formula 
\begin{multline*}
H_{3}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}=2\sum_{k=1}^{M}\theta_{k+1}^{a}\frac{d}{d\theta_{k}^{a}}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}\\
+\frac{2}{N}\sum_{k=1}^{M}\sum_{l\neq k,k+1}\theta_{l}^{a}\frac{d}{d\theta_{k}^{a}}T\left(\theta_{1},\cdots,\theta_{k}\right)T\left(\theta_{k+1},\cdots,\theta_{l-1}\right)\ket{0},
\end{multline*}

\end_inset


\begin_inset Formula 
\begin{multline*}
H_{4}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}=-2i\sum_{k=1}^{M}\theta_{k}^{a}\theta_{k+1}^{a}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}\\
-\frac{2i}{N}\sum_{k=1}^{M}\sum_{l\neq k,k+1}\theta_{k}^{a}\theta_{l}^{a}T\left(\theta_{1},\cdots,\theta_{k}\right)T\left(\theta_{k+1},\cdots,\theta_{l-1}\right)\ket{0},
\end{multline*}

\end_inset


\begin_inset Formula 
\begin{multline*}
H_{5}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}=-2i\sum_{k=1}^{M}\frac{d}{d\theta_{k}^{a}}\frac{d}{d\theta_{k+1}^{a}}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}\\
-\frac{2i}{N}\sum_{k=1}^{M}\sum_{l\neq k,k+1}\frac{d}{d\theta_{k}^{a}}\frac{d}{d\theta_{l}^{a}}T\left(\theta_{1},\cdots,\theta_{k}\right)T\left(\theta_{k+1},\cdots,\theta_{l-1}\right)\ket{0}.
\end{multline*}

\end_inset

 Similarly, the action of 
\begin_inset Formula $\Delta H$
\end_inset

 on single trace state is
\begin_inset Formula 
\[
\Delta HT\left(\theta_{1},\dots,\theta_{M}\right)\ket{0}=\frac{2}{N}\sum_{i=1}^{M}\sum_{j\neq i+1}^{M}T\left(\theta_{j},\dots\theta_{i}\right)T\left(\theta_{i+1},\dots\theta_{j-1}\right)\ket{0}.
\]

\end_inset

 The actions of 
\begin_inset Formula $H_{i}$
\end_inset

 on double traces are 
\begin_inset CommandInset citation
LatexCommand cite
key "Thorn:2015wli"

\end_inset

 
\begin_inset Formula 
\begin{multline*}
H_{1}T\left(\theta_{1}\cdots\theta_{K}\right)T\left(\eta_{1}\cdots\eta_{L}\right)\ket{0}_{\mathrm{Fussion}}=\frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}\left(s-2\theta_{k}^{a}\frac{d}{d\theta_{k}^{a}}\right)T\left(\theta_{k+1}\cdots\theta_{k}\eta_{l}\cdots\eta_{l-1}\right)\ket{0}\\
+\frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}\left(s-2\eta_{l}^{a}\frac{d}{d\eta_{l}^{a}}\right)T\left(\theta_{k}\cdots\theta_{k-1}\eta_{l+1}\cdots\eta_{l}\right)\ket{0},
\end{multline*}

\end_inset


\begin_inset Formula 
\begin{multline*}
H_{2}T\left(\theta_{1}\cdots\theta_{K}\right)T\left(\eta_{1}\cdots\eta_{L}\right)\ket{0}_{\mathrm{Fussion}}=\frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}\theta_{k}^{a}\frac{d}{d\eta_{l}}T\left(\theta_{k+1}\cdots\theta_{k}\eta_{l}\cdots\eta_{l-1}\right)\ket{0}\\
+\frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}\eta_{l}^{a}\frac{d}{d\theta_{k}^{a}}T\left(\theta_{k}\cdots\theta_{k-1}\eta_{l+1}\cdots\eta_{l}\right)\ket{0}.
\end{multline*}

\end_inset

 Similarly, the action of 
\begin_inset Formula $\Delta H$
\end_inset

 on double trace states is
\begin_inset Formula 
\begin{multline*}
\Delta HT\left(\theta_{1}\cdots\theta_{K}\right)T\left(\eta_{1}\cdots\eta_{L}\right)\ket{0}_{\mathrm{Fussion}}=\frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}T\left(\theta_{k+1}\cdots\theta_{k}\eta_{l}\cdots\eta_{l-1}\right)\ket{0}\\
+\frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}T\left(\theta_{k}\cdots\theta_{k-1}\eta_{l+1}\cdots\eta_{l}\right)\ket{0}.
\end{multline*}

\end_inset


\end_layout

\begin_layout Standard
It is noteworthy the action of 
\begin_inset Formula $\Delta H$
\end_inset

 on color singlet is the same as a term 
\begin_inset Formula $H^{\prime}$
\end_inset

, which is defined as
\begin_inset Formula 
\[
H^{\prime}=\frac{1}{N}\sum_{n=0}^{s}\sum_{k=0}^{s}\frac{1}{n!k!}\left(\tr\bar{\phi}_{a_{1}\cdots a_{n}}\normord{\phi_{b_{1}\cdots b_{k}}\bar{\phi}_{b_{1}\cdots b_{k}}}\phi_{a_{1}\cdots a_{n}}+\left(-\right)^{nk}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\normord{\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}}\phi_{b_{1}\cdots b_{k}}\right),
\]

\end_inset

 where 
\begin_inset Formula $\normord{\phi_{b_{1}\cdots b_{k}}\bar{\phi}_{b_{1}\cdots b_{k}}}$
\end_inset

 indicates normal ordering.
 Let us sketch the proof here.
 The similar proof for 
\begin_inset Formula $s=1$
\end_inset

 case has been given in 
\begin_inset CommandInset citation
LatexCommand cite
key "Chen:2016hkz"

\end_inset

.
\end_layout

\begin_layout Standard
The color operator is defined as
\begin_inset Formula 
\[
G_{\alpha}^{\beta}=\sum_{n=0}^{s}\frac{1}{n!}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}-\normord{\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{a_{1}\cdots a_{n}}}\right)_{\alpha}^{\beta}.
\]

\end_inset

One can verify that the action of 
\begin_inset Formula $G_{\alpha}^{\beta}$
\end_inset

 on any color singlet vanishes:
\begin_inset Formula 
\[
G_{\alpha}^{\beta}\ket{\text{any color singlet}}=0.
\]

\end_inset

 By expanding 
\begin_inset Formula $G^{2}$
\end_inset

, one then find that
\begin_inset Formula 
\[
\tr G^{2}=N\left(H^{\prime}-\Delta H\right),
\]

\end_inset

 from which it follows that 
\begin_inset Formula $\Delta H$
\end_inset

 and 
\begin_inset Formula $H^{\prime}$
\end_inset

 lead to the same action on any color singlets.
  
\end_layout

\begin_layout Section
\begin_inset CommandInset label
LatexCommand label
name "sec:App_h_kl"

\end_inset

Calculation of 
\begin_inset Formula $h_{kl}$
\end_inset


\end_layout

\begin_layout Standard
In this appendix, we will find the expression of 
\begin_inset Formula $h_{kl}^{a}$
\end_inset

 in terms of lowering and raising operators.
 The 
\begin_inset Formula $h_{kl}^{a}$
\end_inset

 in the language of 
\begin_inset Formula $\theta_{k}$
\end_inset

 is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
h_{kl}=-2\left(1-2\theta_{k}\frac{d}{d\theta_{k}}\right)-2\theta_{k}\frac{d}{d\theta_{l}}-2\theta_{l}\frac{d}{d\theta_{k}}-2i\theta_{k}\theta_{l}-2i\frac{d}{d\theta_{k}}\frac{d}{d\theta_{l}}+2\xi+2\delta_{k,l}.
\]

\end_inset

We now will temporary drop the last two constant terms and add them back
 in the end of the calculation.
\end_layout

\begin_layout Standard
First, we express 
\begin_inset Formula $\theta_{k}$
\end_inset

 and 
\begin_inset Formula $\frac{d}{d\theta_{k}}$
\end_inset

 in terms of 
\begin_inset Formula $\alpha_{n}$
\end_inset

 and 
\begin_inset Formula $\beta_{n}$
\end_inset

: 
\begin_inset Formula 
\[
\theta_{k}=\frac{1}{\sqrt{M}}\sum_{n=0}^{M-1}\alpha_{n}e^{2\pi ikn/M},\quad\frac{d}{d\theta_{k}}=\frac{1}{\sqrt{M}}\sum_{n=0}^{M-1}\beta_{n}e^{2\pi ikn/M}.
\]

\end_inset

 It then follows 
\begin_inset Formula 
\begin{eqnarray*}
\theta_{k}\frac{d}{d\theta_{k}} & = & \frac{1}{M}\sum_{n,m=0}^{M-1}\alpha_{n}\beta_{m}\exp\left(2\pi ik\frac{m+n}{M}\right)\\
\theta_{k}\frac{d}{d\theta_{l}}+\theta_{l}\frac{d}{d\theta_{k}} & = & \frac{1}{M}\sum_{n,m=0}^{M-1}\left(\alpha_{n}\beta_{m}+\alpha_{m}\beta_{n}\right)\left[\exp\left(2\pi i\frac{kn+lm}{M}\right)\right]\\
\theta_{k}\theta_{l} & = & \frac{1}{M}\sum_{n,m=0}^{M-1}\alpha_{n}\alpha_{m}\exp\left(2\pi i\frac{kn+lm}{M}\right)\\
\frac{d}{d\theta_{k}}\frac{d}{d\theta_{l}} & = & \frac{1}{M}\sum_{n,m=0}^{M-1}\beta_{n}\beta_{m}\exp\left(2\pi i\frac{kn+lm}{M}\right).
\end{eqnarray*}

\end_inset

 Substituting above into 
\begin_inset Formula $h_{kl}$
\end_inset

, 
\begin_inset Formula 
\begin{eqnarray*}
h_{kl} & = & -2+\frac{4}{M}\sum_{n,m=0}^{M-1}\alpha_{n}\beta_{m}\exp\left(2\pi ik\frac{m+n}{M}\right)\\
 &  & -\frac{2}{M}\sum_{n,m=0}^{M-1}\left(\alpha_{n}\beta_{m}+\alpha_{m}\beta_{n}+i\alpha_{n}\alpha_{m}+i\beta_{n}\beta_{m}\right)\exp\left(2\pi i\frac{kn+lm}{M}\right)\\
 & = & -2+\frac{4}{M}\sum_{n,m=1}^{M-1}\alpha_{n}\beta_{m}\exp\left(2\pi ik\frac{m+n}{M}\right)\\
 &  & -\frac{2}{M}\sum_{n,m=1}^{M-1}\left(\alpha_{n}\beta_{m}+\alpha_{m}\beta_{n}+i\alpha_{n}\alpha_{m}+i\beta_{n}\beta_{m}\right)\exp\left(2\pi i\frac{kn+lm}{M}\right)\\
 &  & +\frac{2}{M}\sum_{n=1}^{M-1}\left(\alpha_{n}\beta_{0}+\alpha_{0}\beta_{n}-i\alpha_{n}\alpha_{0}-i\beta_{n}\beta_{0}\right)\left[\exp\left(2\pi i\frac{kn}{M}\right)-\exp\left(2\pi i\frac{ln}{M}\right)\right]\\
 & \equiv & h_{kl}^{\left(1\right)}+h_{kl}^{\left(0\right)},
\end{eqnarray*}

\end_inset

 where in last step we have define the terms involving zero modes as 
\begin_inset Formula $h_{kl}^{\left(0\right)}$
\end_inset

 and the rest as 
\begin_inset Formula $h_{kl}^{\left(1\right)}$
\end_inset

.
 
\end_layout

\begin_layout Standard
Using 
\begin_inset Formula 
\[
\alpha_{k}=c_{k}F_{M-k}^{\dagger}+s_{k}F_{k},\quad\beta_{k}=-s_{k}F_{M-k}^{\dagger}+c_{k}F_{k},\quad k=1,\cdots,M-1,
\]

\end_inset

we have 
\begin_inset Formula 
\[
\alpha_{n}\alpha_{m}=c_{n}c_{m}F_{M-n}^{\dagger}F_{M-m}^{\dagger}+s_{n}s_{m}F_{n}F_{m}+c_{n}s_{m}F_{M-n}^{\dagger}F_{m}-c_{m}s_{n}F_{M-m}^{\dagger}F_{n}+c_{n}s_{n}\delta_{m+n,M}
\]

\end_inset


\begin_inset Formula 
\[
\alpha_{n}\beta_{m}=-c_{n}s_{m}F_{M-n}^{\dagger}F_{M-m}^{\dagger}+c_{m}s_{n}F_{n}F_{m}+c_{n}c_{m}F_{M-n}^{\dagger}F_{m}+s_{n}s_{m}F_{M-m}^{\dagger}F_{n}+s_{n}^{2}\delta_{m+n,M}
\]

\end_inset


\begin_inset Formula 
\[
\beta_{n}\beta_{m}=s_{n}s_{m}F_{M-n}^{\dagger}F_{M-m}^{\dagger}+c_{n}c_{m}F_{n}F_{m}-c_{m}s_{n}F_{M-n}^{\dagger}F_{m}+c_{n}s_{m}F_{M-m}^{\dagger}F_{n}+c_{n}s_{n}\delta_{m+n,M}.
\]

\end_inset

 Now
\begin_inset Formula 
\begin{align*}
h_{kl}^{\left(1\right)}= & -2+\frac{4}{M}\sum_{n,m=1}^{M-1}\alpha_{n}\beta_{m}\exp\left(2\pi ik\frac{m+n}{M}\right)\\
 & -\frac{2}{M}\sum_{n,m=1}^{M-1}\left(\alpha_{n}\beta_{m}+\alpha_{m}\beta_{n}+i\alpha_{n}\alpha_{m}+i\beta_{n}\beta_{m}\right)\exp\left(2\pi i\frac{kn+lm}{M}\right)\\
= & -2+\frac{4}{M}\sum_{n,m=1}^{M-1}\exp\left(2\pi ik\frac{m+n}{M}\right)\\
 & \times\left(-c_{n}s_{m}F_{M-n}^{\dagger}F_{M-m}^{\dagger}+c_{m}s_{n}F_{n}F_{m}+c_{n}c_{m}F_{M-n}^{\dagger}F_{m}+s_{n}s_{m}F_{M-m}^{\dagger}F_{n}\right)\\
 & -\frac{2}{M}\sum_{n,m=1}^{M-1}\exp\left(2\pi i\frac{kn+lm}{M}\right)\left(-c_{n}s_{m}+c_{m}s_{n}+ic_{n}c_{m}+is_{n}s_{m}\right)F_{M-n}^{\dagger}F_{M-m}^{\dagger}\\
 & -\frac{2}{M}\sum_{n,m=1}^{M-1}\exp\left(2\pi i\frac{kn+lm}{M}\right)\left(c_{m}s_{n}-c_{n}s_{m}+is_{m}s_{n}+ic_{n}c_{m}\right)F_{n}F_{m}\\
 & -\frac{2}{M}\sum_{n,m=1}^{M-1}\left[\exp\left(2\pi i\frac{kn+lm}{M}\right)+\exp\left(2\pi i\frac{km+ln}{M}\right)\right]\left(c_{n}c_{m}+s_{n}s_{m}\right)F_{M-n}^{\dagger}F_{m}\\
 & -\frac{2}{M}\sum_{n,m=1}^{M-1}\left[\exp\left(2\pi i\frac{kn+lm}{M}\right)-\exp\left(2\pi i\frac{km+ln}{M}\right)\right]\left(ic_{n}s_{m}-ic_{m}s_{n}\right)F_{M-n}^{\dagger}F_{m}\\
 & +\frac{4}{M}\sum_{n=1}^{M-1}s_{n}^{2}-\frac{2}{M}\sum_{n=1}^{M-1}\exp\left(2n\pi i\frac{k-l}{M}\right)\left(2s_{n}^{2}+2ic_{n}s_{n}\right).
\end{align*}

\end_inset

After combining like terms and antisymmetrize 
\begin_inset Formula $F^{\dagger}F^{\dagger}$
\end_inset

 and 
\begin_inset Formula $FF$
\end_inset

 terms, we obtain 
\begin_inset Formula 
\begin{equation}
h_{kl}^{\left(1\right)}=\frac{2}{M}\sum_{n,m=1}^{M-1}\left(A_{nm}^{\dagger}F_{n}^{\dagger}F_{m}^{\dagger}+A_{nm}F_{n}F_{m}+2A_{-n,m}F_{n}^{\dagger}F_{m}\right)-\frac{2}{M}\cot\frac{\pi}{2M}+\frac{2}{M}\cot\frac{2\left(k-l\right)+1}{2M}\pi,\label{eq:app_hkl1}
\end{equation}

\end_inset

 where 
\begin_inset Formula 
\[
A_{nm}=\exp\left(2\pi ik\frac{m+n}{M}\right)\sin\frac{m-n}{2M}\pi+\exp\left(2\pi i\frac{\left(k+l\right)\left(m+n\right)}{2M}\right)\sin\frac{\left(l-k-1/2\right)\left(m-n\right)\pi}{M}.
\]

\end_inset


\end_layout

\begin_layout Standard
Applying 
\begin_inset Formula 
\[
\alpha_{n}\beta_{0}=\exp\left(-\frac{i\pi}{4}\right)c_{n}F_{M-n}^{\dagger}F_{0}+s_{n}F_{n}F_{0}
\]

\end_inset


\begin_inset Formula 
\[
\alpha_{0}\beta_{n}=\exp\left(\frac{i\pi}{4}\right)\left(s_{n}F_{M-n}^{\dagger}F_{0}^{\dagger}+c_{n}F_{0}^{\dagger}F_{n}\right)
\]

\end_inset


\begin_inset Formula 
\[
\alpha_{n}\alpha_{0}=\exp\left(\frac{i\pi}{4}\right)\left(c_{n}F_{M-n}^{\dagger}F_{0}^{\dagger}-s_{n}F_{0}^{\dagger}F_{n}\right)
\]

\end_inset


\begin_inset Formula 
\[
\beta_{n}\beta_{0}=\exp\left(-\frac{i\pi}{4}\right)\left(-s_{n}F_{M-n}^{\dagger}F_{0}+c_{n}F_{n}F_{0}\right)
\]

\end_inset

 to 
\begin_inset Formula $h_{kl}^{\left(0\right)}$
\end_inset

, we have
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
h_{kl}^{\left(0\right)} & = & \frac{2}{M}\sum_{n=1}^{M-1}\left(\alpha_{n}\beta_{0}+\alpha_{0}\beta_{n}-i\alpha_{n}\alpha_{0}-i\beta_{n}\beta_{0}\right)\left[\exp\left(2\pi i\frac{kn}{M}\right)-\exp\left(2\pi i\frac{ln}{M}\right)\right]\\
 & = & \frac{2}{M}\sum_{n=1}^{M-1}X_{n}^{\dagger}\left(F_{0}^{\dagger}F_{n}^{\dagger}-F_{n}^{\dagger}F_{0}\right)+\mathrm{h.c.},
\end{eqnarray*}

\end_inset

 where
\begin_inset Formula 
\[
X_{n}=i\left[\exp\left(2\pi i\frac{ln}{M}\right)-\exp\left(2\pi i\frac{kn}{M}\right)\right]\exp\left(-\frac{in\pi}{2M}\right).
\]

\end_inset

 Now we see that 
\begin_inset Formula 
\begin{eqnarray*}
A_{n0} & = & -\exp\left(2\pi ik\frac{n}{M}\right)\sin\frac{n\pi}{2M}-\exp\left(2\pi i\frac{\left(k+l\right)n}{2M}\right)\sin\frac{\left(l-k-1/2\right)n\pi}{M}\\
 & = & -\exp\left(2\pi ik\frac{n}{M}\right)\sin\frac{n\pi}{2M}-\frac{1}{2i}\exp\left(2\pi i\frac{\left(l-1/2\right)n}{M}\right)+\frac{1}{2i}\exp\left(2\pi i\frac{\left(k+1/2\right)n}{M}\right)\\
 & = & \exp\left(2\pi ik\frac{n}{M}\right)\left(-\sin\frac{n\pi}{2M}+\frac{1}{2i}\exp\left(\frac{in\pi}{2M}\right)\right)-\frac{1}{2i}\exp\left(2\pi i\frac{ln}{M}\right)\exp\left(-\frac{in\pi}{2M}\right)\\
 & = & -\frac{i}{2}\exp\left(2\pi ik\frac{n}{M}\right)\exp\left(-\frac{in\pi}{2M}\right)+\frac{i}{2}\exp\left(2\pi i\frac{ln}{M}\right)\exp\left(-\frac{in\pi}{2M}\right)\\
 & = & \frac{1}{2}X_{n}.
\end{eqnarray*}

\end_inset

Hence, to add 
\begin_inset Formula $h_{kl}^{\left(0\right)}$
\end_inset

 terms, we can simply change the 
\begin_inset Formula $m,n$
\end_inset

 index of 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:app_hkl1"

\end_inset

 to start from 
\begin_inset Formula $0$
\end_inset

.
 Finally, adding back the constant terms, we have 
\begin_inset Formula 
\[
h_{kl}=\frac{2}{M}\sum_{n,m=0}^{M-1}\left(A_{nm}^{\dagger}F_{n}^{\dagger}F_{m}^{\dagger}+A_{nm}F_{n}F_{m}+2A_{-n,m}F_{n}^{\dagger}F_{m}\right)+\frac{2}{M}\gamma
\]

\end_inset

 where 
\begin_inset Formula 
\[
\gamma=-\cot\frac{\pi}{2M}+\cot\frac{2\left(k-l\right)+1}{2M}\pi+M\xi
\]

\end_inset


\end_layout

\begin_layout Section
\begin_inset CommandInset label
LatexCommand label
name "sec:app_AFF"

\end_inset

Calculation of 
\begin_inset Formula $\left\langle \cdots\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\exp\left(\frac{1}{2}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right)\right\rangle $
\end_inset


\end_layout

\begin_layout Standard
In this appendix we will derive the formula 
\begin_inset Formula 
\begin{multline}
\left\langle \cdots\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\exp\left(\frac{1}{2}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right)\right\rangle =\\
-\frac{2}{M}\tr\left(S^{*}C^{-1}\bar{A}\right)\left\langle \cdots\exp\left(\frac{1}{2}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right)\right\rangle +\frac{2}{M}\Omega_{mn}\left\langle \cdots f_{m}^{\dagger}f_{n}^{\dagger}\exp\left(\frac{1}{2}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right)\right\rangle \label{eq:app_AFF_1}
\end{multline}

\end_inset

 where 
\begin_inset Formula $\bar{A}\equiv A^{\dagger}$
\end_inset

 and
\begin_inset Formula 
\[
\Omega\equiv C^{-1}\bar{A}\left(C^{-1}\right)^{T},
\]

\end_inset

 and relations among 
\begin_inset Formula $F_{n}^{\dagger}$
\end_inset

, 
\begin_inset Formula $f_{k}$
\end_inset

, and 
\begin_inset Formula $f_{k}^{\dagger}$
\end_inset

 are given by 
\begin_inset Formula 
\[
F_{m}=\sum_{n=0}^{M-1}\left(f_{n}C_{mn}+f_{n}^{\dagger}S_{mn}\right),\quad0\leq m\leq M-1
\]

\end_inset

 with
\begin_inset Formula 
\[
C_{mn}M_{nl}+S_{ml}=0.
\]

\end_inset


\end_layout

\begin_layout Standard
In the following calculation, we will suppress summation symbol and use
 repeating indices to indicate summation.
 Let 
\begin_inset Formula $X=\frac{1}{2}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}$
\end_inset

, 
\begin_inset Formula $\ket{G}=\exp\left(X\right)\ket{0}$
\end_inset

, then 
\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none

\begin_inset Formula 
\[
F_{n}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\ket{G}=\exp\left(X\right)\left(\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}-\left[X,\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\right]+\frac{1}{2}\left[X,\left[X,\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\right]\right]+\cdots\right)\ket{0}.
\]

\end_inset

Now let us calculate each term in the parenthesis of RHS.
 For the first term
\family default
\series default
\shape default
\size default
\emph default
\bar default
\strikeout default
\uuline default
\uwave default
\noun default
\color inherit

\begin_inset Formula 
\begin{eqnarray*}
\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\ket{0} & = & \left(\bar{A}_{nm}f_{i}^{\dagger}f_{j}^{\dagger}C_{ni}^{*}C_{mj}^{*}+\bar{A}_{nm}f_{i}S_{ni}^{*}f_{j}^{\dagger}C_{mj}^{*}\right)\ket{0}\\
 & = & \left(\bar{A}_{nm}f_{i}^{\dagger}f_{j}^{\dagger}C_{ni}^{*}C_{mj}^{*}+\bar{A}_{nm}S_{ni}^{*}C_{mj}^{*}\left\{ f_{i},f_{j}^{\dagger}\right\} \right)\ket{0}\\
 & = & \left[f_{i}^{\dagger}\left(C^{\dagger}\bar{A}C^{*}\right)_{ij}f_{j}^{\dagger}+\tr\left(\bar{A}C^{*}S^{\dagger}\right)\right]\ket{0}
\end{eqnarray*}

\end_inset

For the second term, we first find 
\begin_inset Formula 
\begin{eqnarray*}
\left[\frac{1}{2}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},F_{m}^{\dagger}\right] & = & \frac{1}{2}M_{kl}\left[f_{k}^{\dagger}f_{l}^{\dagger},F_{m}^{\dagger}\right]\\
 & = & \frac{1}{2}M_{kl}\left(f_{k}^{\dagger}\left\{ f_{l}^{\dagger},F_{m}^{\dagger}\right\} -\left\{ f_{k}^{\dagger},F_{m}^{\dagger}\right\} f_{l}^{\dagger}\right)\\
 & = & \frac{1}{2}M_{kl}\left(f_{k}^{\dagger}\delta_{ln}S_{mn}^{*}-\delta_{kn}S_{mn}^{*}f_{l}^{\dagger}\right)\\
 & = & -S_{ml}^{*}M_{lk}f_{k}^{\dagger},
\end{eqnarray*}

\end_inset

 where in the second step we used the identity 
\begin_inset Formula 
\[
\left[AB,C\right]=A\left\{ B,C\right\} -\left\{ A,C\right\} B,
\]

\end_inset

and in the last step the property that 
\begin_inset Formula $M_{kl}$
\end_inset

 is antisymmetric.
 We then have 
\begin_inset Formula 
\begin{eqnarray*}
\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},F_{n}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\right] & = & F_{n}^{\dagger}\bar{A}_{nm}\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},F_{m}^{\dagger}\right]+\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},F_{n}^{\dagger}\right]\bar{A}_{nm}F_{m}^{\dagger}\\
 & = & -F_{n}^{\dagger}\bar{A}_{nm}S_{ml}^{*}M_{lk}f_{k}^{\dagger}-S_{nl}^{*}M_{lk}f_{k}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\\
 & = & \bar{A}_{nm}S_{ml}^{*}M_{lk}\left(f_{k}^{\dagger}F_{n}^{\dagger}-\left\{ F_{n}^{\dagger},f_{k}^{\dagger}\right\} \right)-S_{nl}^{*}M_{lk}f_{k}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\\
 & = & \bar{A}_{nm}S_{ml}^{*}M_{lk}\left(f_{k}^{\dagger}F_{n}^{\dagger}-S_{nk}^{*}\right)-S_{nl}^{*}M_{lk}f_{k}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\\
 & = & \bar{A}_{nm}S_{ml}^{*}M_{lk}f_{k}^{\dagger}F_{n}^{\dagger}-S_{nl}^{*}M_{lk}f_{k}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}-\bar{A}_{nm}S_{ml}^{*}M_{lk}S_{nk}^{*}\\
 & = & f_{k}^{\dagger}M_{kl}\left(S^{\dagger}\right)_{lm}\bar{A}_{mn}F_{n}^{\dagger}+f_{k}^{\dagger}M_{kl}\left(S^{\dagger}\right)_{ln}\bar{A}_{nm}F_{m}^{\dagger}-\bar{A}_{nm}S_{ml}^{*}M_{lk}\left(S^{\dagger}\right)_{kn}\\
 & = & 2f_{k}^{\dagger}\left(MS^{\dagger}\bar{A}\right)_{km}F_{m}^{\dagger}-\tr\left(\bar{A}S^{*}MS^{\dagger}\right).
\end{eqnarray*}

\end_inset

 It then follows 
\begin_inset Formula 
\[
\left[X,F_{n}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\right]\ket{0}=\left[2f_{k}^{\dagger}\left(MS^{\dagger}\bar{A}C^{*}\right)_{kl}f_{l}^{\dagger}-\tr\left(\bar{A}S^{*}MS^{\dagger}\right)\right]\ket{0}.
\]

\end_inset

 For the third term
\begin_inset Formula 
\begin{eqnarray*}
\left[X,\left[X,\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\right]\right] & = & \left[X,2f_{k}^{\dagger}\left(MS^{\dagger}\bar{A}\right)_{km}F_{m}^{\dagger}\right]-\zero{\left[X,\tr\left(\bar{A}S^{*}MS^{\dagger}\right)\right]}\\
 & = & 2f_{i}^{\dagger}\left(MS^{\dagger}\bar{A}\right)_{im}\left[\frac{1}{2}\sum_{k,l}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger},F_{m}^{\dagger}\right]\\
 & = & -2f_{k}^{\dagger}\left(MS^{\dagger}\bar{A}S^{*}M\right)_{kl}f_{l}^{\dagger}.
\end{eqnarray*}

\end_inset

 It follows the higher order commutation all vanish.
 We therefore have
\begin_inset Formula 
\begin{eqnarray*}
F_{n}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\ket{G} & = & \exp\left(X\right)\left(\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}-\left[X,\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\right]+\frac{1}{2}\left[X,\left[X,\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\right]\right]\right)\ket{0}\\
 & = & \left[f_{k}^{\dagger}\left(C^{\dagger}\bar{A}C^{*}\right)_{kl}f_{l}^{\dagger}+\tr\left(\bar{A}C^{*}S^{\dagger}\right)\right]\ket{G}\\
 &  & -\left[2f_{k}^{\dagger}\left(MS^{\dagger}\bar{A}C^{*}\right)_{kl}f_{l}^{\dagger}-\tr\left(\bar{A}S^{*}MS^{\dagger}\right)\right]\ket{G}\\
 &  & -f_{k}^{\dagger}\left(MS^{\dagger}\bar{A}S^{*}M\right)_{kl}f_{l}^{\dagger}\ket{G}\\
 & = & \tr\left[\bar{A}\left(C^{*}+S^{*}M\right)S^{\dagger}\right]\ket{G}\\
 &  & +f_{k}^{\dagger}\left(C^{\dagger}\bar{A}C^{*}-2MS^{\dagger}\bar{A}C^{*}-MS^{\dagger}\bar{A}S^{*}M\right)_{kl}f_{l}^{\dagger}\ket{G}\\
 & = & \tr\left[\bar{A}\left(C^{*}+S^{*}M\right)S^{\dagger}\right]\ket{G}\\
 &  & +f_{k}^{\dagger}\left(C^{\dagger}\bar{A}C^{*}-MS^{\dagger}\bar{A}C^{*}+C^{\dagger}\bar{A}S^{*}M-MS^{\dagger}\bar{A}S^{*}M\right)_{kl}f_{l}^{\dagger}\ket{G}\\
 & = & \tr\left[\bar{A}\left(C^{*}+S^{*}M\right)S^{\dagger}\right]\ket{G}\\
 &  & +f_{k}^{\dagger}\left[C^{\dagger}\bar{A}\left(C^{*}+S^{*}M\right)-MS^{\dagger}\bar{A}\left(S^{*}M+C^{*}\right)\right]_{kl}f_{l}^{\dagger}\ket{G}\\
 & = & \tr\left[\bar{A}\left(C^{*}+S^{*}M\right)S^{\dagger}\right]\ket{G}\\
 &  & +f_{k}^{\dagger}\left[\left(C^{*}+S^{*}M\right)^{T}\bar{A}\left(C^{*}+S^{*}M\right)\right]_{kl}f_{l}^{\dagger}\ket{G}.
\end{eqnarray*}

\end_inset

 Since 
\begin_inset Formula 
\[
C^{*}+S^{*}M=C^{*}-C^{*}M^{*}M=C^{*}\left(I-M^{*}M\right)=C^{*}\left(I+MM^{\dagger}\right)^{T}=C^{*}\left(C^{-1}C^{-1\dagger}\right)^{T}=\left(C^{-1}\right)^{T},
\]

\end_inset

 we have 
\begin_inset Formula 
\[
F_{n}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\ket{G}=-\tr\left(S^{*}C^{-1}\bar{A}\right)\ket{G}+f_{k}^{\dagger}\left[C^{-1}\bar{A}\left(C^{-1}\right)^{T}\right]_{kl}f_{l}^{\dagger}\ket{G}
\]

\end_inset


\end_layout

\begin_layout Section
More Mathematical Details
\end_layout

\begin_layout Subsection
Symmetry of Matrices
\end_layout

\begin_layout Standard
Let us explore the symmetry of the matrices.
 For convenience, we define 
\begin_inset Formula 
\[
\hat{n}=\begin{cases}
L-n, & \quad1\leq n<L\\
M-n-1+L, & \quad L\leq n<M-1\\
M-1, & \quad n=M-1
\end{cases}.
\]

\end_inset

From 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_C_mn1"

\end_inset

-
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_S_mM1"

\end_inset

, we see that, for 
\begin_inset Formula $1\leq m\leq M-1$
\end_inset

,
\begin_inset Formula 
\begin{equation}
C_{mn}=C_{M-m,\hat{n}}^{*},\quad S_{mn}=-S_{M-m,\hat{n}}^{*},\quad1\leq n<M-1,\label{eq:overlap_CS_symmetry1}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
C_{m,M-1}=-iC_{M-m,M-1}^{*},\quad S_{m,M-1}=-iS_{M-m,M-1}^{*}.\label{eq:overlap_CS_symmetry2}
\end{equation}

\end_inset

We now rename the indices of 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_CMS"

\end_inset

 as 
\begin_inset Formula $m\to M-m$
\end_inset

, 
\begin_inset Formula $n\to\hat{n}$
\end_inset

, 
\begin_inset Formula 
\begin{equation}
\sum_{n=1}^{M-2}C_{M-m,\hat{n}}M_{\hat{n}\hat{l}}+C_{M-m,M-1}M_{M-1,\hat{l}}+S_{M-m,\hat{l}}=0.
\end{equation}

\end_inset

 Taking compex conjugate and applying 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_CS_symmetry1"

\end_inset

 and 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_CS_symmetry2"

\end_inset

 yields
\begin_inset Formula 
\begin{equation}
\sum_{n=1}^{M-2}C_{mn}M_{\hat{n}\hat{l}}^{*}+iC_{m,M-1}M_{M-1,\hat{l}}^{*}-S_{ml}=0,\quad1\leq l<M-1,\label{eq:overlap_CMS2}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\sum_{n=1}^{M-2}C_{mn}M_{\hat{n},M-1}^{*}+iC_{m,M-1}M_{M-1,M-1}^{*}+iS_{m,M-1}=0.\label{eq:over_CMS3}
\end{equation}

\end_inset

Comparing 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_CMS2"

\end_inset

 and 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:over_CMS3"

\end_inset

 with 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_CMS"

\end_inset

, we arrive at
\begin_inset Formula 
\[
M_{nl}=-M_{\hat{n}\hat{l}}^{*},\quad M_{M-1,l}=-iM_{M-1,\hat{l}}^{*},\quad M_{n,M-1}=-iM_{\hat{n},M-1}^{*}.
\]

\end_inset


\end_layout

\begin_layout Standard
Now let us consider 
\begin_inset Formula $C^{-1}$
\end_inset

, which satisfies 
\begin_inset Formula 
\[
C_{mn}C_{nl}^{-1}-\delta_{ml}=0.
\]

\end_inset

 Renaming the indecie as 
\begin_inset Formula $m\to M-m$
\end_inset

, 
\begin_inset Formula $n\to\hat{n}$
\end_inset

, 
\begin_inset Formula $l\to M-l$
\end_inset

 and taking complex conjugates yield 
\begin_inset Formula 
\[
C_{M-m,\hat{n}}^{*}\left(C_{\hat{n},M-l}^{-1}\right)^{*}-\delta_{ml}=0.
\]

\end_inset

Using 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_CS_symmetry1"

\end_inset

 and 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:overlap_CS_symmetry2"

\end_inset

, we obtain 
\begin_inset Formula 
\[
\sum_{n=1}^{M-2}C_{mn}C_{\hat{n},M-l}^{-1*}+iC_{m,M-1}C_{M-1,M-l}^{-1*}-\delta_{ml}=0,
\]

\end_inset

 from which it follows
\begin_inset Formula 
\[
C_{M-1,m}^{-1}=iC_{M-1,M-m}^{-1*},\quad C_{nm}^{-1}=C_{\hat{n},M-m}^{-1*},\quad1\leq n<M-1
\]

\end_inset


\end_layout

\begin_layout Standard
With above results, we can show that 
\begin_inset Formula $\gamma^{\prime}=\gamma-\tr S^{*}C^{-1}A^{\dagger}$
\end_inset

 is always real.
 As 
\begin_inset Formula $\gamma$
\end_inset

 is real, we only need to show 
\begin_inset Formula $\tr S^{*}C^{-1}A^{\dagger}$
\end_inset

 is real.
 By inspection, we see that
\begin_inset Formula 
\[
A_{nm}=-A_{M-n,M-m}^{*},
\]

\end_inset

which implies 
\begin_inset Formula $A^{\dagger}$
\end_inset

 also satisifies the same relation 
\begin_inset Formula 
\[
A_{nm}^{\dagger}=-\left(A_{M-n,M-m}^{\dagger}\right)^{*}.
\]

\end_inset

 Now let 
\begin_inset Formula $X=\tr S^{*}C^{-1}A^{\dagger}$
\end_inset

, then 
\begin_inset Formula 
\[
X^{*}=S_{mn}C_{nl}^{-1*}A_{ml}.
\]

\end_inset

 Renaming indices as 
\begin_inset Formula $m\to M-m$
\end_inset

, 
\begin_inset Formula $n\to\hat{n}$
\end_inset

, 
\begin_inset Formula $l\to M-l$
\end_inset

, 
\begin_inset Formula 
\begin{align*}
X^{*} & =S_{M-m,\hat{n}}C_{\hat{n},M-l}^{-1*}A_{M-m,M-l}\\
 & =\sum_{n=1}^{M-2}\sum_{m,l=1}^{M-1}S_{mn}^{*}C_{nl}^{-1}A_{ml}^{*}+S_{M-m,M-1}C_{M-1,M-l}^{-1*}A_{M-m,M-l}\\
 & =\sum_{n=1}^{M-2}\sum_{m,l=1}^{M-1}S_{mn}^{*}C_{nl}^{-1}A_{ml}^{*}+\left(-i\right)S_{m,M-1}^{*}\left(-i\right)C_{M-1,l}^{-1}\left(-1\right)A_{ml}^{*}\\
 & =\sum_{m,n,l=1}^{M-1}S_{mn}^{*}C_{nl}^{-1}A_{ml}^{*}=X.
\end{align*}

\end_inset

It then follows that 
\begin_inset Formula $X$
\end_inset

 is real and 
\begin_inset Formula $\gamma^{\prime}$
\end_inset

 is real.
\end_layout

\begin_layout Standard
We now consider the matrix 
\begin_inset Formula $\Omega$
\end_inset

.
 We have 
\begin_inset Formula 
\[
\Omega_{nk}=C_{nm}^{-1}A_{lm}^{*}C_{kl}^{-1}.
\]

\end_inset

 Changing indices as 
\begin_inset Formula $n\to\hat{n}$
\end_inset

, 
\begin_inset Formula $m\to M-m$
\end_inset

, 
\begin_inset Formula $l\to M-l$
\end_inset

, 
\begin_inset Formula $k\to\hat{k}$
\end_inset

 and taking complex conjugate yields 
\begin_inset Formula 
\begin{align*}
\Omega_{\hat{n}\hat{k}}^{*} & =\sum_{m,l=1}^{M-1}C_{\hat{n},M-m}^{-1*}A_{M-l,M-m}C_{\hat{k},M-l}^{-1*}
\end{align*}

\end_inset

 If 
\begin_inset Formula $n,k<M-1$
\end_inset

, then 
\begin_inset Formula 
\[
\Omega_{\hat{n}\hat{k}}^{*}=-\sum_{m,l=1}^{M-1}C_{nm}^{-1}A_{lm}C_{kl}^{-1}=-\Omega_{nk}
\]

\end_inset


\end_layout

\begin_layout Section
Divergence of Bosonic String One-Loop Amplitudes
\end_layout

\begin_layout Standard
In this appendix we consider the divergence of bosonic string one-loop amplitude
s.
 Here, we only consider the case of two string vertex.
 The leading order divergence of open string one-loop amplitude is
\begin_inset Formula 
\[
A\sim\int_{0}^{1}\frac{dq}{q}w^{\left(D-26\right)/24}q^{\left(2-D\right)/12}\left[f\left(q^{2}\right)\right]^{2-D}
\]

\end_inset

 where 
\begin_inset Formula 
\[
f\left(q^{2}\right)=\prod_{i=1}^{\infty}\left(1-q^{2n}\right)
\]

\end_inset


\begin_inset Formula 
\[
q=\exp\left(\frac{2\pi^{2}}{\ln w}\right)
\]

\end_inset

The divergence comes from the case when 
\begin_inset Formula $q\to0$
\end_inset

, i.e., 
\begin_inset Formula $w\to1^{-}$
\end_inset

.
 
\end_layout

\begin_layout Standard
For two string interaction, the conformal mapping is 
\begin_inset Formula 
\[
\rho=\alpha_{1}\left(\ln\psi\left(\frac{x_{1}}{z},w\right)-\ln\psi\left(\frac{w}{z},w\right)\right)
\]

\end_inset

 where 
\begin_inset Formula 
\[
\ln\psi\left(x,w\right)=\ln\left(\frac{1-x}{\sqrt{x}}\right)+\frac{\ln^{2}x}{2\ln w}+\sum_{n=1}^{\infty}\left[\ln\left(1-w^{n}x\right)+\ln\left(1-w^{n}/x\right)-2\ln\left(1-w^{n}\right)\right].
\]

\end_inset

 Let 
\begin_inset Formula $x\to-1+\epsilon$
\end_inset

, 
\begin_inset Formula $w\to1-\delta$
\end_inset

, 
\begin_inset Formula 
\begin{align*}
\Re\ln\psi\left(x,\omega\right) & =\Re\ln\left(\frac{2-\epsilon}{\sqrt{-1+\epsilon}}\right)+\Re\frac{\ln^{2}\left(-1+\epsilon\right)}{2\ln\left(1-\delta\right)}+\Re\sum_{n=1}^{\infty}\ln\left(1-w^{n}x-w^{n}/x+w^{2n}\right)\\
 & =\ln\frac{2-\epsilon}{\sqrt{1-\epsilon}}+\frac{\ln^{2}\left(1-\epsilon\right)-\pi^{2}}{2\ln\left(1-\delta\right)}+\sum_{n=1}^{\infty}\ln\left(1-w^{n}\left(-1+\epsilon\right)+\frac{w^{n}}{1-\epsilon}+w^{2n}\right)\\
 & \approx\ln2+\frac{\epsilon^{2}}{8}-\frac{\epsilon^{2}+\epsilon^{3}}{2\delta}+\frac{\pi^{2}}{2\delta}\\
 & +\sum_{n=1}^{\infty}\ln\left(1+2w^{n}+w^{2n}+\epsilon^{2}w^{n}+\epsilon^{3}w^{n}\right)+\mathcal{O}\left(\epsilon^{3}\right)\\
 & \approx\ln2+\frac{\epsilon^{2}}{8}-\frac{\epsilon^{2}+\epsilon^{3}}{2\delta}+\frac{\pi^{2}}{2\delta}\\
 & +\sum_{n=1}^{\infty}2\ln\left(1+w^{n}\right)+\sum_{n=1}^{\infty}\frac{w^{n}}{\left(1+w^{n}\right)^{2}}\left(\epsilon^{2}+\epsilon^{3}\right)+\mathcal{O}\left(\epsilon^{3}\right)
\end{align*}

\end_inset


\begin_inset Formula 
\[
\Re\ln\psi\left(x_{1},w\right)-\Re\ln\psi\left(x_{2},w\right)=\left(\epsilon_{1}^{2}-\epsilon_{2}^{2}\right)\left(8-\frac{1}{2\delta}+X\right)+\left(\epsilon_{1}^{3}-\epsilon_{2}^{3}\right)\left(X-\frac{1}{2\delta}\right),
\]

\end_inset

 where 
\begin_inset Formula 
\[
\epsilon_{1}=\frac{x_{1}}{z}+1,\quad\epsilon_{2}=\frac{w}{z}+1
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\Re\rho\left(\epsilon_{1},\epsilon_{2};\delta\right) & =\alpha_{1}\left(\epsilon_{1}^{2}-\epsilon_{2}^{2}\right)\left(8-\frac{1}{2\delta}+X\right)+\alpha_{1}\left(\epsilon_{1}^{3}-\epsilon_{2}^{3}\right)\left(X-\frac{1}{2\delta}\right)\\
 & \equiv\alpha_{1}\left(\epsilon_{1}^{2}-\epsilon_{2}^{2}\right)A+\alpha_{1}\left(\epsilon_{1}^{3}-\epsilon_{2}^{3}\right)B
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\Re\frac{d\rho}{dz} & =\Re\frac{\partial\rho}{\partial\epsilon_{1}}\frac{d\epsilon_{1}}{dz}+\Re\frac{\partial\rho}{\partial\epsilon_{2}}\frac{d\epsilon_{2}}{dz}\\
 & =\alpha_{1}\left(2\epsilon_{1}A+3\epsilon_{1}^{2}B\right)\left(-\frac{x_{1}}{z^{2}}\right)-\alpha_{1}\left(2\epsilon_{2}A+3\epsilon_{2}^{2}B\right)\left(-\frac{w}{z^{2}}\right)\\
 & =2\frac{\alpha_{1}}{z^{2}}A\left(-\epsilon_{1}x_{1}+\epsilon_{2}w\right)+3\frac{\alpha_{1}}{z^{2}}B\left(-\epsilon_{1}^{2}x_{1}+\epsilon_{2}^{2}w\right)
\end{align*}

\end_inset

 The length of the cut is determined by
\begin_inset Formula 
\begin{multline*}
\frac{d}{dx}\ln\psi\left(x,w\right)=\frac{\sqrt{x}}{1-x}+\left(-\frac{1}{2}x^{-3/2}-\frac{1}{2}x^{-1/2}\right)+\frac{\ln x}{x\ln w}+\sum_{n=1}^{\infty}\left[-\frac{w^{n}}{1-w^{n}x}+\frac{w^{n}}{x^{2}\left(1-w^{n}/x\right)}\right]\\
=\frac{\sqrt{x}}{1-x}-\frac{1+x}{2x\sqrt{x}}+\frac{\ln x}{x\ln w}+\sum_{n=1}^{\infty}w^{n}\left[\frac{1}{x\left(x-w^{n}\right)}-\frac{1}{1-w^{n}x}\right]
\end{multline*}

\end_inset

 
\begin_inset Formula 
\[
\frac{d\rho}{dz}=-\alpha_{1}\frac{1}{z^{2}}\left(\right)
\]

\end_inset


\end_layout

