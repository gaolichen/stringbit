\begin_inset Formula 
\begin{align*}
\left\langle 1\right\rangle _{V} & =\frac{2}{M}\gamma^{\prime\left(V\right)}\left\langle \exp\left(\frac{1}{2}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right)\right\rangle +\frac{2}{M}\Omega_{mn}^{\left(V\right)}\zero{\left\langle f_{m}^{\dagger}f_{n}^{\dagger}\exp\left(\frac{1}{2}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right)\right\rangle }\\
 & =\frac{2}{M}\gamma^{\prime\left(V\right)}=\frac{4}{\sqrt{3}}-4+2\xi,
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\left\langle f_{2}f_{0}\right\rangle _{V} & =\frac{2}{M}\gamma^{\prime\left(V\right)}\zero{\left\langle f_{2}f_{0}\exp\left(\frac{1}{2}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right)\right\rangle }+\frac{2}{M}\Omega_{mn}^{\left(V\right)}\left\langle f_{2}f_{0}f_{m}^{\dagger}f_{n}^{\dagger}\exp\left(\frac{1}{2}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right)\right\rangle \\
 & =\frac{4}{M}\Omega_{02}^{\left(V\right)}=2\sqrt{2}\left(1-\frac{2}{\sqrt{3}}\right),
\end{align*}

\end_inset

 Likewise
\begin_inset Formula 
\[
\left\langle 1\right\rangle _{W}=\frac{2}{M}\gamma^{\prime\left(W\right)}=-\frac{8}{\sqrt{3}}+4\xi,\quad\left\langle f_{2}f_{0}\right\rangle _{W}=\frac{4}{M}\Omega_{02}^{\left(W\right)}=2\sqrt{2}\left(1-\frac{2}{\sqrt{3}}\right).
\]

\end_inset

 For 
\begin_inset Formula $L=2$
\end_inset

, 
\begin_inset Formula 
\[
\left\langle 1\right\rangle _{V}=2\xi,\quad\left\langle f_{2}f_{0}\right\rangle _{V}=0,\quad\left\langle 1\right\rangle _{W}=-\frac{8}{\sqrt{3}}+4\xi,\quad\left\langle f_{2}f_{0}\right\rangle _{W}=-2\sqrt{2}\left(1-\frac{2}{\sqrt{3}}\right)
\]

\end_inset

Applying formula 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:energy_pert_DeltaE"

\end_inset

, we obtain 
\begin_inset Formula 
\[
\Delta E_{G}=\frac{1}{N^{2}}\left(-66\sqrt{3}\xi^{4}+360\xi^{3}-242\sqrt{3}\xi^{2}+228\xi-33\sqrt{3}\right).
\]

\end_inset

 
\end_layout

\begin_layout Standard
From the results and the formula 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:energy_pert_DeltaE"

\end_inset

, we see that 
\begin_inset Formula $\Delta E_{G}$
\end_inset

 is a polynomial of 
\begin_inset Formula $\xi$
\end_inset

 of degree 
\begin_inset Formula $2s$
\end_inset

.
