\begin_layout Section
\begin_inset CommandInset label
LatexCommand label
name "sec:app_hamiltonian"

\end_inset

The Hamiltonian and its action on color singlets
\end_layout

\begin_layout Standard
The (anti)communication relations among string bit creation and annihilation
 operators is
\begin_inset Formula 
\begin{align}
\left[\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta},\left(\bar{\phi}_{b_{1}\cdots b_{m}}\right)_{\gamma}^{\delta}\right]_{\pm} & \equiv\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\left(\bar{\phi}_{b_{1}\cdots b_{m}}\right)_{\gamma}^{\delta}-\left(-1\right)^{mn}\left(\bar{\phi}_{b_{1}\cdots b_{m}}\right)_{\gamma}^{\delta}\left(\phi_{a_{1}\cdots a_{n}}\right)_{\alpha}^{\beta}\nonumber \\
 & =\delta_{mn}\delta_{\alpha}^{\delta}\delta_{\gamma}^{\beta}\sum_{P}\left(-1\right)^{P}\delta_{a_{1}b_{P_{1}}}\cdots\delta_{a_{n}b_{P_{n}}},\label{eq:App-H-commutation}
\end{align}

\end_inset

 where the sum runs over all permutations of 
\begin_inset Formula $1,2,\dots,n$
\end_inset

.
 
\end_layout

\begin_layout Standard
The Hamiltonian in the paper can be written as 
\begin_inset Formula 
\[
H_{S}=H_{1}+H_{2}+H_{3}+H_{4}+H_{5}+s\xi\Delta H,
\]

\end_inset

 where 
\begin_inset Formula $H_{i}$
\end_inset

 are terms introduced by refs.
 
\begin_inset CommandInset citation
LatexCommand cite
key "Thorn:2014hia,Thorn:2015wli"

\end_inset


\begin_inset Formula 
\[
H_{1}=\frac{2}{N}\sum_{n=0}^{s}\sum_{k=0}^{s}\frac{s-2n}{n!k!}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}\phi_{a_{1}\cdots a_{n}},
\]

\end_inset


\begin_inset Formula 
\[
H_{2}=\frac{2}{N}\sum_{n=0}^{s-1}\sum_{k=0}^{s-1}\frac{\left(-1\right)^{k}}{n!k!}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\bar{\phi}_{bb_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}\phi_{ba_{1}\cdots a_{n}},
\]

\end_inset


\begin_inset Formula 
\[
H_{3}=\frac{2}{N}\sum_{n=0}^{s-1}\sum_{k=0}^{s-1}\frac{\left(-1\right)^{k}}{n!k!}\tr\bar{\phi}_{ba_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{bb_{1}\cdots b_{k}}\phi_{a_{1}\cdots a_{n}},
\]

\end_inset


\begin_inset Formula 
\[
H_{4}=\frac{2i}{N}\sum_{n=0}^{s-1}\sum_{k=0}^{s-1}\frac{\left(-1\right)^{k}}{n!k!}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}\phi_{bb_{1}\cdots b_{k}}\phi_{ba_{1}\cdots a_{n}},
\]

\end_inset


\begin_inset Formula 
\[
H_{5}=-\frac{2i}{N}\sum_{n=0}^{s-1}\sum_{k=0}^{s-1}\frac{\left(-1\right)^{k}}{n!k!}\tr\bar{\phi}_{ba_{1}\cdots a_{n}}\bar{\phi}_{bb_{1}\cdots b_{k}}\phi_{b_{1}\cdots b_{k}}\phi_{a_{1}\cdots a_{n}},
\]

\end_inset

 and 
\begin_inset Formula $\Delta H$
\end_inset

 is a new term introduced
\begin_inset Formula 
\begin{align*}
\Delta H & =\frac{2}{N}\left(\sum_{n=0}^{s}\sum_{k=0}^{s}\frac{1}{n!k!}\tr\bar{\phi}_{b_{1}\cdots b_{k}}\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\phi_{b_{1}\cdots b_{k}}-\tilde{M}\right),\\
\tilde{M} & =\sum_{n=0}^{s}\frac{1}{n!}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}-\frac{1}{N}\sum_{n=0}^{s}\frac{1}{n!}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\tr\phi_{a_{1}\cdots a_{n}}.
\end{align*}

\end_inset

 
\begin_inset Formula $H_{S}$
\end_inset

 commutes with the supersymmetry operators 
\begin_inset Formula 
\begin{equation}
Q^{a}=\sum_{n=0}^{s-1}\frac{\left(-1\right)^{n}}{n!}\tr\left[e^{i\pi/4}\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{aa_{1}\cdots a_{n}}+e^{-i\pi/4}\bar{\phi}_{aa_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}\right],\label{eq:App_ham_Q}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\left\{ Q^{a},Q^{b}\right\} =2M\delta_{ab},
\end{equation}

\end_inset

 which will guarantee equal numbers of bosonic and fermionic eigenstates
 at each energy level.
 
\end_layout

\begin_layout Standard
Using the commutation relations 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:App-H-commutation"

\end_inset

, we obtain the action of 
\begin_inset Formula $H_{i}$
\end_inset

 on single trace states 
\begin_inset CommandInset citation
LatexCommand cite
key "Thorn:2015wli"

\end_inset


\begin_inset Formula 
\begin{align*}
H_{1}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0} & =2\sum_{k=1}^{M}\left(s-2\theta_{k}^{a}\frac{d}{d\theta_{k}^{a}}\right)T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}\\
 & \hspace{1em}+\frac{2}{N}\sum_{k=1}^{M}\left(s-2\theta_{k}^{a}\frac{d}{d\theta_{k}^{a}}\right)\sum_{l\neq k,k+1}T\left(\theta_{1},\cdots,\theta_{k}\right)T\left(\theta_{k+1},\cdots,\theta_{l-1}\right)\ket{0},
\end{align*}

\end_inset

 
\begin_inset Formula 
\begin{align*}
H_{2}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0} & =2\sum_{k=1}^{M}\theta_{k}^{a}\frac{d}{d\theta_{k+1}^{a}}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}\\
 & \hspace{1em}+\frac{2}{N}\sum_{k=1}^{M}\sum_{l\neq k,k+1}\theta_{k}^{a}\frac{d}{d\theta_{l}^{a}}T\left(\theta_{1},\cdots,\theta_{k}\right)T\left(\theta_{k+1},\cdots,\theta_{l-1}\right)\ket{0},
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
H_{3}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0} & =2\sum_{k=1}^{M}\theta_{k+1}^{a}\frac{d}{d\theta_{k}^{a}}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}\\
 & \hspace{1em}+\frac{2}{N}\sum_{k=1}^{M}\sum_{l\neq k,k+1}\theta_{l}^{a}\frac{d}{d\theta_{k}^{a}}T\left(\theta_{1},\cdots,\theta_{k}\right)T\left(\theta_{k+1},\cdots,\theta_{l-1}\right)\ket{0},
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
H_{4}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0} & =-2i\sum_{k=1}^{M}\theta_{k}^{a}\theta_{k+1}^{a}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}\\
 & \hspace{1em}-\frac{2i}{N}\sum_{k=1}^{M}\sum_{l\neq k,k+1}\theta_{k}^{a}\theta_{l}^{a}T\left(\theta_{1},\cdots,\theta_{k}\right)T\left(\theta_{k+1},\cdots,\theta_{l-1}\right)\ket{0},
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
H_{5}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0} & =-2i\sum_{k=1}^{M}\frac{d}{d\theta_{k}^{a}}\frac{d}{d\theta_{k+1}^{a}}T\left(\theta_{1},\cdots,\theta_{M}\right)\ket{0}\\
 & \hspace{1em}-\frac{2i}{N}\sum_{k=1}^{M}\sum_{l\neq k,k+1}\frac{d}{d\theta_{k}^{a}}\frac{d}{d\theta_{l}^{a}}T\left(\theta_{1},\cdots,\theta_{k}\right)T\left(\theta_{k+1},\cdots,\theta_{l-1}\right)\ket{0}.
\end{align*}

\end_inset

 Similarly, the action of 
\begin_inset Formula $\Delta H$
\end_inset

 on single trace state is
\begin_inset Formula 
\[
\Delta HT\left(\theta_{1},\dots,\theta_{M}\right)\ket{0}=\frac{2}{N}\sum_{i=1}^{M}\sum_{j\neq i+1}^{M}T\left(\theta_{j},\dots\theta_{i}\right)T\left(\theta_{i+1},\dots\theta_{j-1}\right)\ket{0}.
\]

\end_inset

 The actions of 
\begin_inset Formula $H_{i}$
\end_inset

 on double traces are 
\begin_inset CommandInset citation
LatexCommand cite
key "Thorn:2015wli"

\end_inset

 
\begin_inset Formula 
\begin{align*}
H_{1}T\left(\theta_{1}\cdots\theta_{K}\right)T\left(\eta_{1}\cdots\eta_{L}\right)\ket{0}_{\mathrm{Fussion}} & =\frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}\left(s-2\theta_{k}^{a}\frac{d}{d\theta_{k}^{a}}\right)T\left(\theta_{k+1}\cdots\theta_{k}\eta_{l}\cdots\eta_{l-1}\right)\ket{0}\\
 & \hspace{1em}+\frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}\left(s-2\eta_{l}^{a}\frac{d}{d\eta_{l}^{a}}\right)T\left(\theta_{k}\cdots\theta_{k-1}\eta_{l+1}\cdots\eta_{l}\right)\ket{0},
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
H_{2}T\left(\theta_{1}\cdots\theta_{K}\right)T\left(\eta_{1}\cdots\eta_{L}\right)\ket{0}_{\mathrm{Fussion}} & =\frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}\theta_{k}^{a}\frac{d}{d\eta_{l}}T\left(\theta_{k+1}\cdots\theta_{k}\eta_{l}\cdots\eta_{l-1}\right)\ket{0}\\
 & \hspace{1em}+\frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}\eta_{l}^{a}\frac{d}{d\theta_{k}^{a}}T\left(\theta_{k}\cdots\theta_{k-1}\eta_{l+1}\cdots\eta_{l}\right)\ket{0}.
\end{align*}

\end_inset

 Similarly, the action of 
\begin_inset Formula $\Delta H$
\end_inset

 on double trace states is
\begin_inset Formula 
\begin{align*}
\Delta HT\left(\theta_{1}\cdots\theta_{K}\right)T\left(\eta_{1}\cdots\eta_{L}\right)\ket{0}_{\mathrm{Fussion}} & =\frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}T\left(\theta_{k+1}\cdots\theta_{k}\eta_{l}\cdots\eta_{l-1}\right)\ket{0}\\
 & \hspace{1em}+\frac{2}{N}\sum_{k=1}^{K}\sum_{l=1}^{L}T\left(\theta_{k}\cdots\theta_{k-1}\eta_{l+1}\cdots\eta_{l}\right)\ket{0}.
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
It is noteworthy that the action of 
\begin_inset Formula $\Delta H$
\end_inset

 on color singlet is the same as a term 
\begin_inset Formula $H^{\prime}$
\end_inset

, which is defined as
\begin_inset Formula 
\[
H^{\prime}=\frac{1}{N}\sum_{n=0}^{s}\sum_{k=0}^{s}\frac{1}{n!k!}\left(\tr\bar{\phi}_{a_{1}\cdots a_{n}}\normord{\phi_{b_{1}\cdots b_{k}}\bar{\phi}_{b_{1}\cdots b_{k}}}\phi_{a_{1}\cdots a_{n}}+\left(-\right)^{nk}\tr\bar{\phi}_{a_{1}\cdots a_{n}}\normord{\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{b_{1}\cdots b_{k}}}\phi_{b_{1}\cdots b_{k}}\right),
\]

\end_inset

 where 
\begin_inset Formula $\normord{\phi_{b_{1}\cdots b_{k}}\bar{\phi}_{b_{1}\cdots b_{k}}}$
\end_inset

 indicates normal ordering.
 Let us sketch the proof here.
 The similar proof for the 
\begin_inset Formula $s=1$
\end_inset

 case has been given in ref.
 
\begin_inset CommandInset citation
LatexCommand cite
key "Chen:2016hkz"

\end_inset

.
\end_layout

\begin_layout Standard
The color operator is defined as
\begin_inset Formula 
\[
G_{\alpha}^{\beta}=\sum_{n=0}^{s}\frac{1}{n!}\left(\bar{\phi}_{a_{1}\cdots a_{n}}\phi_{a_{1}\cdots a_{n}}-\normord{\phi_{a_{1}\cdots a_{n}}\bar{\phi}_{a_{1}\cdots a_{n}}}\right)_{\alpha}^{\beta}.
\]

\end_inset

One can verify that the action of 
\begin_inset Formula $G_{\alpha}^{\beta}$
\end_inset

 on any color singlet vanishes:
\begin_inset Formula 
\[
G_{\alpha}^{\beta}\ket{\text{any color singlet}}=0.
\]

\end_inset

 By expanding 
\begin_inset Formula $G^{2}$
\end_inset

, one then find that
\begin_inset Formula 
\[
\tr G^{2}=N\left(H^{\prime}-\Delta H\right),
\]

\end_inset

 from which it follows that 
\begin_inset Formula $\Delta H$
\end_inset

 and 
\begin_inset Formula $H^{\prime}$
\end_inset

 lead to the same action on any color singlet.
\end_layout

\begin_layout Section
\begin_inset CommandInset label
LatexCommand label
name "sec:App_normal_psibar"

\end_inset

Verifying the normalization condition for 
\begin_inset Formula $\bar{\psi}_{G}$
\end_inset


\end_layout

\begin_layout Standard
In this appendix we show that the conjugate eigenfunction of 
\begin_inset Formula $s=1$
\end_inset

, defined as eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:conj_psibarG"

\end_inset

, satisfies the normalization condition 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:conj_normalization"

\end_inset

.
 We first show that 
\begin_inset Formula $\int d^{M}\theta\,\bar{\psi}_{G}\psi_{G}=1$
\end_inset

.
 For odd 
\begin_inset Formula $M$
\end_inset

, 
\begin_inset Formula 
\begin{align*}
\int d^{M}\theta\,\bar{\psi}_{G}\psi_{G} & =\left(-i\right)^{\left\lfloor M/2\right\rfloor }\int d^{M}\theta\,\alpha_{0}\prod_{i=1}^{\left\lfloor M/2\right\rfloor }\left(-s_{i}+c_{i}\alpha_{M-i}\alpha_{i}\right)\left(c_{i}-s_{i}\alpha_{M-i}\alpha_{i}\right)\\
 & =\left(-i\right)^{\left\lfloor M/2\right\rfloor }\int d^{M}\theta\,\alpha_{0}\prod_{i=1}^{\left\lfloor M/2\right\rfloor }\left(s_{i}^{2}+c_{i}^{2}\right)\alpha_{M-i}\alpha_{i}\\
 & =\left(-i\right)^{\left\lfloor M/2\right\rfloor }\int d^{M}\theta\,\prod_{i=1}^{M}\alpha_{M-i}\\
 & =1,
\end{align*}

\end_inset

 where in the last step we used
\begin_inset Foot
status open

\begin_layout Plain Layout
We do not prove the formula 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:App_psiG_alpha_prod"

\end_inset

 here.
 But we have verified it by Mathematica program.
\end_layout

\end_inset


\begin_inset Formula 
\begin{equation}
\int d^{M}\theta\,\prod_{i=1}^{M}\alpha_{M-i}\equiv\int d^{M}\theta\,\alpha_{M-1}\alpha_{M-2}\cdots\alpha_{0}=i^{\left\lfloor \left(M-1\right)/2\right\rfloor }.\label{eq:App_psiG_alpha_prod}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Similarly, we can show 
\begin_inset Formula $\int d^{M}\theta\,\bar{\psi}_{G}\psi_{G}=1$
\end_inset

 for even 
\begin_inset Formula $M$
\end_inset

.
 
\end_layout

\begin_layout Standard
To show
\begin_inset Formula $\int d^{M}\theta\,\bar{\psi}_{G}\psi_{r}=0$
\end_inset

 for 
\begin_inset Formula $r\neq G$
\end_inset

, it suffices to show that 
\begin_inset Formula $\int d^{M}\theta\,\bar{\psi}_{G}F_{k}^{\dagger}\psi^{\prime}$
\end_inset

 vanishes for all 
\begin_inset Formula $0\leq k\leq M-1$
\end_inset

 and any eigenfunction 
\begin_inset Formula $\psi^{\prime}$
\end_inset

.
 If 
\begin_inset Formula $k=0$
\end_inset

, it clearly vanishes because both 
\begin_inset Formula $F_{0}^{\dagger}$
\end_inset

 and 
\begin_inset Formula $\bar{\psi}_{G}$
\end_inset

 contain the Grassmann odd operator 
\begin_inset Formula $\alpha_{0}$
\end_inset

.
 If 
\begin_inset Formula $0<k<M/2$
\end_inset

, 
\begin_inset Formula 
\begin{equation}
\int d^{M}\theta\,\bar{\psi}_{G}F_{k}^{\dagger}\psi^{\prime}=\int d^{M}\theta\,\left(\tilde{F}_{k}^{\dagger\pm}\bar{\psi}_{G}\right)\psi^{\prime}.\label{eq:App_psiG_norm1}
\end{equation}

\end_inset

RHS of 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:App_psiG_norm1"

\end_inset

 vanishes because of 
\begin_inset Formula 
\begin{equation}
\tilde{F}_{k}^{\dagger\pm}\bar{\psi}_{G}=0,\label{eq:App_psiG_Ftilde_psibar}
\end{equation}

\end_inset

which can be verified by checking that 
\begin_inset Formula 
\[
\tilde{F}_{k}^{\dagger\pm}\left(-s_{k}+c_{k}\alpha_{M-k}\alpha_{k}\right)=\tilde{F}_{M-k}^{\dagger\pm}\left(-s_{k}+c_{k}\alpha_{M-k}\alpha_{k}\right)=0,
\]

\end_inset


\begin_inset Formula 
\[
\left[\tilde{F}_{k}^{\dagger\pm},-s_{l}+c_{l}\alpha_{M-l}\alpha_{l}\right]=0,\quad k\neq l,\, k\neq M-l.
\]

\end_inset

Similarly, we can show that 
\begin_inset Formula $\tilde{F}_{k}^{\dagger\pm}\bar{\psi}_{G}=0$
\end_inset

 for 
\begin_inset Formula $M/2\leq k\leq M-1$
\end_inset

.
 Therefore, the normalization condition 
\begin_inset Formula $\int d^{M}\theta\,\bar{\psi}_{G}\psi_{r}=\delta_{Gr}$
\end_inset

 is proved.
 
\end_layout

\begin_layout Section
\begin_inset CommandInset label
LatexCommand label
name "sec:App_h_kl"

\end_inset

Calculation of 
\begin_inset Formula $h_{kl}$
\end_inset


\end_layout

\begin_layout Standard
In this appendix, we will find the expression of 
\begin_inset Formula $h_{kl}^{a}$
\end_inset

 in terms of lowering and raising operators.
 The 
\begin_inset Formula $h_{kl}^{a}$
\end_inset

 in the language of 
\begin_inset Formula $\theta_{k}$
\end_inset

 is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
h_{kl}=-2\left(1-2\theta_{k}\frac{d}{d\theta_{k}}\right)-2\theta_{k}\frac{d}{d\theta_{l}}-2\theta_{l}\frac{d}{d\theta_{k}}-2i\theta_{k}\theta_{l}-2i\frac{d}{d\theta_{k}}\frac{d}{d\theta_{l}}+2\xi+2\delta_{k,l}.
\]

\end_inset

We now temporary drop the last two constant terms and will add them back
 in the end of the calculation.
\end_layout

\begin_layout Standard
Using 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:dia_fourier:2"

\end_inset

, we express 
\begin_inset Formula $\theta_{k}$
\end_inset

 and 
\begin_inset Formula $\frac{d}{d\theta_{k}}$
\end_inset

 in terms of 
\begin_inset Formula $\alpha_{n}$
\end_inset

 and 
\begin_inset Formula $\beta_{n}$
\end_inset

:
\begin_inset Formula 
\begin{align*}
\theta_{k}\frac{d}{d\theta_{k}} & =\frac{1}{M}\sum_{n,m=0}^{M-1}\alpha_{n}\beta_{m}\exp\left(2\pi ik\frac{m+n}{M}\right)\\
\theta_{k}\frac{d}{d\theta_{l}}+\theta_{l}\frac{d}{d\theta_{k}} & =\frac{1}{M}\sum_{n,m=0}^{M-1}\left(\alpha_{n}\beta_{m}+\alpha_{m}\beta_{n}\right)\left[\exp\left(2\pi i\frac{kn+lm}{M}\right)\right]\\
\theta_{k}\theta_{l} & =\frac{1}{M}\sum_{n,m=0}^{M-1}\alpha_{n}\alpha_{m}\exp\left(2\pi i\frac{kn+lm}{M}\right)\\
\frac{d}{d\theta_{k}}\frac{d}{d\theta_{l}} & =\frac{1}{M}\sum_{n,m=0}^{M-1}\beta_{n}\beta_{m}\exp\left(2\pi i\frac{kn+lm}{M}\right).
\end{align*}

\end_inset

 Substituting above into 
\begin_inset Formula $h_{kl}$
\end_inset

 and rearranging, we obtain
\begin_inset Formula 
\[
h_{kl}=h_{kl}^{\left(1\right)}+h_{kl}^{\left(0\right)}
\]

\end_inset

where 
\begin_inset Formula $h_{kl}^{\left(0\right)}$
\end_inset

 are the terms with zero modes and 
\begin_inset Formula $h_{kl}^{\left(1\right)}$
\end_inset

 are the terms without, 
\begin_inset Formula 
\begin{align*}
h_{kl}^{\left(1\right)} & =-2+\frac{4}{M}\sum_{n,m=1}^{M-1}\alpha_{n}\beta_{m}\exp\left(2\pi ik\frac{m+n}{M}\right)\\
 & \hspace{1em}-\frac{2}{M}\sum_{n,m=1}^{M-1}\left(\alpha_{n}\beta_{m}+\alpha_{m}\beta_{n}+i\alpha_{n}\alpha_{m}+i\beta_{n}\beta_{m}\right)\exp\left(2\pi i\frac{kn+lm}{M}\right),
\end{align*}

\end_inset


\begin_inset Formula 
\[
h_{kl}^{\left(0\right)}=\frac{2}{M}\sum_{n=1}^{M-1}\left(\alpha_{n}\beta_{0}+\alpha_{0}\beta_{n}-i\alpha_{n}\alpha_{0}-i\beta_{n}\beta_{0}\right)\left[\exp\left(2\pi i\frac{kn}{M}\right)-\exp\left(2\pi i\frac{ln}{M}\right)\right].
\]

\end_inset


\end_layout

\begin_layout Standard
Let us first consider 
\begin_inset Formula $h_{kl}^{\left(1\right)}$
\end_inset

.
 We express nonzero modes 
\begin_inset Formula $\alpha_{m}$
\end_inset

 and 
\begin_inset Formula $\beta_{m}$
\end_inset

 in terms of raising and lowering operators.
 Using 
\begin_inset Formula 
\[
\alpha_{k}=c_{k}F_{M-k}^{\dagger}+s_{k}F_{k},\quad\beta_{k}=-s_{k}F_{M-k}^{\dagger}+c_{k}F_{k},\quad k=1,\cdots,M-1,
\]

\end_inset

we have
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{subequations}
\backslash
label{eq:App_hkl_ab}
\end_layout

\end_inset

 
\begin_inset Formula 
\begin{equation}
\alpha_{n}\alpha_{m}=c_{n}c_{m}F_{M-n}^{\dagger}F_{M-m}^{\dagger}+s_{n}s_{m}F_{n}F_{m}+c_{n}s_{m}F_{M-n}^{\dagger}F_{m}-c_{m}s_{n}F_{M-m}^{\dagger}F_{n}+c_{n}s_{n}\delta_{m+n,M},\label{eq:App_hkl_ab:1}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\alpha_{n}\beta_{m}=-c_{n}s_{m}F_{M-n}^{\dagger}F_{M-m}^{\dagger}+c_{m}s_{n}F_{n}F_{m}+c_{n}c_{m}F_{M-n}^{\dagger}F_{m}+s_{n}s_{m}F_{M-m}^{\dagger}F_{n}+s_{n}^{2}\delta_{m+n,M},\label{eq:App_hkl_ab:2}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\beta_{n}\beta_{m}=s_{n}s_{m}F_{M-n}^{\dagger}F_{M-m}^{\dagger}+c_{n}c_{m}F_{n}F_{m}-c_{m}s_{n}F_{M-n}^{\dagger}F_{m}+c_{n}s_{m}F_{M-m}^{\dagger}F_{n}+c_{n}s_{n}\delta_{m+n,M}.\label{eq:App_hkl_ab:3}
\end{equation}

\end_inset


\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{subequations}
\end_layout

\end_inset

We then apply eqs.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:App_hkl_ab"

\end_inset

 to 
\begin_inset Formula $h_{kl}^{\left(1\right)}$
\end_inset

, collect like terms, and antisymmetrize 
\begin_inset Formula $F^{\dagger}F^{\dagger}$
\end_inset

 and 
\begin_inset Formula $FF$
\end_inset

 terms to give 
\begin_inset Formula 
\begin{equation}
h_{kl}^{\left(1\right)}=\frac{2}{M}\sum_{n,m=1}^{M-1}\left(A_{nm}^{\dagger}F_{n}^{\dagger}F_{m}^{\dagger}+A_{nm}F_{n}F_{m}+2A_{-n,m}F_{n}^{\dagger}F_{m}\right)-\frac{2}{M}\cot\frac{\pi}{2M}+\frac{2}{M}\cot\frac{2\left(k-l\right)+1}{2M}\pi,\label{eq:App_hkl1}
\end{equation}

\end_inset

 where 
\begin_inset Formula 
\begin{equation}
A_{nm}=\exp\left(2\pi ik\frac{m+n}{M}\right)\sin\frac{m-n}{2M}\pi+\exp\left(2\pi i\frac{\left(k+l\right)\left(m+n\right)}{2M}\right)\sin\frac{\left(l-k-1/2\right)\left(m-n\right)\pi}{M}.\label{eq:App_hkl_Anm}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Similarly, applying 
\begin_inset Formula 
\[
\alpha_{n}\beta_{0}=\exp\left(-\frac{i\pi}{4}\right)c_{n}F_{M-n}^{\dagger}F_{0}+s_{n}F_{n}F_{0}
\]

\end_inset


\begin_inset Formula 
\[
\alpha_{0}\beta_{n}=\exp\left(\frac{i\pi}{4}\right)\left(s_{n}F_{M-n}^{\dagger}F_{0}^{\dagger}+c_{n}F_{0}^{\dagger}F_{n}\right)
\]

\end_inset


\begin_inset Formula 
\[
\alpha_{n}\alpha_{0}=\exp\left(\frac{i\pi}{4}\right)\left(c_{n}F_{M-n}^{\dagger}F_{0}^{\dagger}-s_{n}F_{0}^{\dagger}F_{n}\right)
\]

\end_inset


\begin_inset Formula 
\[
\beta_{n}\beta_{0}=\exp\left(-\frac{i\pi}{4}\right)\left(-s_{n}F_{M-n}^{\dagger}F_{0}+c_{n}F_{n}F_{0}\right)
\]

\end_inset

 to 
\begin_inset Formula $h_{kl}^{\left(0\right)}$
\end_inset

 yields
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
h_{kl}^{\left(0\right)} & =\frac{2}{M}\sum_{n=1}^{M-1}\left(\alpha_{n}\beta_{0}+\alpha_{0}\beta_{n}-i\alpha_{n}\alpha_{0}-i\beta_{n}\beta_{0}\right)\left[\exp\left(2\pi i\frac{kn}{M}\right)-\exp\left(2\pi i\frac{ln}{M}\right)\right]\\
 & =\frac{2}{M}\sum_{n=1}^{M-1}X_{n}^{\dagger}\left(F_{0}^{\dagger}F_{n}^{\dagger}-F_{n}^{\dagger}F_{0}\right)+\mathrm{h.c.},
\end{align*}

\end_inset

 where
\begin_inset Formula 
\[
X_{n}=i\left[\exp\left(2\pi i\frac{ln}{M}\right)-\exp\left(2\pi i\frac{kn}{M}\right)\right]\exp\left(-\frac{in\pi}{2M}\right).
\]

\end_inset

 Now from eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:App_hkl_Anm"

\end_inset

, we see that 
\begin_inset Formula 
\begin{align*}
A_{n0} & =-\exp\left(2\pi ik\frac{n}{M}\right)\sin\frac{n\pi}{2M}-\exp\left(2\pi i\frac{\left(k+l\right)n}{2M}\right)\sin\frac{\left(l-k-1/2\right)n\pi}{M}\\
 & =-\exp\left(2\pi ik\frac{n}{M}\right)\sin\frac{n\pi}{2M}-\frac{1}{2i}\exp\left(2\pi i\frac{\left(l-1/2\right)n}{M}\right)+\frac{1}{2i}\exp\left(2\pi i\frac{\left(k+1/2\right)n}{M}\right)\\
 & =\exp\left(2\pi ik\frac{n}{M}\right)\left(-\sin\frac{n\pi}{2M}+\frac{1}{2i}\exp\left(\frac{in\pi}{2M}\right)\right)-\frac{1}{2i}\exp\left(2\pi i\frac{ln}{M}\right)\exp\left(-\frac{in\pi}{2M}\right)\\
 & =-\frac{i}{2}\exp\left(2\pi ik\frac{n}{M}\right)\exp\left(-\frac{in\pi}{2M}\right)+\frac{i}{2}\exp\left(2\pi i\frac{ln}{M}\right)\exp\left(-\frac{in\pi}{2M}\right)\\
 & =\frac{1}{2}X_{n}.
\end{align*}

\end_inset

Hence, to add 
\begin_inset Formula $h_{kl}^{\left(0\right)}$
\end_inset

 terms to 
\begin_inset Formula $h_{kl}^{\left(1\right)}$
\end_inset

, we can simply change the 
\begin_inset Formula $m,n$
\end_inset

 index of 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:App_hkl1"

\end_inset

 to start from 
\begin_inset Formula $0$
\end_inset

.
 Finally, adding back the constant terms, we have 
\begin_inset Formula 
\begin{equation}
h_{kl}=\frac{2}{M}\sum_{n,m=0}^{M-1}\left(A_{nm}^{\dagger}F_{n}^{\dagger}F_{m}^{\dagger}+A_{nm}F_{n}F_{m}+2A_{-n,m}F_{n}^{\dagger}F_{m}\right)+\frac{2}{M}\mu\label{eq:App_hkl_h}
\end{equation}

\end_inset

 where 
\begin_inset Formula $A_{mn}$
\end_inset

 is given by 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:App_hkl_Anm"

\end_inset

 and 
\begin_inset Formula 
\begin{equation}
\mu=-\cot\frac{\pi}{2M}+\cot\frac{2\left(k-l\right)+1}{2M}\pi+M\xi.\label{eq:App_hkl_mu}
\end{equation}

\end_inset


\end_layout

\begin_layout Section
\begin_inset CommandInset label
LatexCommand label
name "sec:app_AFF"

\end_inset

Calculation of 
\begin_inset Formula $\left\langle \cdots\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\exp\left(\frac{1}{2}f_{k}^{\dagger}D_{kl}f_{l}^{\dagger}\right)\right\rangle $
\end_inset


\end_layout

\begin_layout Standard
In this appendix we will derive the formula 
\begin_inset Formula 
\begin{multline}
\left\langle \cdots\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\exp\left(\frac{1}{2}f_{k}^{\dagger}D_{kl}f_{l}^{\dagger}\right)\right\rangle =\\
-\frac{2}{M}\tr\left(S^{*}C^{-1}\bar{A}\right)\left\langle \cdots\exp\left(\frac{1}{2}f_{k}^{\dagger}D_{kl}f_{l}^{\dagger}\right)\right\rangle +\frac{2}{M}B_{mn}\left\langle \cdots f_{m}^{\dagger}f_{n}^{\dagger}\exp\left(\frac{1}{2}f_{k}^{\dagger}D_{kl}f_{l}^{\dagger}\right)\right\rangle \label{eq:App_AFF_1}
\end{multline}

\end_inset

 where 
\begin_inset Formula $\bar{A}\equiv A^{\dagger}$
\end_inset

 and 
\begin_inset Formula $B=C^{-1}\bar{A}\left(C^{-1}\right)^{T}$
\end_inset

, and relations among 
\begin_inset Formula $F_{n}^{\dagger}$
\end_inset

, 
\begin_inset Formula $f_{k}$
\end_inset

, and 
\begin_inset Formula $f_{k}^{\dagger}$
\end_inset

 are given by 
\begin_inset Formula 
\begin{equation}
F_{m}=\sum_{n=0}^{M-1}\left(f_{n}C_{mn}+f_{n}^{\dagger}S_{mn}\right),\quad0\leq m\leq M-1.\label{eq:App_AFF_Ffrelation}
\end{equation}

\end_inset

 with 
\begin_inset Formula $C_{mn}D_{nl}+S_{ml}=0$
\end_inset

.
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $X=\frac{1}{2}f_{k}^{\dagger}D_{kl}f_{l}^{\dagger}$
\end_inset

, 
\begin_inset Formula $\ket{G}=\exp\left(X\right)\ket{0}$
\end_inset

, then 
\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none

\begin_inset Formula 
\begin{equation}
F_{n}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\ket{G}=\exp\left(X\right)\left(\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}-\left[X,\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\right]+\frac{1}{2}\left[X,\left[X,\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\right]\right]+\cdots\right)\ket{0}.\label{eq:App_AFF_res1}
\end{equation}

\end_inset

Now let us calculate each term in the parenthesis of RHS of eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:App_AFF_res1"

\end_inset

.
 For the first term
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{subequations}
\backslash
label{eq:App_AFF_subres}
\end_layout

\end_inset


\family default
\series default
\shape default
\size default
\emph default
\bar default
\strikeout default
\uuline default
\uwave default
\noun default
\color inherit

\begin_inset Formula 
\begin{align}
\bar{A}_{nm}F_{n}^{\dagger}F_{m}^{\dagger}\ket{0} & =\left(\bar{A}_{nm}f_{i}^{\dagger}f_{j}^{\dagger}C_{ni}^{*}C_{mj}^{*}+\bar{A}_{nm}f_{i}S_{ni}^{*}f_{j}^{\dagger}C_{mj}^{*}\right)\ket{0}\nonumber \\
 & =\left(\bar{A}_{nm}f_{i}^{\dagger}f_{j}^{\dagger}C_{ni}^{*}C_{mj}^{*}+\bar{A}_{nm}S_{ni}^{*}C_{mj}^{*}\left\{ f_{i},f_{j}^{\dagger}\right\} \right)\ket{0}\nonumber \\
 & =\left[f_{i}^{\dagger}\left(C^{\dagger}\bar{A}C^{*}\right)_{ij}f_{j}^{\dagger}+\tr\left(\bar{A}C^{*}S^{\dagger}\right)\right]\ket{0}\label{eq:App_AFF_subres:1}
\end{align}

\end_inset

For the second term of RHS of 
\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:App_AFF_res1"

\end_inset


\family default
\series default
\shape default
\size default
\emph default
\bar default
\strikeout default
\uuline default
\uwave default
\noun default
\color inherit
, we first find 
\begin_inset Formula 
\begin{align*}
\left[\frac{1}{2}f_{k}^{\dagger}D_{kl}f_{l}^{\dagger},F_{m}^{\dagger}\right] & =\frac{1}{2}D_{kl}\left[f_{k}^{\dagger}f_{l}^{\dagger},F_{m}^{\dagger}\right]\\
 & =\frac{1}{2}D_{kl}\left(f_{k}^{\dagger}\left\{ f_{l}^{\dagger},F_{m}^{\dagger}\right\} -\left\{ f_{k}^{\dagger},F_{m}^{\dagger}\right\} f_{l}^{\dagger}\right)\\
 & =\frac{1}{2}D_{kl}\left(f_{k}^{\dagger}\delta_{ln}S_{mn}^{*}-\delta_{kn}S_{mn}^{*}f_{l}^{\dagger}\right)\\
 & =-S_{ml}^{*}D_{lk}f_{k}^{\dagger},
\end{align*}

\end_inset

 where in the second step we used the identity 
\begin_inset Formula 
\[
\left[AB,C\right]=A\left\{ B,C\right\} -\left\{ A,C\right\} B,
\]

\end_inset

and in the last step the property that 
\begin_inset Formula $D_{kl}$
\end_inset

 is antisymmetric.
 We then have 
\begin_inset Formula 
\begin{align*}
\left[\frac{1}{2}f_{k}^{\dagger}D_{kl}f_{l}^{\dagger},F_{n}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\right] & =F_{n}^{\dagger}\bar{A}_{nm}\left[\frac{1}{2}f_{k}^{\dagger}D_{kl}f_{l}^{\dagger},F_{m}^{\dagger}\right]+\left[\frac{1}{2}f_{k}^{\dagger}D_{kl}f_{l}^{\dagger},F_{n}^{\dagger}\right]\bar{A}_{nm}F_{m}^{\dagger}\\
 & =-F_{n}^{\dagger}\bar{A}_{nm}S_{ml}^{*}D_{lk}f_{k}^{\dagger}-S_{nl}^{*}D_{lk}f_{k}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\\
 & =\bar{A}_{nm}S_{ml}^{*}D_{lk}\left(f_{k}^{\dagger}F_{n}^{\dagger}-\left\{ F_{n}^{\dagger},f_{k}^{\dagger}\right\} \right)-S_{nl}^{*}D_{lk}f_{k}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\\
 & =\bar{A}_{nm}S_{ml}^{*}D_{lk}\left(f_{k}^{\dagger}F_{n}^{\dagger}-S_{nk}^{*}\right)-S_{nl}^{*}D_{lk}f_{k}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\\
 & =\bar{A}_{nm}S_{ml}^{*}D_{lk}f_{k}^{\dagger}F_{n}^{\dagger}-S_{nl}^{*}D_{lk}f_{k}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}-\bar{A}_{nm}S_{ml}^{*}D_{lk}S_{nk}^{*}\\
 & =f_{k}^{\dagger}D_{kl}\left(S^{\dagger}\right)_{lm}\bar{A}_{mn}F_{n}^{\dagger}+f_{k}^{\dagger}D_{kl}\left(S^{\dagger}\right)_{ln}\bar{A}_{nm}F_{m}^{\dagger}-\bar{A}_{nm}S_{ml}^{*}D_{lk}\left(S^{\dagger}\right)_{kn}\\
 & =2f_{k}^{\dagger}\left(DS^{\dagger}\bar{A}\right)_{km}F_{m}^{\dagger}-\tr\left(\bar{A}S^{*}DS^{\dagger}\right).
\end{align*}

\end_inset

 It then follows 
\begin_inset Formula 
\begin{equation}
\left[X,F_{n}^{\dagger}\bar{A}_{nm}F_{m}^{\dagger}\right]\ket{0}=\left[2f_{k}^{\dagger}\left(DS^{\dagger}\bar{A}C^{*}\right)_{kl}f_{l}^{\dagger}-\tr\left(\bar{A}S^{*}DS^{\dagger}\right)\right]\ket{0}.\label{eq:App_AFF_subres:2}
\end{equation}

\end_inset

 For the third term RHS of 
