\begin_layout Standard
According to formular 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:energy_pert_DeltaE_s1"

\end_inset

, we need to calculate 
\begin_inset Formula 
\begin{eqnarray*}
\left\langle F_{1}^{\left(2\right)}f_{0}\right\rangle _{V} & = & \frac{2}{M}\gamma^{\prime\left(V\right)}\left\langle f_{1}f_{0}\exp\left(\frac{1}{2}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right)\right\rangle +\frac{2}{M}\Omega_{mn}^{\left(V\right)}\left\langle f_{1}f_{0}f_{m}^{\dagger}f_{n}^{\dagger}\exp\left(\frac{1}{2}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right)\right\rangle \\
 & = & \frac{4}{M}\Omega_{0n}^{\left(V\right)}\left\langle f_{1}f_{n}^{\dagger}\exp\left(\frac{1}{2}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right)\right\rangle \\
 & = & \frac{4}{M}\Omega_{01}^{\left(V\right)}\\
 & = & -\frac{2-2i}{\sqrt{3}}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\left\langle F_{1}^{\left(2\right)}f_{2}\right\rangle _{V} & = & \frac{2}{M}\gamma^{\prime\left(V\right)}\left\langle f_{1}f_{2}\exp\left(\frac{1}{2}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right)\right\rangle +\frac{2}{M}\Omega_{mn}^{\left(V\right)}\left\langle f_{1}f_{2}f_{m}^{\dagger}f_{n}^{\dagger}\exp\left(\frac{1}{2}f_{k}^{\dagger}M_{kl}f_{l}^{\dagger}\right)\right\rangle \\
 & = & -\frac{2}{M}\gamma^{\prime\left(V\right)}M_{12}-\frac{4}{M}\Omega_{12}^{\left(V\right)}\\
 & = & 4\left(1-\frac{1}{\sqrt{3}}\right)e^{-i\frac{\pi}{4}}
\end{eqnarray*}

\end_inset

 Likewise
\begin_inset Formula 
\[
\left\langle F_{1}^{\left(2\right)}f_{0}\right\rangle _{W}=\frac{4}{M}\Omega_{01}^{\left(W\right)}=-\frac{2-2i}{\sqrt{3}},\quad\left\langle F_{1}^{\left(2\right)}f_{2}\right\rangle _{W}=-\frac{2}{M}\gamma^{\prime\left(W\right)}M_{12}-\frac{4}{M}\Omega_{12}^{\left(W\right)}=8\left(1-\frac{2}{\sqrt{3}}\right)e^{-i\frac{\pi}{4}}.
\]

\end_inset

For 
\begin_inset Formula $M=3$
\end_inset

, the ground energy 
\begin_inset Formula $E_{0}=-4\sqrt{3}$
\end_inset

.
 We therefore have 
\begin_inset Formula 
\begin{eqnarray*}
\Delta E_{0} & = & \frac{1}{N^{2}}\frac{KLM\left|\det C\right|}{E_{0}-E_{0}^{\left(1\right)}-E_{1}^{\left(2\right)}}\left(\left\langle F_{1}^{\left(2\right)}f_{0}\right\rangle _{W}^{*}\left\langle F_{1}^{\left(2\right)}f_{0}\right\rangle _{V}+\left\langle F_{1}^{\left(2\right)}f_{2}\right\rangle _{W}^{*}\left\langle F_{1}^{\left(2\right)}f_{2}\right\rangle _{V}\right)\\
 & = & \frac{1}{N^{2}}\frac{1\cdot2\cdot3}{-4\sqrt{3}-4}\frac{2+\sqrt{3}}{4}\left[\left(-\frac{2+2i}{\sqrt{3}}\right)\left(-\frac{2-2i}{\sqrt{3}}\right)+8\left(1-\frac{2}{\sqrt{3}}\right)4\left(1-\frac{1}{\sqrt{3}}\right)\right]\\
 & = & -\frac{3}{2}\left(3\sqrt{3}-5\right)\frac{1}{N^{2}}.
\end{eqnarray*}

\end_inset


